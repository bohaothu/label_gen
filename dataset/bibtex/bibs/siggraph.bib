  keywords =     "clipping; computer graphics; homogeneous coordinates",
  meetingaddress = "Atlanta, Ga",
  meetingdate =  "Aug 23--25 1978",
  meetingdate2 = "08/23--25/78",
}

@Article{Braid:1978:SCS,
