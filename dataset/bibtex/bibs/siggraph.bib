@Article{Ward:1988:RTS,
  author =       "Gregory J. Ward and Francis M. Rubinstein and Robert
                 D. Clear",
  title =        "A Ray Tracing Solution for Diffuse Interreflection",
  journal =      j-COMP-GRAPHICS,
  volume =       "22",
