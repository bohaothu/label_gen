}

@Article{Wein:1978:HLR,
  author =       "M. Wein and P. Tanner and G. Bechthold and N.
                 Burtnyk",
  title =        "Hidden Line Removal for Vector Graphics",
  journal =      j-COMP-GRAPHICS,
  volume =       "12",
  number =       "3",
