@InProceedings{Szekely:1990:TMA,
  author =       "P. Szekely",
  title =        "Template-based mapping of application data to
                 interactive displays",
  crossref =     "ACM:1990:UTA",
  pages =        "1--9",
  year =         "1990",
  bibdate =      "Thu Dec 14 18:48:37 MST 1995",
  abstract =     "This paper describes a template-based method for
                 constructing interactive displays with the
                 building-blocks (widgets) provided in a user interface
                 toolkit. Templates specify how to break down complex
                 application objects into smaller pieces, specify the
                 graphical components (widgets) to be used for
                 displaying each piece, and specify their layout.
                 Complex interfaces are constructed by recursively
                 applying templates, thus constructing a tree of widgets
                 to display a complex application object. The
                 template-based method is more general than the
                 interactive, WYSIWYG interface builders in that it can
                 specify dynamic displays for application data that
                 changes at run time. The template-based method also
                 leads to more consistent, extendable and modifiable
                 interfaces. (15 Refs.)",
  acknowledgement = ack-nhfb,
  affiliation =  "Inf. Sci. Inst., Univ. of Southern California, Marina
                 de Rey, CA, USA",
  classification = "C6115 (Programming support); C6170 (Expert systems);
                 C6180G (Graphical user interfaces)",
  keywords =     "Application data; Graphical components; Interactive
                 displays; Template based mapping; User interface
                 toolkit; Widgets",
  thesaurus =    "Graphical user interfaces; Interactive systems;
                 Software tools; User interface management systems",
}

@InProceedings{Kim:1990:DUI,
  author =       "W. C. Kim and J. D. Foley",
  title =        "{DON}: user interface presentation design assistant",
  crossref =     "ACM:1990:UTA",
  pages =        "10--20",
  year =         "1990",
  bibdate =      "Thu Dec 14 18:48:37 MST 1995",
  abstract =     "The authors describe a design tool, DON, which assists
                 user interface designers in generating menu and dialog
                 box presentations. An integrated knowledge base model
                 services as the foundation for developing the set of
                 design rules to automate various activities of the
                 design process. Useful and reusable knowledge about the
                 organization of menus and dialog boxes is identified
                 and encapsulated in the form of design rules. The basic
                 approach the authors take is embedding a top-down
                 design methodology in a tool that assists designers in
                 organizing the information, selecting appropriate
                 interface object classes and their attributes, and
                 placing selected interface objects in a dialog box or a
                 menu in a meaningful, logical, and consistent manner.
                 They let the designer specify the conceptual design of
                 an application, maintain high-level style preference
                 profiles, customize the appearances of interface object
                 classes which make up an interface presentation, and
                 control the priority of organization rules. The tool
                 then automatically generates the user interface
                 presentation. (28 Refs.)",
  acknowledgement = ack-nhfb,
  affiliation =  "Dept. of Electr. Eng. and Comput. Sci., George
                 Washington Univ., DC, USA",
  classification = "C6115 (Programming support); C6170 (Expert systems);
                 C6180G (Graphical user interfaces)",
  keywords =     "Attributes; Dialog box; Dialog box presentations; DON;
                 Integrated knowledge base model; Menu; Organization
                 rules; Style preference profiles; Top-down design
                 methodology; User interface presentation design
                 assistant",
  thesaurus =    "Graphical user interfaces; Knowledge based systems;
                 Software tools; User interface management systems",
}

@InProceedings{Wiecha:1990:GUI,
  author =       "C. Wiecha and S. Boies",
  title =        "Generating user interfaces: principles and use of
                 {ITS} style rules",
  crossref =     "ACM:1990:UTA",
  pages =        "21--30",
  year =         "1990",
  bibdate =      "Thu Dec 14 18:48:37 MST 1995",
  abstract =     "Historically, two types of layered architectures have
                 been developed in attempts to provide the required
                 flexibility in applications: user interface management
                 systems (UIMS) and toolkits UIMS separate the
                 application and its interface. Back end computations
                 are separated from dialog control and style. Style,
                 however, is often treated in a single interface layer.
                 Toolkits separate style from the application. Dialog
                 control remains in the back end while the
                 implementation of interaction techniques is hidden in a
                 code library. ITS is a four layer architecture with
                 separate tools for back end computation, dialog
                 control, rule-based interface style, and the
                 implementation of graphic primitives used in the style.
                 Recent work has focused particularly on the rule layer.
                 The authors begin by reviewing previous work in UIMS
                 and toolkits. Then they show how the rule layer in ITS
                 combines the benefits of both UIMS and toolkits. (22
                 Refs.)",
  acknowledgement = ack-nhfb,
  affiliation =  "IBM Thomas J. Watson Res. Center, Yorktown Heights,
                 NY, USA",
  classification = "C6115 (Programming support); C6170 (Expert systems);
                 C6180G (Graphical user interfaces)",
  keywords =     "Back end computation; Dialog control; Graphic
                 primitives; ITS style rules; Layered architectures;
                 Rule-based interface style; Toolkits; UIMS; User
                 interface management systems; User interfaces
                 generation",
  thesaurus =    "Graphical user interfaces; Knowledge based systems;
                 Software tools; User interface management systems",
}

@InProceedings{Jones:1990:SIO,
  author =       "W. Jones and P. Williams and G. Robertson and V.
                 Joloboff and M. Conner",
  title =        "In search of the ideal operating system for user
                 interfacing",
  crossref =     "ACM:1990:UTA",
  pages =        "31--35",
  year =         "1990",
  bibdate =      "Thu Dec 14 18:48:37 MST 1995",
  abstract =     "As the computing environment's lowest common
                 denominator, the operating system must always play a
                 game of catch-up to provide system-wide support for the
                 changing demands and opportunities of its user
                 environment. Never has this been more true than now.
                 The basics of an operating system include management of
                 processing time, memory, interprocess and inter-machine
                 communication and persistent storage. Choices made in
                 the provision of these services can impact the user
                 interface, even when many layers interceded. This paper
                 discusses operating system issues that impact the
                 following user interfacing concerns: real-time
                 performance; persistent storage; interoperability and
                 data transfer; and transparency. (0 Refs.)",
  acknowledgement = ack-nhfb,
  affiliation =  "Arthur D. Little Enterprises, Cambridge, MA, USA",
  classification = "C6115 (Programming support); C6150J (Operating
                 systems); C6180 (User interfaces)",
  keywords =     "Data transfer; Inter-machine communication;
                 Interoperability; Interprocess; Memory; Operating
                 system; Persistent storage; Processing time; Real-time
                 performance; Transparency; User interfacing",
  thesaurus =    "Operating systems [computers]; Software tools; User
                 interface management systems",
}

@InProceedings{Pittman:1990:VVU,
  author =       "J. H. Pittman and C. J. Kitrick",
  title =        "{VUIMS}: a visual user interface management system",
  crossref =     "ACM:1990:UTA",
  pages =        "36--46",
  year =         "1990",
  bibdate =      "Thu Dec 14 18:48:37 MST 1995",
  abstract =     "VUIMS is an object-oriented user interface management
                 system that was designed to support reconfigurable
                 components. VUIMS consists of a collection of objects
                 and a semantically rich token language. The objects
                 implement primitive presentation and interaction
                 functions. The token language controls interaction and
                 visual style. High level objects can be created from
                 primitive objects using token templates. The user
                 interface and application are controlled by token
                 streams that are emitted in response to user actions.
                 VUIMS supports a variety of presentation and
                 interaction styles through simple, robust manipulation
                 of a hierarchy of visual panels with a rich set of
                 relationships and constraints. VUIMS has been used to
                 implement two commercial high-performance computer
                 graphics applications and an online help system. (15
                 Refs.)",
  acknowledgement = ack-nhfb,
  affiliation =  "Wavefront Technologies Inc., Santa Barbara, CA, USA",
