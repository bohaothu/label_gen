@Article{Franklin:1978:GDD,
  author =       "William Randolph Franklin and Harry R. Lewis",
  title =        "{3-D} Graphic Display of Discrete Spatial Data by
                 Prism Maps",
  journal =      j-COMP-GRAPHICS,
  volume =       "12",
  number =       "3",
  pages =        "70--75",
