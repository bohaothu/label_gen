@Article{Arvo:1994:FAE,
  author =       "James Arvo and Kenneth Torrance and Brian Smits",
  title =        "A Framework for the Analysis of Error in Global
                 Illumination Algorithms",
  journal =      j-COMP-GRAPHICS,
  volume =       "28",
  number =       "{Annual Conference Series}",
  pages =        "75--84",
  month =        jul,
  year =         "1994",
  CODEN =        "CGRADI, CPGPBZ",
  ISSN =         "0097-8930",
  bibdate =      "Mon Oct 4 18:51:53 MDT 1999",
  bibsource =    "http://www.acm.org/pubs/toc/",
  URL =          "http://www.acm.org:80/pubs/citations/proceedings/graph/192161/p75-arvo/",
  abstract =     "In this paper we identify sources of error in global
                 illumination algorithms and derive bounds for each
                 distinct category. Errors arise from three sources:
                 inaccuracies in the boundary data, discretization, and
                 computation. Boundary data consists of surface
                 geometry, reflectance functions, and emission
                 functions, all of which may be perturbed by errors in
                 measurement or simulation, or by simplifications made
                 for computational efficiency. Discretization error is
                 introduced by replacing the continuous radiative
                 transfer equation with a finite-dimensional linear
                 system, usually by means of boundary elements and a
                 corresponding projection method. Finally, computational
                 errors perturb the finite-dimensional linear system
                 through imprecise form factors, inner products,
                 visibility, etc., as well as by halting iterative
                 solvers after a finite number of steps. Using the error
                 taxonomy introduced in the paper we examine existing
                 global illumination algorithms and suggest new avenues
                 of research.",
  acknowledgement = ack-nhfb,
  keywords =     "ACM; computer graphics; measurement; performance;
                 SIGGRAPH; theory",
  subject =      "{\bf I.3.7} Computing Methodologies, COMPUTER
                 GRAPHICS, Three-Dimensional Graphics and Realism.",
}

@Article{Berman:1994:MPC,
  author =       "Deborah F. Berman and Jason T. Bartell and David H.
                 Salesin",
  title =        "Multiresolution Painting and Compositing",
  journal =      j-COMP-GRAPHICS,
