@Article{Kaufman:1978:SDI,
  author =       "Arie A. Kaufman",
  title =        "System Design and Implementation of {BGRAF2}",
  journal =      j-COMP-GRAPHICS,
  volume =       "12",
  number =       "3",
  pages =        "87--92",
  month =        aug,
  year =         "1978",
  CODEN =        "CGRADI, CPGPBZ",
