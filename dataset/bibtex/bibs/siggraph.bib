@Article{Lee:1985:SOS,
  author =       "Mark E. Lee and Richard A. Redner and Samuel P.
                 Uselton",
  title =        "Statistically Optimized Sampling for Distributed Ray
                 Tracing",
  journal =      j-COMP-GRAPHICS,
  volume =       "19",
  number =       "3",
  pages =        "61--68",
  month =        jul,
