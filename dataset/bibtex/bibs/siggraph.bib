@Article{Finkelstein:1994:MC,
  author =       "Adam Finkelstein and David H. Salesin",
  title =        "Multiresolution Curves",
  journal =      j-COMP-GRAPHICS,
  volume =       "28",
  number =       "{Annual Conference Series}",
  pages =        "261--268",
  month =        jul,
  year =         "1994",
  CODEN =        "CGRADI, CPGPBZ",
  ISSN =         "0097-8930",
  bibdate =      "Mon Oct 4 18:51:53 MDT 1999",
  bibsource =    "http://www.acm.org/pubs/toc/",
  URL =          "http://www.acm.org:80/pubs/citations/proceedings/graph/192161/p261-finkelstein/",
  abstract =     "We describe a multiresolution curve representation,
                 based on wavelets, that conveniently supports a variety
                 of operations: smoothing a curve; editing the overall
                 form of a curve while preserving its details; and
                 approximating a curve within any given error tolerance
                 for scan conversion. We present methods to support
                 continuous levels of smoothing as well as direct
                 manipulation of an arbitrary portion of the curve; the
