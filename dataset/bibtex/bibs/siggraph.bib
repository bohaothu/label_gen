@InProceedings{Blumenthal:1990:SAI,
  author =       "B. Blumenthal",
  title =        "Strategies for automatically incorporating metaphoric
                 attributes in interface designs",
  crossref =     "ACM:1990:UTA",
  pages =        "66--75",
  year =         "1990",
  bibdate =      "Thu Dec 14 18:48:37 MST 1995",
  abstract =     "Recently a number of projects have addressed the
                 problem of automatically producing interface designs
                 for computer applications. The core of each of these
                 design tools takes as input a specification of the
                 functionality and information that the application is
                 to present to the user and produces as output an
                 interface specified in terms of some toolkit of
                 interface techniques. Although there are a number of
                 sets of guidelines for effective interface designs,
                 they are all inadequate for a completely automated
                 interface design system. The most obvious inadequacy is
                 in the area of aesthetic decisions; most sets of
                 interface guidelines either lack guidelines for
                 determining the specific visual appearances and
                 organization of an interface, or else such guidelines
                 are too vague to be used automatically. Decisions about
                 what aspects of the application to emphasize and what
                 aspects to obscure are also poorly specified. Such
                 decisions are implicit in any interface design, but
                 guidelines for making them are vague at best, and often
                 absent altogether. The MAID system addresses some of
                 these inadequacies by using knowledge-based
                 descriptions of real-world objects to guide decisions
                 that are underspecified by a given set of design
                 guidelines. (22 Refs.)",
  acknowledgement = ack-nhfb,
  affiliation =  "Dept. of Comput. Sci., Texas Univ. at Austin, TX,
                 USA",
  classification = "C6115 (Programming support); C6170 (Expert systems);
                 C6180 (User interfaces)",
  keywords =     "Aesthetic decisions; Automatically incorporating
                 metaphoric attributes; Interface designs;
                 Knowledge-based descriptions; MAID system; Real-world
                 objects; Visual appearances",
  thesaurus =    "Interactive systems; Knowledge based systems; Software
                 tools; User interface management systems",
}

@InProceedings{Feiner:1990:WWW,
  author =       "S. Feiner and C. Beshers",
  title =        "Worlds within worlds: metaphors for exploring
                 $n$-dimensional virtual worlds",
  crossref =     "ACM:1990:UTA",
  pages =        "76--83",
  year =         "1990",
  bibdate =      "Thu Dec 14 18:48:37 MST 1995",
  abstract =     "n-Vision is a testbed for exploring n-dimensional
                 worlds containing functions of an arbitrary number of
                 variables. Although their interaction devices and
                 display hardware are inherently $3$D, the authors
                 demonstrate how they can be used to support interaction
                 with these higher-dimensional objects. They introduce a
                 new interaction metaphor developed for the system,
                 which they call `worlds within worlds': nested
                 heterogeneous coordinate systems that allow the user to
                 view and manipulate functions. Objects in their world
                 may be explored with a set of tools. They describe an
                 example n-Vision application in `financial
                 visualization,' where the functions are models of
                 financial instruments. n-Vision's software architecture
                 supports a hierarchy of arbitrarily transformed, nested
                 boxes that defines an interactive space within which
                 information is displayed and input obtained. The
                 design, modeled in part after the hierarchical $2$D
                 windows of the X Window System, is intended to provide
                 an environment that is well suited to the use of true
                 $3$D input and stereo display devices. Boxes are
                 associated with event handlers that support $3$D
                 motion, enter, and leave events, and provide
                 recognition of finger gestures. (23 Refs.)",
  acknowledgement = ack-nhfb,
  affiliation =  "Dept. of Comput. Sci., Columbia Univ., New York, NY,
                 USA",
  classification = "C6115 (Programming support); C6180G (Graphical user
                 interfaces)",
  keywords =     "2D windows; Display hardware; Financial visualization;
                 Interaction devices; Interaction metaphor; Metaphors;
                 N-dimensional virtual worlds; N-Vision; Nested
                 heterogeneous coordinate systems; Software
                 architecture; Testbed; X Window System",
  thesaurus =    "Graphical user interfaces; Software tools; User
                 interface management systems",
}

@InProceedings{Karrer:1990:REO,
