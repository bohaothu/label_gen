@INPROCEEDINGS{Wei2007,
  author = {R. Wei and K. Wu},
  title = {Exception Resolution Service for RBAC systems},
  booktitle = {CSCWD: International Conference on Computer Supported Cooperative
	Work in Design},
  year = {2007},
  venue = {CSCWD},
  firstlevel = {Prac},
  secondlevel = {Technology},
  thirdlevel = {Collaborative Environment CSCW},
  pages = {840-845},
  abstract = {We propose an exception resolution service for a role based access
	control (RBAC) system as a supplementary service, that will increase
	the flexibility of RBAC systems. Our model will keep the security
	policies based on a RBAC system while some not role based administrations
	are possible. In other words, the not role based access permission
	is not independent from the RBAC system. The related definitions
	and functions are given. An implementation example shows that the
	service is useful and efficient in practice.},
