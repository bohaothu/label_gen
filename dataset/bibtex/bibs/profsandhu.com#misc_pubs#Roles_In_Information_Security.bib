@ARTICLE{Braghin2006,
  author = {Chiara Braghin and Daniele Gorla and Vladimiro Sassone},
  title = {Role-based access control for a distributed calculus},
  journal = {Journal of Computer Security},
  year = {2006},
  volume = {14},
  pages = {113-155},
  venue = {JCS},
  firstlevel = {Theo},
  secondlevel = {Role Model/Design},
  thirdlevel = {Formalization},
  number = {2},
  abstract = {Rle-based access control (RBAC) is increasingly attracting attention
	because it reduces the complexity and cost of security administration
	by interposing the notion of rle in the assignment of permissions
	to users. In this paper, we present a formal framework relying on
	an extension of the -calculus to study the behaviour of concurrent
	systems in a RBAC scenario. We define a type system ensuring that
	the specified policy is respected during computations, and a behavioural
	equivalence to equate systems. We then consider a more sophisticated
	feature that can be easily integrated in our framework, i.e., the
	possibility of automatically adding rle activations and deactivations
	to processes to be run under a given policy (whenever possible).
	Finally, we show how the framework can be easily extended to express
	significant extensions of the core RBAC model, such as rles hierarchies
	or constraints determining the acceptability of the system components.},
  bibsource = {DBLP, http://dblp.uni-trier.de},
  doi = {http://iospress.metapress.com/openurl.asp?genre=article{\&}issn=0926-227X{\&}volume=14{\&}issue=2{\&}spage=113},
