@INPROCEEDINGS{Schier1998a,
  author = {Kathrin Schier},
  title = {A role and task based security model for multifunctional smartcard
	applications in the area of electronic commerce},
  booktitle = {SEC '98 : International Conference On Information Security, IFIP
	Tc.11 Conference},
  year = {1998},
  venue = {SEC},
  firstlevel = {Prac},
  secondlevel = {Industry},
  thirdlevel = {E-Commerce},
  abstract = {A role and task based security model (R&T model) can ensure a secure
	access to many different services through an application based security
	framework. It can be implemented in a multifunctional smartcard.
	Electronic commerce demands different security requirements for its
	many different applications. In the near future one smartcard may
	be used for many electronic commerce applications, such as payment
	systems, access to banking services and financial transactions over
	the Internet. The R&T model can be used and implemented on a smartcard
	in order to ensure both the users personal need for application based
	security and his right to informational self determination - as the
	fundamental right of privacy is defined in the German legal system.
	A successful application of the model can help the user navigate
	a secure way through the jungle of electronic.},
  timestamp = {2009.01.23}
}

