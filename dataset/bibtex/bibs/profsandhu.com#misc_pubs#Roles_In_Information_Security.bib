@INPROCEEDINGS{Cai2006,
  author = {Hongxia Cai and Tao Yu and Minglun Fang},
  title = {Access Control Model of Manufacturing Grid},
  booktitle = {PROLAMAT},
  year = {2006},
  venue = {PROLAMAT},
  firstlevel = {Prac},
  secondlevel = {Technology},
  thirdlevel = {Network Grid},
  pages = {938-943},
  abstract = {The overall motivation for Manufacturing Grid is to realize resource
	sharing and collaboration in the field of Manufacturing. In order
	to protect the shared resources and services, the access control
	guarantees in the MG is necessary and important. Due to inherent
