@Article{Myles:2005:ETS,
  author =       "Ginger Myles and Christian Collberg and Zachary
                 Heidepriem and Armand Navabi",
  title =        "The evaluation of two software watermarking
                 algorithms",
  journal =      j-SPE,
  volume =       "35",
  number =       "10",
  pages =        "923--938",
  month =        aug,
  year =         "2005",
  CODEN =        "SPEXBL",
