@Article{Fleissner:2007:EAA,
  author =       "Sebastian Fleissner and Elisa L. A. Baniassad",
  title =        "Epi-aspects: aspect-oriented conscientious software",
  journal =      j-SIGPLAN,
  volume =       "42",
  number =       "10",
