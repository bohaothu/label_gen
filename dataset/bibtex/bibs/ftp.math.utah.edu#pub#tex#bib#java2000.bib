@Article{Ekman:2007:JEJ,
  author =       "Torbj{\"o}rn Ekman and G{\"o}rel Hedin",
  title =        "The {JastAdd} Extensible {Java} compiler",
  journal =      j-SIGPLAN,
  volume =       "42",
