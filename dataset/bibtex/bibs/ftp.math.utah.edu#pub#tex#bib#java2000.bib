@Article{Muschevici:2008:MDP,
  author =       "Radu Muschevici and Alex Potanin and Ewan Tempero and
                 James Noble",
  title =        "Multiple dispatch in practice",
  journal =      j-SIGPLAN,
  volume =       "43",
  number =       "10",
  pages =        "563--582",
  month =        sep,
  year =         "2008",
