@article{ a096_hilger2006,
Author = {Hilger, A. and Kardjilov, N. and Strobl, M. and Treimer, W. and
   Banhart, J.},
Title = {{The new cold neutron radiography and tomography instrument CONRAD at
   HMI Berlin}},
Journal = {{Physica B-Condensed Matter}},
Year = {{2006}},
