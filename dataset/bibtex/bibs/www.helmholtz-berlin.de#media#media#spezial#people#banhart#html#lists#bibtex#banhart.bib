@article{ d036_manke2008,
Author = {Manke, Ingo and Kardjilov, Nikolay and Hilger, Andre and Strobl, Markus
   and Dawson, Martin and Banhart, John},
Title = {{Visualization of magnetic fields with spin-polarized neutrons}},
Journal = {{Materials Testing}},
Year = {{2008}},
Volume = {{50}},
Number = {{10}},
Pages = {{572-579}},
Abstract = {{Visualization of magnetic fields with spin-polarized neutrons. Neutron
   imaging often provides important complementary information to that
   yielded by x-rays. Neutrons, however, also have a permanent magnetic
   dipole moment, making them highly sensitive to magnetic fields - a
   property that can be combined with standard radiographic and
   tomographic imaging approaches to reveal macroscopic magnetic
   properties. This technique has recently been developed at the
   Hahn-Meitner Institute (HMI) in Berlin for the vizualisation of
   magnetic fields both within and around matter, information that is
   inaccessible using other methods.}},
ISSN = {{0025-5300}},
Unique-ID = {{ISI:000271598200006}},
}
@article{ d037_kardjilov2008,
Author = {Kardjilov, Nikolay and Hilger, Andre and Manke, Ingo and Garcia-Moreno, Francisco and Banhart, John},
Title = {{Bragg-edge Imaging with Neutrons}},
Journal = {{Materials Testing}-MATERIALS AND COMPONENTS TECHNOLOGY AND APPLICATION}},
Year = {{2008}},
Volume = {{50}},
Number = {{10}},
Pages = {{569-571}},
Abstract = {{Cold neutrons have a wavelength that is in the same range as the d(hkl)
   spaces for polycrystalline metal materials. Therefore imaging of
   polycrystalline materials with monochromatic cold neutrons provides a
   unique contrast due to coherent Bragg scattering. Here, we present
   investigations of metallic materials with energy-selective neutron
   radiography around the Bragg-edges for polycrystalline materials.
   Polychromatic and monochromatic radiographic measurements of welded
   composite materials are shown. The results give interesting insights
   into the manufacturing process.}},
ISSN = {{0025-5300}},
Unique-ID = {{ISI:000271598200005}},
}
