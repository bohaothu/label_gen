Book-Group-Author = {{ESA}},
Title = {{Metallic foam experiments under microgravity}},
Booktitle = {{18th ESA Symposium on European Rocket and Balloon Programmes and Related Research}},
Series = {{ESA Special Publications}},
Year = {{2007}},
Volume = {{647}},
Pages = {{389-392}},
Note = {{18th ESA Symposium on European Rocket and Balloon Programmes and
   Related Research, Visby, Sweden, JUN 03-07, 2007}},
Abstract = {{X-ray analysis was found to be a very powerful method for the in-situ
   study of the metal foaming process on Earth. Under normal conditions,
   the inevitable presence of gravitationally-driven drainage makes
   investigation of coarsening very difficult, because of the rapid
   variations of foam properties induced by the gravitational flow. Under
   microgravity we can remove these limitations in order to generate
   improved models of the foaming metals. So it is possible to isolate
   some of the key effects which govern foam evolution, namely drainage,
   flow, coarsening, and coalescence. Experiments scheduled to fly on the
   XRMON and mu g-FOAM MAP projects and using our ground equipment are
   presented. Microgravity experiments are prepared to fly on parabolic
   flights and Maser 11.}},
ISSN = {{0379-6566}},
Unique-ID = {{ISI:000255234200063}},
}

