	Pages = {911--918},
	Pii = {btl035},
	Pmid = {16455750},
	Title = {Detection of gene copy number changes in {CGH} microarrays using a spatially correlated mixture model.},
	Url = {http://dx.doi.org/10.1093/bioinformatics/btl035},
	Volume = {22},
	Year = {2006},
