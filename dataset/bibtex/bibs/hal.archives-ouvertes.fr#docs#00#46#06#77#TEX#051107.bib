	Pages = {290--307},
	Title = {Stochastic segmentation models for array-based comparative genomic hybridization data analysis},
	Url = {http://biostatistics.oxfordjournals.org/cgi/content/abstract/9/2/290},
	Volume = {9},
	Year = {2008},
