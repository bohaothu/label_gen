	Journal = {{BMC} Genomics},
	Keywords = {NGS, ASE},
	Note = {{PMID:} 19740431},
	Pages = {422},
	Title = {Allele-specific expression assays using Solexa},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/19740431},
