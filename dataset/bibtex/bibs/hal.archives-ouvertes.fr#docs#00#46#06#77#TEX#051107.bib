	Keywords = {GWAS, genotyping microarrays},
	Month = feb,
	Number = {2},
	Pages = {747--753},
	Shorttitle = {Local Phylogeny Mapping of Quantitative Traits},
	Title = {Local Phylogeny Mapping of Quantitative Traits: Higher Accuracy and Better Ranking Than {Single-Marker} Association in Genomewide Scans},
	Url = {http://www.genetics.org/cgi/content/abstract/181/2/747},
	Volume = {181},
	Year = {2009},
