	Pages = {2335--42},
	Title = {Combining {DNA} expression with positional information to detect functional silencing of chromosomal regions},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/14668216},
	Volume = {19},
	Year = {2003},
