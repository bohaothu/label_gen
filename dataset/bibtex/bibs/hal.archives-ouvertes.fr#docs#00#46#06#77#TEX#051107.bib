	Month = mar,
	Number = {6},
	Pages = {2013--2025},
	Shorttitle = {{QuantiSNP}},
	Title = {{QuantiSNP:} an Objective Bayes {Hidden-Markov} Model to detect and accurately map copy number variation using {SNP} genotyping data},
