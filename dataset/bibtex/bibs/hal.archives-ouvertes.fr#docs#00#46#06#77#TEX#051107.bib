	Doi = {10.1093/bioinformatics/btp113},
	Journal = {Bioinformatics},
	Month = apr,
	Number = {8},
	Pages = {1026--1032},
	Title = {Statistical inferences for isoform expression in {RNA-Seq}},
	Url = {http://bioinformatics.oxfordjournals.org/cgi/content/abstract/25/8/1026},
