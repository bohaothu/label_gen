	Issn = {1753-6561},
	Journal = {{BMC} Proceedings},
	Number = {Suppl 1},
	Pages = {S141},
	Title = {A mixture model approach to multiple testing for the genetic analysis of gene expression},
	Url = {http://www.biomedcentral.com/1753-6561/1/S1/S141},
	Volume = {1},
	Year = {2007},
