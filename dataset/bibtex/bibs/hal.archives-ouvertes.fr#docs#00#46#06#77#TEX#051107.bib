	Number = {51},
	Pages = {17765--17770},
	Pii = {0407979101},
	Pmid = {15591353},
	Title = {Comparative genomic hybridization using oligonucleotide microarrays and total genomic {DNA}.},
	Url = {http://dx.doi.org/10.1073/pnas.0407979101},
