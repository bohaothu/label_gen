	Month = {Nov},
	Number = {21},
	Pages = {6240--6245},
	Pdf = {HymanKauraniemi02.pdf},
	Pmid = {12414653},
	Title = {{Impact of DNA amplification on gene expression patterns in breast cancer.}},
	Volume = {62},
	Year = {2002},
