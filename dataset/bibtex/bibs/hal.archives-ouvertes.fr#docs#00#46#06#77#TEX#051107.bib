	Journal = {Bioinformatics},
	Keywords = {Copy-number/Expression, regional gene expression},
	Month = oct,
	Number = {15},
	Pages = {2390--2398},
	Title = {{GEPIS--quantitative} gene expression profiling in normal and cancer tissues},
	Url = {http://bioinformatics.oxfordjournals.org/cgi/content/abstract/20/15/2390},
	Volume = {20},
