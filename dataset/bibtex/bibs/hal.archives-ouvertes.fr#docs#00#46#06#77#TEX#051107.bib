	Keywords = {Copy-number/Expression, regional gene expression, RITE},
	Month = sep,
	Number = {18},
	Pages = {5781--5784},
	Title = {Genome-wide Identification of Chromosomal Regions of Increased Tumor Expression by Transcriptome Analysis},
