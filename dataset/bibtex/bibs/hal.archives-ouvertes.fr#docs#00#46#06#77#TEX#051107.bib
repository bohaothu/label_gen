	Title = {The Sanger {FASTQ} file format for sequences with quality scores, and the {Solexa/Illumina} {FASTQ} variants},
	Url = {http://nar.oxfordjournals.org/cgi/content/abstract/gkp1137v1},
	Year = {2009},
