@article{PETE92A,
  author =     {I. R. Peterson},
  address =     {UNIV MAINZ,INST PHYS CHEM,JAKOB WELDER WEG 11,
                  W-6500 MAINZ, GERMANY},
  title =     {Phases of Amphiphilic Monolayers in Different
                  Environments},
  journal =     PHYS,
  year =     1992,
  volume =     32,
  number =     46,
  pages =     {245--247},
  keywords =     {CHAIN-FATTY-ACIDS; ORIENTATIONAL-ORDER; amphiphiles;
                  membrane; crystallography},
  annote =     {Amphiphiles are organic molecules which
                  spontaneously form two-dimensional phases,
                  particularly at pre-existing interfaces. This is the
                  physical basis for cell membranes and detergents,
                  and is very important in colloidal systems. Recently
                  available techniques have provided proof that
                  monolayers of many amphiphiles display a rich
                  variety of phases. Many of these phases display
                  behaviour characteristic for low-dimensional
                  (i.e. highly anisotropic) systems, and some are
                  hexatic. In addition to the experimental conditions
                  of surface pressure and temperature, the particular
                  phase adopted by a monolayer depends on the chain
                  length, the headgroup and its environment. The
                  present paper shows that, although not all
                  structures are possible, the same structures recur
                  under suitable conditions with different headgroups
                  and different environments, including the
                  commensurate environment provided by a multilayer
                  structure. The common factor limiting the choice of
                  structure appears to be the aliphatic chains, and
                  the particular structure which actually occurs
                  appears to be determined by interaction of the
                  environment with the headgroups. This idea is
                  developed and shown to predict correctly the
                  variation with chain length of two of the
                  upright-to-tilted phase transitions observed in
                  monolayers on the water surface. It is possible to
                  talk about a bulk pressure within the monolayer,
                  which is influenced by the interactions at the
                  monolayer interfaces and which controls the
                  molecular tilt.}
}

