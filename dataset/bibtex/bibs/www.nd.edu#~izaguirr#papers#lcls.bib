  pages =     {8978-8982},
  nack =     {This work supported in part by the National Science
                  Foundation Grant CCR-9408973.},
  abstract =     {Symplectic numerical integrators, such as the
                  St\"ormer-Verlet method, are useful in preserving
                  certain important properties that are not preserved
                  by conventional numerical integrators. This paper
                  analyzes the St\"ormer-Verlet method as applied to
                  the simple harmonic model, which is an important
                  model for molecular dynamics
                  simulations. Restricting our attention to the
                  one-dimensional case, both the exact solution and
                  the St\"ormer-Verlet solution to this model are
                  expressed as functions of the number of time steps
                  taken, and then both of these functions are
                  interpreted geometrically. The paper shows the
                  existence of an upper bound on the error from the
                  St\"ormer-Verlet method, and then an example is
                  worked to demonstrate the closeness of this bound.}
}

@article{HaOk??,
  author =     {D.~J. Hardy and D. I. Okunbor},
  title =     {Efficient Symplectic Multiple Time Step Integration},
  note =     {work in progress},
  nack =     {This work was supported in part by National Science
                  Foundation Grant CCR-9408973.},
  abstract =     {Multiple time stepping increases the efficiency of
                  the leapfrog/St\"ormer/Verlet integrator while
                  retaining symplecticness, time-reversibility, and
                  second-order accuracy. The potential energy function
                  is partitioned to allow different parts of the
                  system to be integrated using multiples of a
                  smallest stepsize. We present efficient algorithms
                  for this approach for both bonded and nonbonded
                  forces, along with numerical experiments that
                  demonstrate the increased efficiency.}
}

