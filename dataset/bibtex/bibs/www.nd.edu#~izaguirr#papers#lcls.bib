@Article{LPSm80,
  author =      {S. W. de Leeuw and J. W. Perram and E. R. Smith},
  title =      {Simulation of electrostatic systems in periodic boundary conditions},
  journal =      {Proceedings of the Royal Society of London. Series A, Mathematical and Physical Sciences},
  year =      1980,
  volume =     373,
  number =     1752,
  pages =     {27-56},
  annote =     {The effective interactions of ions, dipoles and
  higher-order multipoles under periodic boundary conditions are
  calculated where the array of periodic replications forms an
  infinite sphere surrounded by a vacuum. Discrepancies between the
  results of different methods of calculation are resolved and some
  shape-dependent effects are discussed briefly. In a simulation under
  these periodic boundary conditions, the net Hamiltonian contains a
  positive term proportional to the square of the net dipole moment of
  the configuration. Surrounding the infinite sphere by a continuum of
  dielectric constant ε' changes this positive term, the coefficient
  being zero as    . We report on the simulation of a dense fluid of hard
  spheres with embedded point dipoles; simulations are made for
  different values of ε', showing how the Kirkwood g-factor and the
  long-range part of h Δ (r) depend on ε' in a finite simulation. We
  show how this dependence on ε' nonetheless leads to a dielectric
  constant for the system that is independent of ε'. In particular,
  the Clausius-Mosotti and Kirkwood formulae for the dielectric
  constant ε of the system give consistent values. }
}

