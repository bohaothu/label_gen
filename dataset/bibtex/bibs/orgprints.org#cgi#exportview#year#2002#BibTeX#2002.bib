@misc{orgprints468,
          editor = {G. Kjellsson and B. Boelt},
           title = {{\O}kologisk jordbrug nu og i fremtiden},
          author = {B. Boelt and I. Bertelsen},
            year = {2002},
          series = {F{\O}JO-rapport},
           pages = {31--43},
         journal = {Konsekvenser af genmodificerede afgr{\o}der for {\o}kologisk jordbrug},
             url = {http://orgprints.org/468/},
        abstract = {Det ideologiske grundlag for {\o}kologisk jordbrug bygger p{\ra} tre fundamentale principper: kredsl{\o}b, forsigtighed og n{\ae}rhed. Anvendelse af genmodificerede organismer (GMO) er ikke tilladt og begrundelsen herfor er 
? uacceptable risici i forhold til f{\o}devarernes indvirken p{\ra} sundheden
? uacceptable risici i forhold til milj{\o}et
? den irreversible natur af de genetiske {\ae}ndringer og det faktum, at de {\ae}ndrede gener uundg{\ra}eligt vil sprede sig b{\ra}de i afgr{\o}der og ukrudtsplanter
? udbredelsen af gensplejsede afgr{\o}der forhindrer b{\ra}de forbrugeres og landm{\ae}nds muligheder for frit at frav{\ae}lge gensplejsede afgr{\o}der og f{\o}devarer.
Der har gennem de seneste {\ra}r v{\ae}ret en stigning i antallet af {\o}kologiske jordbrugsbedrifter, hvilket har medf{\o}rt en stigning i det {\o}kologiske areal, s{\ra}ledes at 6,5\% af det totale landbrugsareal i Danmark nu drives {\o}kologisk. Stigningen har overvejende v{\ae}ret for{\ra}rsaget af en st{\o}rre interesse for oml{\ae}gning af planteavlsbedrifter, og denne produktionsgren d{\ae}kkede i 2000 34\% af det {\o}kologiske areal.
Arealfordelingen inden for bedriftstype viser, at 53\% af det {\o}kologiske jordbrugsareal anvendes i kv{\ae}gbedrifter, medens kun 5\% anvendes i svinebedrifter. Imidlertid er produktionen af mejeriprodukter for {\o}jeblikket st{\o}rre end forbruget p{\ra} hjemmemarkedet, og den fremtidige udvikling inden for denne produktionsgren er i h{\o}j grad afh{\ae}ngig af firmaernes succes p{\ra} eksportmarkederne. Der er for {\o}jeblikket en meget positiv situation for oml{\ae}gning til {\o}kologisk planteavl som f{\o}lge af en gunstig prisudvikling p{\ra} bl.a. korn og {\ae}rter. Imidlertid er n{\ae}ringsstofforsyningen begr{\ae}nsende for udbyttets st{\o}rrelse p{\ra} husdyrl{\o}se planteavlsbrug. Der vurderes at v{\ae}re gode muligheder for en st{\o}rre afs{\ae}tning af {\o}kologisk svinek{\o}d, men den nuv{\ae}rende produktion er meget beskeden.
Forbruget af {\o}kologiske f{\o}devarer i Danmark er stagneret inden for de seneste {\ra}r. Imidlertid synes forbrugeren fortsat at have stor tillid til de {\o}kologiske produkter, og som begrundelse for valg af {\o}kologiske f{\o}devarer n{\ae}vnes hensyn til sundhed, milj{\o} og dyrevelf{\ae}rd. I den forbindelse fremh{\ae}ver en r{\ae}kke interviewede eksperter, at {\o}kologiens styrke er, at der ikke anvendes GMO.
For {\o}jeblikket anvendes ikke-{\o}kologiske produkter af uds{\ae}d, g{\o}dning og foder. Anvendelsen af ikke-{\o}kologisk uds{\ae}d foreg{\ra}r imidlertid p{\ra} dispensation, og tilladelse til brug af ikke-{\o}kologisk foder g{\ae}lder kun indtil efter{\ra}ret 2005.
Den fremtidige udvikling inden for {\o}kologisk jordbrug forventes i stor udstr{\ae}kning at p{\ra}virkes af tilg{\ae}ngeligheden af 
? afs{\ae}tning af de {\o}kologiske produkter s{\ra}vel i ind- som udland.
? {\o}kologisk uds{\ae}d af "passende" sorter
? {\o}kologisk foder ? specielt proteinafgr{\o}der, samt
? tilstr{\ae}kkelig n{\ae}ringsstofforsyning i de {\o}kologiske s{\ae}dskifter, is{\ae}r p{\ra} husdyrl{\o}se planteavlsbedrifter.
}
}

@misc{orgprints467,
          editor = {G. Kjellsson and B. Boelt},
           title = {Kontrolleret spredning},
          author = {B. Boelt and I. Bertelsen},
            year = {2002},
          series = {F{\O}JO-rapport},
           pages = {45--61},
         journal = {Konsekvenser af genmodificerede afgr{\o}der for {\o}kologisk jordbrug},
             url = {http://orgprints.org/467/},
        abstract = {Det er tilladt at anvende ikke-{\o}kologiske produkter i {\o}kologisk produktion. Det drejer sig om uds{\ae}d, g{\o}dning og foder. Import af ikke-{\o}kologiske produkter vurderes at udg{\o}re en potentiel risiko for spredning af genmodificerede organismer (GMO) til det {\o}kologiske jordbrug, og denne risiko forventes at stige i takt med en stigende anvendelse af GM-planter i det konventionelle jordbrug s{\ra}vel uden for EU som ved fors{\o}gsuds{\ae}tninger i EU.
Forsyningen med {\o}kologisk uds{\ae}d er mangelfuld inden for b{\ae}lgs{\ae}d, hvidkl{\o}ver, lucerne, roer og gr{\o}nsager. En dansk ekspertgruppe  opbygger hvert {\ra}r en liste af "passende" sorter inden for en given art, og hvis forsyningen af {\o}kologisk uds{\ae}d af "passende" sort er mangelfuld, anvendes ikke-{\o}kologisk ubejdset uds{\ae}d. Dette kan imidlertid v{\ae}re problematisk i de tilf{\ae}lde, hvor uds{\ae}den er opformeret i et omr{\ra}de, hvor der har v{\ae}ret uds{\ae}tningsfors{\o}g med GM-sorter, eller hvor disse generelt anvendes i dyrkningen. I s{\ra}danne tilf{\ae}lde forventes der at v{\ae}re en risiko for spredning af GM-egenskaber ved fr{\o}- og pollenspredning, hvilket bekr{\ae}ftes af franske unders{\o}gelser af majs i 2001. Risikoen for indblanding af GMO ved anvendelse af ikke-{\o}kologisk uds{\ae}d er aktuel for raps og majs. For andre afgr{\o}der som b{\ae}lgs{\ae}d, roer, kl{\o}ver, gr{\ae}s, lucerne og gr{\o}nsager vil der p{\ra} sigt v{\ae}re en tilsvarende risiko.
Is{\ae}r de {\o}kologiske planteavlsbedrifter anvender ikke-{\o}kologiske g{\o}dningsmidler i stor udstr{\ae}kning, da tilstedev{\ae}relsen af {\o}kologisk husdyrg{\o}dning er utilstr{\ae}kkelig til at d{\ae}kke n{\ae}ringsstofbehovet i {\O}st-danmark. Den importerede organiske g{\o}dning er overvejende ikke-{\o}kologisk husdyrg{\o}dning, men det er ogs{\ra} tilladt at anvende byaffald. Disse produkter kan i nogen udstr{\ae}kning indeholde GMO eller rester heraf, men forekomsten af fr{\o} af GM-planter, som kan etableres og opformeres i et {\o}kologisk s{\ae}dskifte, vurderes at v{\ae}re meget beskeden.
{\O}kologiske svine- og fjerkr{\ae}producenter er meget afh{\ae}ngige af import af b{\ra}de {\o}kologisk og ikke-{\o}kologisk foder. St{\o}rst n{\ae}ringsv{\ae}rdi og foderudnyttelse opn{\ra}s, n{\ra}r korn suppleres med sojab{\o}nne, men anvendelsen af GM-sorter inden for sojab{\o}nne er imidlertid meget stor, og afgr{\o}den produceres fortrinsvis uden for EU. Der er i 2001 p{\ra}vist GM-soja i alle unders{\o}gte pr{\o}ver af ikke-{\o}kologiske foderblandinger i Danmark (i alt 12 pr{\o}ver er unders{\o}gt).
Omfanget af spredningsrisikoen i det {\o}kologiske s{\ae}dskifte er vurderet ud fra GM-fr{\o}ets evne til at etablere sig i marken, evne til at vokse og udvikle sig i en anden afgr{\o}de samt evne til at gennemf{\o}re en hel livscyklus med heraf f{\o}lgende risiko for fr{\o}spild. I den forbindelse fremst{\ra}r raps og specielt v{\ra}rraps som den art, som for {\o}jeblikket udg{\o}r den st{\o}rste spredningsrisiko.

}
}

