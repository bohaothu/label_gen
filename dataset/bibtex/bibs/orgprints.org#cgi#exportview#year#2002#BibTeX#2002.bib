           title = {The development of an integrated modelling system to support decisions on organic farms},
          author = {C. Julian and N Smith and David Oglethorpe and J Wright},
       publisher = {Organic Centre Wales, Institute of Rural Studies, University of Wales Aberystwyth},
            year = {2002},
           pages = {161--164},
