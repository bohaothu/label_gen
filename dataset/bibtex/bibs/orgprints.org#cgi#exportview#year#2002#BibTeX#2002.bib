@unpublished{orgprints176,
           title = {Rapport om skudafmodning i {\ae}ble},
          author = {Senior Scientist Hanne Lindhard and Senior Scientist Ole Callesen},
            year = {2002},
             url = {http://orgprints.org/176/},
        abstract = {Skudafmodning og endeknopdannelse i {\ae}bler afh{\ae}nger af sorten og af grundstammen. Der er i Forskergruppe for frugt og b{\ae}r indsamlet skudafmodningsdata i perioden 1986-2001.
Oprindeligt blev arbejdet sat i gang for at unders{\o}ge om skudafmodningstidspunktet relaterede til grundstammens vinterh{\ra}rf{\o}rhed.
I forbindelse med {\o}kologisk dyrkning har disse skudafmodnings data ogs{\ra} aktualitet, men af en anden {\ra}rsag. 
{\AE}bleskurv angriber hele s{\ae}sonen s{\ra} l{\ae}nge der er modtageligt l{\o}v. {\AE}ldre blade og skud er vanskelige eller helt umuligt for patogenet at inficere. Et v{\ae}sentlig skurvsmittepotentiale om for{\ra}ret er overvintrende skurv p{\ra} grene og knopsk{\ae}l. Smittekilden i tr{\ae}et kan ikke reduceres ved hj{\ae}lp af {\o}get oms{\ae}tning af bladmasse. Den overvintrende bladmasse anses som det v{\ae}senligste  smittepotentiale.
{\AE}bleskurv som overvintre i vedet er meget t{\ae}t p{\ra} det nye l{\o}v og frugter og har derfor en kort smittevej til modtaelig blade og frugter om for{\ra}ret. Derfor er det vigtigt at reducere mulighederne for at skurven overvintrer i grene og knopskel.
Ved tidlig skudafmodning og l{\o}vfald ville potentialet for udvikling af {\ae}bleskurv i en {\ae}bleplantager om efter{\ra}ret reduceres. N{\ra}r v{\ae}ksten stopper tidligt vil de potentielle infektions steder for svampen reduceres. Derved bliver skudafmodningstidspunktet ogs{\ra} relevant for {\o}kologisk produktion.
Hvis man ud fra viden om skudafmodning af grundstammer og sorter kan anbefale de tidligst afmodende til {\o}kologisk produktion ville risikoen for overvintrende {\ae}bleskurv og dermed smittetrykktet i plantagen det n{\ae}ste for{\ra}r reduceres.  }
}

@misc{orgprints1235,
          number = {4?2002},
           title = {Neue Regelungen im Tierarzneimittelrecht},
          author = {Matthias Link},
       publisher = {Bioland Verlags GmbH, Mainz},
            year = {2002},
           pages = {34--35},
         journal = {Bioland},
        keywords = {Arzneimittel, Tiermedizin, Veterin{\"a}rmedizin, Homoeopathie, Hom{\"o}opathie, hom{\"o}opathisch},
             url = {http://orgprints.org/1235/},
        abstract = {Das neue Tierarzneimittel-Neuordnungsgesetz ?TAMNOG?, das mit wenigen {\"A}nderungen bis Redaktionsschluss des Beitrags vor der Verk{\"u}ndigung stand, sieht im Wesentlichen vor:

Tier{\"a}rzte d{\"u}rfen in Zukunft
- keine Arzneimittel mehr aus apotheken- oder verschreibungspflichtigen Stoffen herstellen.
- konzentrierte Arzneimittel nur noch verd{\"u}nnen, wenn geeignete Konzentrationen nicht im Handel sind.
- Arzneimittel nur noch umf{\"u}llen und abpacken, wenn geeignete Packungsgr{\"o}{\ss}en nicht im Handel sind.
- nur Fertigarzneimittel beziehen, jedoch keine apotheken- oder verschreibungspflichtigen Grundstoffe.
- Arzneimittel nur noch f{\"u}r den Bedarf von maximal 7 Tagen an landwirtschaftliche Betriebe abgeben, es sei denn bestimmte Mittel m{\"u}ssen {\"u}ber l{\"a}ngere Zeit angewendet werden. Nicht antibiotische Stoffe und Antibiotika zur lokalen Anwendung d{\"u}rfen f{\"u}r den Bedarf von maximal 31 Tagen abgegeben werden, wenn der Bestand monatlich einmal vom Tierarzt begutachtet wird.
- umgewidmete Arzneimittel, die nicht f{\"u}r die zu behandelnde Tierart zugelassen sind, nur noch selber anwenden. Solche Arzneimittel d{\"u}rfen nur noch abgegeben werden, wenn der Bestand regelm{\"a}{\ss}ig tier{\"a}rztlich untersucht wird.

Weiterhin
- d{\"u}rfen Arzneimittel nur noch auf Verschreibung in Apotheken und nur f{\"u}r nicht lebensmittelliefernde Tiere hergestellt werden.
- m{\"u}ssen pharmazeutische Unternehmen und Gro{\ss}h{\"a}ndler die Abgabe von Arzneimitteln an Tier{\"a}rzte offiziell melden.
- d{\"u}rfen F{\"u}tterungsarzneimittel maximal 3 Wirkstoffe enthalten, davon h{\"o}chstens 2 antibiotisch wirksame.

Der Beitrag beschreibt und beurteilt die {\"A}nderungen in der Gesetzgebung im Hinblick auf den {\"o}kologischen Landbau und geht dabei insbesondere auf die Behandlung von Nutztieren mit Hom{\"o}opathika ein.}
}

