          author = {P. Ambus},
            year = {2002},
         journal = {Landsbladet Mark},
             url = {http://orgprints.org/4157/},
        abstract = {Kl{\o}vergr{\ae}s er et vigtigt element i {\o}kologisk m{\ae}lkeproduktion. Flere unders{\o}gelser har dog antydet at denne driftsform rummer en risiko for u{\o}nskede udslip af gasformige kv{\ae}lstof-forbindelser til omgivelserne. En af de gasformer der er s{\ae}rlig opm{\ae}rksomhed omkring, er lattergas, der er en kraftig drivhusgas med en v{\ae}sentlig betydning for den globale opvarmning. Af det samlede danske udslip af drivhusgasser, udg{\o}r lattergas mere end 10\%, hovedsalig udledt ved landbrugsaktivitet. Biologisk kv{\ae}lstof-fiksering er en n{\o}gleproces i kl{\o}vergr{\ae}ssystemer, men vores viden om processen er endnu ufuldst{\ae}ndig, specielt n{\ra}r det g{\ae}lder bidraget fra rodbiomassen. Af samme grund eksisterer der heller ingen viden om, hvor meget af det fikserede kv{\ae}lstof, som atter frigives til atmosf{\ae}ren som lattergas.
{\O}kologisk produktion anses af mange som et initiativ, der kan mindske landbrugets p{\ra}virkning af det omgivende milj{\o}, men for at give en nuanceret vurdering heraf er det dog n{\o}dvendigt at tilvejebringe yderligere viden om samspillet mellem landbrugsproduktion og milj{\o}. Det er p{\ra} denne baggrund at Danmarks JordbrugsForskning og Forskningscenter Ris{\o} gennemf{\o}rer et projekt med det form{\ra}l at bestemme st{\o}rrelsen, reguleringen og sammenh{\ae}ngen mellem kv{\ae}lstof-fiksering og lattergasfrigivelse i kl{\o}vergr{\ae}smarker. 
Resultaterne fra projektet vil indg{\ra} i opbygningen af en model, der beskriver transporten af kv{\ae}lstof fra stald til mark p{\ra} den enkelte bedrift. Kv{\ae}lstof-fiksering er desuden d{\ra}rligt unders{\o}gt p{\ra} de mere sandede jorder, som netop indg{\ra}r i projektet, og resultaterne herfra vil derfor v{\ae}re et v{\ae}rdifuldt bidrag til r{\ra}dgivningstjenesters beslutningsgrundlag.
Resultaterne fra f{\o}rste fors{\o}gs{\ra}r viser en maksimal kv{\ae}lstof-fiksering p{\ra} 185 kg ha-1 i 2. produktions{\ra}r faldende 115 kg i 8. produktions{\ra}r. I lighed hermed fandt vi at lattergasudslippet var st{\o}rst i de f{\o}rste produktions{\ra}r (235-290 g N ha-1) faldende til 166 g N i 8. {\ra}r. Samlet set viser disse resultater at tabet af lattergas i kl{\o}vergr{\ae}s udg{\o}r en beskeden del af det fikserede N.}
}

@unpublished{orgprints123,
           title = {Sources of N2O in organic grass-clover pastures},
          author = {P. Ambus},
            year = {2002},
             url = {http://orgprints.org/123/},
        abstract = {Organic farming practises, and in particular dairy production systems based on grass-clover pastures are becoming increasingly abundant within Danish agriculture. Grass-clover pastures may provide a mitigation option to reduce grassland nitrous oxide (N2O) emissions (Velthof et al. 1998).
The objectives of this work was to examine the relationship between N2O emissions and transformations of inorganic N in organically managed grass-clover pastures of different ages. Results from the project will be used for calibration of the FASSET whole-farm nitrogen transformation model.
}
}

@misc{orgprints481,
          number = {265},
           month = {May},
           title = {Unders{\o}gelse af kv{\ae}lstofbinding og lattergas fra kl{\o}vergr{\ae}smarker},
          author = {Per Ambus},
            year = {2002},
         journal = {{\O}kologisk Jordbrug},
             url = {http://orgprints.org/481/},
        abstract = {{\O}kologisk produktion anses af mange som et initiativ, der kan mindske landbrugets p{\ra}virkning af det omgivende milj{\o}. 

For at give en nuanceret vurdering af, hvordan {\o}kologiske bedrifter p{\ra}virker milj{\o}et, er det dog n{\o}dvendigt at tilvejebringe yderligere viden om samspillet mellem landbrugsproduktion og milj{\o}. Kv{\ae}gs afgr{\ae}sning af kl{\o}vergr{\ae}smarker er et eksempel p{\ra}, at den eksisterende viden endnu er mangelfuld, selvom det er et vigtigt element i {\o}kologisk m{\ae}lkeproduktion. 

Flere unders{\o}gelser peger p{\ra}, at denne driftsform rummer en risiko for u{\o}nskede udslip af gasformige kv{\ae}lstofforbindelser til omgivelserne. Et f{\ae}nomen, der kan observeres fra is{\ae}r urinpletterne efter det gr{\ae}ssende kv{\ae}g. Samtidig medf{\o}rer kl{\o}verets evne til at binde atmosf{\ae}risk kv{\ae}lstof, at der ofte er et h{\o}jt indhold af kv{\ae}lstof i rodzonen, hvilket yderligere forst{\ae}rker risikoen for store udledninger af gasformigt kv{\ae}lstof fra kl{\o}vergr{\ae}smarken. 

En af de gasformer, der er s{\ae}rlig opm{\ae}rksomhed omkring, er lattergas. Lattergas er en kraftig drivhusgas og har en v{\ae}sentlig betydning for den globale opvarmning. Af det samlede danske udslip af drivhusgasser udg{\o}r lattergas mere end 10\%, hovedsalig udledt ved landbrugsaktivitet.

Biologisk kv{\ae}lstoffiksering er en n{\o}gleproces i kl{\o}vergr{\ae}ssystemer, men vores viden om processen er endnu ufuldst{\ae}ndig, specielt n{\ra}r det g{\ae}lder bidraget fra rodbiomassen. Af samme grund eksisterer der heller ingen viden om, hvor meget af det fikserede kv{\ae}lstof, der atter frigives til atmosf{\ae}ren som lattergas.

Det er p{\ra} denne baggrund, Danmark JordbrugsForskning og Forskningscenter Ris{\o} gennemf{\o}rer et projekt med det form{\ra}l at bestemme st{\o}rrelsen, reguleringen og sammenh{\ae}ngen mellem kv{\ae}lstoffiksering og lattergasfrigivelse i kl{\o}vergr{\ae}smarker. 

I projektet indg{\ra}r unders{\o}gelser p{\ra} fors{\o}gsmarker ved Forskningscenter Foulum og p{\ra} studielandbrug. Her bestemmer vi markens totale biologiske fiksering af kv{\ae}lstof og l{\ae}gger s{\ae}rlig v{\ae}gt p{\ra} at klarl{\ae}gge hvor stort bidraget af rodbiomassen er til den samlede kv{\ae}lstoffiksering. Samtidig bestemmer vi markens frigivelse af lattergas under indflydelse af varierende dyret{\ae}thed og jordbundsforhold. Projektet indeholder endvidere studier af de grundl{\ae}ggende fysiske, kemiske og biologiske processer i rodzonen, som er styrende for frigivelsen af lattergas. Disse studier gennemf{\o}res ved Forskningscenter Ris{\o} i fors{\o}gsopstillinger med bl.a. simuleret gr{\ae}sning.

Resultaterne fra projektet vil indg{\ra} i opbygningen af en model, der beskriver transporten af kv{\ae}lstof fra stald til mark p{\ra} den enkelte bedrift. Kv{\ae}lstoffiksering er desuden d{\ra}rligt unders{\o}gt p{\ra} de mere sandede jorder, som netop indg{\ra}r i projektet, og resultaterne herfra vil derfor v{\ae}re et v{\ae}rdifuldt bidrag til beslutningsgrundlaget for r{\ra}dgivningen. 

Det overordnede afkast af projektet er dog ny viden om de milj{\o}m{\ae}ssige konsekvenser af {\o}kologiske dyrkningsformer baseret p{\ra} kl{\o}vergr{\ae}s, hvilket er en n{\o}dvendig foruds{\ae}tning for at kunne give en overordnet helhedsvurdering af milj{\o}effekten af {\o}kologiske driftsformer.}
}

