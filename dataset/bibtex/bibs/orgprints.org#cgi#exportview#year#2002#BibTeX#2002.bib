@misc{orgprints299,
           month = {November},
           title = {Deltagerdriven forskning ? v{\"a}xtodlingsgruppen: Resultat och utv{\"a}rdering av arbetet under 1998 till 2001},
          author = {Karin Svan{\"a}ng and G{\"o}sta Roempke and G{\"o}ran Eriksson and Lars-Birger Johansson and Torvald Lund and Lennart Karlsson and Tommy {\"O}hman and Jan Ihrs{\'e}n and Owe Pettersson and Kjell Eriksson and Christer Jacobsson},
