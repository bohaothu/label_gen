<a name="cl-9470"></a>@article{Yuksel:2006fj,
<a name="cl-9471"></a>	Archiveprefix = {arXiv},
<a name="cl-9472"></a>	Author = {{Y\"uksel}, Hasan},
<a name="cl-9473"></a>	Date-Modified = {2010-09-15 22:44:01 -0700},
<a name="cl-9474"></a>	Doi = {10.1016/j.nuclphysbps.2007.08.031},
<a name="cl-9475"></a>	Eprint = {astro-ph/0609139},
<a name="cl-9476"></a>	Journal = npbpc,
<a name="cl-9477"></a>	Pages = {83--86},
<a name="cl-9478"></a>	Slaccitation = {%%CITATION = ASTRO-PH/0609139;%%},
<a name="cl-9479"></a>	Title = {Positron Annihilations at the Galactic Center: Generating More Questions Than Answers},
<a name="cl-9480"></a>	Volume = {173},
<a name="cl-9481"></a>	Year = {2007},
<a name="cl-9482"></a>	Abstract = {The bulge of our Galaxy is illuminated by the 0.511
<a name="cl-9483"></a>{MeV} gamma-ray line flux from annihilations of
<a name="cl-9484"></a>nonrelativistic positrons. The emission is strongly
<a name="cl-9485"></a>concentrated at the Galactic Center, in contrast to
<a name="cl-9486"></a>gamma-ray maps tracing nucleosynthesis (e.g., the
<a name="cl-9487"></a>1.809 {MeV} line from decaying ^26Al) or cosmic ray
<a name="cl-9488"></a>processes (e.g., the 1-30 {MeV} continuum), which
<a name="cl-9489"></a>reveal a bright disk with a much less prominent
<a name="cl-9490"></a>central region. If positrons are generated at
<a name="cl-9491"></a>relativistic energies, higher-energy gamma rays will
<a name="cl-9492"></a>also be produced from inflight annihilation of
<a name="cl-9493"></a>positrons on ambient electrons. The comparison of
<a name="cl-9494"></a>the gamma-ray spectrum from inflight annihilation to
<a name="cl-9495"></a>the observed diffuse Galactic gamma-ray data
<a name="cl-9496"></a>constrains the injection energies of Galactic
<a name="cl-9497"></a>positrons to be less than 3 {MeV}.},
<a name="cl-9498"></a>	Bdsk-File-1 = {},
<a name="cl-9499"></a>	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.nuclphysbps.2007.08.031}}
