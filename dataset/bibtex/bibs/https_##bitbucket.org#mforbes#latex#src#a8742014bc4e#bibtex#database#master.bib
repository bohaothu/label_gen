<a name="cl-9552"></a>	Adsurl = {http://adsabs.harvard.edu/abs/1978ApJ...225L..11L},
<a name="cl-9553"></a>	Author = {{Leventhal}, M. and {MacCallum}, C.~J. and {Stang}, P.~D.},
