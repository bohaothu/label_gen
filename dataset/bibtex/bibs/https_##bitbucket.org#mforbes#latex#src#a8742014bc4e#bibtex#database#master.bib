<a name="cl-4557"></a>@article{Fayans:2000fk,
<a name="cl-4558"></a>	Author = {Fayans, S. A. and Tolokonnikov, S. V. and Trykov, E. L. and Zawischa, D.},
<a name="cl-4559"></a>	Comment = {This paper describes the method used by Aurel and Yongle Lu to calculate the structure of a vortex.},
