<a name="cl-10333"></a>	Journal = {Astrophys. J.},
<a name="cl-10334"></a>	Pages = {350-359},
<a name="cl-10335"></a>	Slaccitation = {%%CITATION = ASTRO-PH/0408292;%%},
<a name="cl-10336"></a>	Title = {Microwave ISM Emission in the Green Bank Galactic Plane Survey: Evidence for Spinning Dust},
<a name="cl-10337"></a>	Volume = 617,
<a name="cl-10338"></a>	Year = 2004,
<a name="cl-10339"></a>	Abstract = {We observe significant dust-correlated emission
