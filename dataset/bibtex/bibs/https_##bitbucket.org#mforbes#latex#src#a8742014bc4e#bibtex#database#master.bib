<a name="cl-21594"></a>	Pages = {475},
<a name="cl-21595"></a>	Slaccitation = {%%CITATION = PHLTA,B389,475;%%},
<a name="cl-21596"></a>	Title = {Domain Walls are Diamagnetic},
<a name="cl-21597"></a>	Volume = {B389},
<a name="cl-21598"></a>	Year = {1996},
<a name="cl-21599"></a>	Abstract = {It is shown that contrary to a recent claim in the
<a name="cl-21600"></a>literature, the domain walls made of scalar field are
<a name="cl-21601"></a>diamagnetic due to presence of massless fermionic modes
<a name="cl-21602"></a>on the wall. The diamagnetism vanishes at high
<a name="cl-21603"></a>temperature. Thus the domain walls could produce no
<a name="cl-21604"></a>effect on a primordial magnetic field in the early
<a name="cl-21605"></a>universe.}}
<a name="cl-21606"></a>
<a name="cl-21607"></a>@article{Voloshin:2000jp,
<a name="cl-21608"></a>	Archiveprefix = {arXiv},
