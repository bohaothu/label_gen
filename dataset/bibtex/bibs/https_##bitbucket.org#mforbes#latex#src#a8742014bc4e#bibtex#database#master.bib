<a name="cl-660"></a>@misc{Drut:2010a,
<a name="cl-661"></a>	Author = {Drut, Joaqu{\'\i}n E. and L{\"a}hde, Timo A. and Ten, Timour},
<a name="cl-662"></a>	Comments = {4 pages, 3 figures},
<a name="cl-663"></a>	Date-Added = {2011-01-28 17:36:15 -0800},
