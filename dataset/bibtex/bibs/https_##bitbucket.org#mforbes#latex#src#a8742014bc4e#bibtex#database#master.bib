<a name="cl-2594"></a>	Author = {Gaebler, J. P. and Stewart, J. T. and Drake, T. E. and Jin, D. S. and Perali, A. and Pieri, P. and Strinati, G. C.},
<a name="cl-2595"></a>	Date-Added = {2010-05-30 00:56:05 -0700},
