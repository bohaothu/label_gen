<a name="cl-2804"></a>@article{Chen:2005,
<a name="cl-2805"></a>	Author = {Chen, Qijin and Stajic, Jelena and Tan, Shina and Levin, K.},
<a name="cl-2806"></a>	Date-Added = {2010-05-30 00:10:20 -0700},
