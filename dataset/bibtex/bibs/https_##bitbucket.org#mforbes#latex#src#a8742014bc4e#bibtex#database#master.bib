<a name="cl-12096"></a>@article{Beane:2003da,
<a name="cl-12097"></a>	Archiveprefix = {arXiv},
<a name="cl-12098"></a>	Author = {Beane, S. R. and Bedaque, P. F. and Parre{\~n}o, A. and Savage, M. J.},
<a name="cl-12099"></a>	Date-Modified = {2010-08-04 03:20:39 -0700},
<a name="cl-12100"></a>	Doi = {10.1016/j.physletb.2004.02.007},
<a name="cl-12101"></a>	Eprint = {hep-lat/0312004},
