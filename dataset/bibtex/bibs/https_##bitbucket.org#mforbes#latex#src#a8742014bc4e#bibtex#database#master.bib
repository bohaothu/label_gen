<a name="cl-17585"></a>@article{Kogut:1999iv,
<a name="cl-17586"></a>	Archiveprefix = {arXiv},
<a name="cl-17587"></a>	Author = {Kogut, J. B. and Stephanov, M. A. and Toublan, D.},
