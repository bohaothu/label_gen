<a name="cl-166"></a>@article{Wu:2010,
<a name="cl-167"></a>	Archiveprefix = {arXiv},
<a name="cl-168"></a>	Author = {Wu, Lei and Li, Lu and Zhang, Jie-Fang and Mihalache, Dumitru and Malomed, Boris A. and Liu, W. M.},
<a name="cl-169"></a>	Date-Added = {2011-06-25 17:12:36 +0200},
