<a name="cl-1716"></a>	Date-Added = {2010-07-26 20:16:10 -0700},
<a name="cl-1717"></a>	Date-Modified = {2010-08-19 18:50:23 -0700},
<a name="cl-1718"></a>	Doi = {10.1103/PhysRevC.82.024602},
