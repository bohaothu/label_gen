<a name="cl-13278"></a>@article{Anderson:1958pb,
<a name="cl-13279"></a>	Author = {Anderson, P. W.},
<a name="cl-13280"></a>	Date-Modified = {2010-08-19 19:00:28 -0700},
<a name="cl-13281"></a>	Journal = pr,
<a name="cl-13282"></a>	Month = jan,
