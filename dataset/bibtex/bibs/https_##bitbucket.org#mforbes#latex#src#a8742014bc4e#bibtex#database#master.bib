<a name="cl-627"></a>@misc{Patton:2010,
<a name="cl-628"></a>	Author = {Patton, Kelly R. and Sheehy, Daniel E.},
<a name="cl-629"></a>	Comments = {5 pages, 3 figures},
<a name="cl-630"></a>	Date-Added = {2011-02-12 18:07:38 -0800},
