<a name="cl-560"></a>@misc{Trenkwalder:2010,
<a name="cl-561"></a>	Author = {Trenkwalder, A. and Kohstall, C. and Zaccanti, M. and Naik, D. and Sidorov, A. I. and Schreck, F. and Grimm, R.},
<a name="cl-562"></a>	Date-Added = {2011-02-12 18:20:51 -0800},
