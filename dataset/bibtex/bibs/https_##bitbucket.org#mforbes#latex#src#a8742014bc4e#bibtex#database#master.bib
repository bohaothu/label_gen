<a name="cl-15382"></a>@article{Forbes:2006ba,
<a name="cl-15383"></a>	Archiveprefix = {arXiv},
<a name="cl-15384"></a>	Author = {Forbes, Michael McNeil and Zhitnitsky, Ariel R.},
<a name="cl-15385"></a>	Date-Modified = {2010-08-04 04:07:47 -0700},
<a name="cl-15386"></a>	Eprint = {astro-ph/0611506},
<a name="cl-15387"></a>	Journal = jcap,
<a name="cl-15388"></a>	Keywords = {dark matter, x-rays, Chandra},
<a name="cl-15389"></a>	Number = 01,
<a name="cl-15390"></a>	Pages = {023},
<a name="cl-15391"></a>	Slaccitation = {%%CITATION = ASTRO-PH/0611506;%%},
<a name="cl-15392"></a>	Title = {Diffuse X-Rays: Directly Observing Dark Matter?},
<a name="cl-15393"></a>	Volume = 0801,
<a name="cl-15394"></a>	Year = 2008,
<a name="cl-15395"></a>	Abstract = {Several independent observations of the galactic
<a name="cl-15396"></a>core suggest hitherto unexplained sources of
<a name="cl-15397"></a>energy. We suggest that dark matter in the form of
<a name="cl-15398"></a>dense antimatter nuggets could provide a natural
<a name="cl-15399"></a>site for electron and proton annihilation, providing
<a name="cl-15400"></a>{511~keV} photons, gamma rays, and diffuse {keV}
<a name="cl-15401"></a>x-ray radiation. We show that identifying dark
<a name="cl-15402"></a>matter as antimatter nuggets is consistent with the
<a name="cl-15403"></a>observed emissions, and we make definite predictions
<a name="cl-15404"></a>about their spectrum and morphology. If correct, our
<a name="cl-15405"></a>proposal not only identifies dark matter and
<a name="cl-15406"></a>explains baryogenesis, but also allows x-ray
<a name="cl-15407"></a>observations to directly probe the matter
<a name="cl-15408"></a>distribution in our galaxy.},
<a name="cl-15409"></a>	Bdsk-File-1 = {},
<a name="cl-15410"></a>	Bdsk-Url-1 = {http://www.iop.org/EJ/abstract/1475-7516/2008/01/023/},
<a name="cl-15411"></a>	Bdsk-Url-2 = {http://dx.doi.org/10.1088/1475-7516/2008/01/023}}
<a name="cl-15412"></a>
<a name="cl-15413"></a>@article{Forbes:2008uf,
<a name="cl-15414"></a>	Archiveprefix = {arXiv},
<a name="cl-15415"></a>	Author = {Forbes, Michael McNeil and Zhitnitsky, Ariel R.},
<a name="cl-15416"></a>	Date-Modified = {2010-08-04 03:20:39 -0700},
<a name="cl-15417"></a>	Eprint = {0802.3830},
<a name="cl-15418"></a>	Journal = prd,
<a name="cl-15419"></a>	Keywords = {WMAP haze, dark matter, baryogenisis},
<a name="cl-15420"></a>	Number = {8},
<a name="cl-15421"></a>	Pages = {083505},
<a name="cl-15422"></a>	Primaryclass = {astro-ph},
<a name="cl-15423"></a>	Slaccitation = {%%CITATION = 0802.3830;%%},
<a name="cl-15424"></a>	Title = {{WMAP Haze: Directly Observing Dark Matter?}},
<a name="cl-15425"></a>	Volume = {78},
<a name="cl-15426"></a>	Year = 2008,
<a name="cl-15427"></a>	Abstract = {In this paper, we show that dark matter in the form of dense matter/antimatter nuggets could provide a natural and unified explanation for several distinct bands of diffuse radiation from the core of the Galaxy spanning over 13 orders of magnitude in frequency. We fix all of the phenomenological properties of this model by matching to x-ray observations in the keV band, and then calculate the unambiguously predicted thermal emission in the microwave band, at frequencies smaller by 11 orders of magnitude. Remarkably, the intensity and spectrum of the emitted thermal radiation are consistent with---and could entirely explain---the so-called ``WMAP haze'': a diffuse microwave excess observed from the core of our Galaxy by the Wilkinson Microwave Anisotropy Probe (WMAP). This provides another strong constraint of our proposal, and a remarkable nontrivial validation. If correct, our proposal identifies the nature of the dark matter, explains baryogenesis, and provides a means to directly probe the matter distribution in our Galaxy by analyzing several different types of diffuse emissions.},
<a name="cl-15428"></a>	Bdsk-File-1 = {},
<a name="cl-15429"></a>	Bdsk-Url-1 = {http://link.aps.org/abstract/PRD/v78/e083505},
<a name="cl-15430"></a>	Bdsk-Url-2 = {http://dx.doi.org/10.1103/PhysRevD.78.083505}}
