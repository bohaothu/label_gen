<a name="cl-17094"></a>@article{Iwazaki:1997xm,
<a name="cl-17095"></a>	Archiveprefix = {arXiv},
<a name="cl-17096"></a>	Author = {Iwazaki, Aiichi},
<a name="cl-17097"></a>	Eprint = {hep-ph/9703237},
<a name="cl-17098"></a>	Journal = {Phys. Lett.},
<a name="cl-17099"></a>	Pages = {304--308},
<a name="cl-17100"></a>	Slaccitation = {%%CITATION = PHLTA,B406,304;%%},
<a name="cl-17101"></a>	Title = {Ferromagnetism of axion domain wall},
<a name="cl-17102"></a>	Volume = {B406},
<a name="cl-17103"></a>	Year = {1997},
<a name="cl-17104"></a>	Abstract = {We show that axion domain wall is ferromagnetic in the
<a name="cl-17105"></a>universe with nonvanishing baryon number or lepton
<a name="cl-17106"></a>number. It is caused by protons and electrons bounded
<a name="cl-17107"></a>to the domain wall with their spins aligned. These
<a name="cl-17108"></a>bound states arise due to attractive potentials
<a name="cl-17109"></a>generated through pseudo-vector couplings between the
<a name="cl-17110"></a>fermions and the axion. Using a model of hadronic
<a name="cl-17111"></a>axions we predict existence of a primordial magnetic
<a name="cl-17112"></a>field with strength {$10^{-13}$} Gauss at
<a name="cl-17113"></a>recombination.}}
