<a name="cl-18502"></a>	Slaccitation = {%%CITATION = HEP-PH 0001095;%%},
<a name="cl-18503"></a>	Title = {Masses of the Goldstone modes in the CFL phase of QCD at finite density},
<a name="cl-18504"></a>	Volume = {B479},
<a name="cl-18505"></a>	Year = 2000,
<a name="cl-18506"></a>	Abstract = {We construct the U_L(3) x U_R(3) effective
