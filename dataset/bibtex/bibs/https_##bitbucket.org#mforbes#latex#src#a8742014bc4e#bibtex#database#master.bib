<a name="cl-17425"></a>	Slaccitation = {%%CITATION = HEP-LAT 0106019;%%},
<a name="cl-17426"></a>	Title = {Lattice {QCD} at high temperature and density},
<a name="cl-17427"></a>	Volume = 583,
<a name="cl-17428"></a>	Year = 2002,
<a name="cl-17429"></a>	Abstract = {After a brief introduction into basic aspects of the
