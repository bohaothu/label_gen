<a name="cl-611"></a>@misc{Salasnich:2010a,
<a name="cl-612"></a>	Author = {Salasnich, Luca},
<a name="cl-613"></a>	Comments = {7 pages, 6 figures, accepted to publication in Phys. Rev. A},
<a name="cl-614"></a>	Date-Added = {2011-02-12 18:10:27 -0800},
<a name="cl-615"></a>	Date-Modified = {2011-02-12 18:10:59 -0800},
