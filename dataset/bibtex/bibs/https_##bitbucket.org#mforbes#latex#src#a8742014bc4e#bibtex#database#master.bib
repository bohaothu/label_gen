<a name="cl-2837"></a>@article{Timmermans:1999,
<a name="cl-2838"></a>	Author = {Timmermans, Eddy and Tommasini, Paolo and Hussein, Mahir and Kerman, Arthur},
<a name="cl-2839"></a>	Date-Added = {2010-05-30 00:04:10 -0700},
<a name="cl-2840"></a>	Date-Modified = {2010-08-04 04:02:19 -0700},
