<a name="cl-17081"></a>	Journal = prd,
<a name="cl-17082"></a>	Pages = {2435--2438},
<a name="cl-17083"></a>	Slaccitation = {%%CITATION = PHRVA,D56,2435;%%},
<a name="cl-17084"></a>	Title = {Ferromagnetic domain wall and primordial magnetic field},
<a name="cl-17085"></a>	Volume = {56},
<a name="cl-17086"></a>	Year = {1997},
<a name="cl-17087"></a>	Abstract = {We show that coherent magnetic field is generated
<a name="cl-17088"></a>spontaneously when a large domain wall is created in
<a name="cl-17089"></a>the early universe. It is caused by two dimensional
<a name="cl-17090"></a>massless fermions bounded to the domain wall soliton.
<a name="cl-17091"></a>We point out that the magnetic field is a candidate of
<a name="cl-17092"></a>primordial magnetic field.}}
