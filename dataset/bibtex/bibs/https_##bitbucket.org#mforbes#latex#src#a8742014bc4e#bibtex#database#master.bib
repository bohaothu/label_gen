<a name="cl-12297"></a>@article{LC:2002,
<a name="cl-12298"></a>	Author = {Littlejohn, Robert G. and Cargo, Matthew},
<a name="cl-12299"></a>	Date-Modified = {2010-08-19 19:00:40 -0700},
<a name="cl-12300"></a>	Doi = {10.1063/1.1481388},
<a name="cl-12301"></a>	Group = {Algorithms},
<a name="cl-12302"></a>	Journal = {J.~Chem.~Phys.},
<a name="cl-12303"></a>	Keywords = {quantum theory; eigenvalues and eigenfunctions; Bessel functions; Hankel transforms; interpolation; matrix algebra; DVR},
<a name="cl-12304"></a>	Month = jul,
<a name="cl-12305"></a>	Number = 1,
<a name="cl-12306"></a>	Pages = {27-36},
<a name="cl-12307"></a>	Publisher = {AIP},
<a name="cl-12308"></a>	Title = {Bessel discrete variable representation bases},
<a name="cl-12309"></a>	Volume = 117,
<a name="cl-12310"></a>	Year = 2002,
<a name="cl-12311"></a>	Abstract = {Discrete variable representation (DVR) basis sets on
<a name="cl-12312"></a>the radial half-line, based on Bessel functions, are
<a name="cl-12313"></a>presented. These are Hankel transforms of the
<a name="cl-12314"></a>eigenfunctions of the particle in a spherical box in
<a name="cl-12315"></a>k space, but there is no box or bound on the radial
<a name="cl-12316"></a>variable r. The grid points extend to infinity on
<a name="cl-12317"></a>the r axis. The DVR functions are exactly
<a name="cl-12318"></a>orthonormal and exactly satisfy the interpolation
<a name="cl-12319"></a>properties usually associated with DVR
<a name="cl-12320"></a>functions. The exact matrix elements of the kinetic
<a name="cl-12321"></a>energy are computed, and the use of the Bessel DVR
<a name="cl-12322"></a>functions in radial eigenvalue problems is
<a name="cl-12323"></a>illustrated. The phase space or semiclassical
<a name="cl-12324"></a>interpretation of the Bessel DVR functions is
<a name="cl-12325"></a>presented, and variations on these functions,
<a name="cl-12326"></a>corresponding to alternative boundary conditions in
<a name="cl-12327"></a>k space, are discussed. An interesting feature of
<a name="cl-12328"></a>Bessel DVR functions is that they are based on a
<a name="cl-12329"></a>finite basis representation that is continuously
<a name="cl-12330"></a>infinite.},
<a name="cl-12331"></a>	Bdsk-File-1 = {},
<a name="cl-12332"></a>	Bdsk-Url-1 = {http://dx.doi.org/10.1063/1.1481388},
<a name="cl-12333"></a>	Bdsk-Url-2 = {http://link.aip.org/link/?JCP/117/27/1}}
