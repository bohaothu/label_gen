<a name="cl-649"></a>	Keywords = {unitary gas; cold atoms; Monte Carlo; fermions; structure factor; contact},
<a name="cl-650"></a>	Number = {5},
<a name="cl-651"></a>	Pages = {695},
<a name="cl-652"></a>	Title = {{M}olecular signatures in the structure factor of an interacting {F}ermi gas},
<a name="cl-653"></a>	Volume = {75},
<a name="cl-654"></a>	Year = {2006},
<a name="cl-655"></a>	Abstract = {The static and dynamic structure factors of an interacting Fermi gas along the BCS-BEC crossover are calculated at momentum transfer $\hbar{\bf k}$ higher than the Fermi momentum. The spin structure factor is found to be very sensitive to the correlations associated with the formation of molecules. On the BEC side of the crossover, even close to unitarity, clear evidence is found for a molecular excitation at $\hbar^2 k^2 /4m$, where $m$ is the atomic mass. Both quantum Monte Carlo and dynamic mean-field results are presented.},
<a name="cl-656"></a>	Bdsk-File-1 = {},
