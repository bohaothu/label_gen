<a name="cl-13314"></a>@article{Applegate:1985qt,
<a name="cl-13315"></a>	Author = {Applegate, J. H. and Hogan, C. J.},
<a name="cl-13316"></a>	Eref = {http://publish.aps.org/abstract/PRD/V31/P3037/},
<a name="cl-13317"></a>	Journal = prd,
