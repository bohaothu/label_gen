<a name="cl-16441"></a>	Date-Modified = {2010-08-19 19:00:16 -0700},
<a name="cl-16442"></a>	Editor = {K.~L.~Chan and K.~S.~Cheng and H.~P.~Singh},
<a name="cl-16443"></a>	Eprint = {astro-ph/0012541},
<a name="cl-16444"></a>	Keywords = {Astrophysics},
