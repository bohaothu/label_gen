<a name="cl-14571"></a>	Slaccitation = {%%CITATION = HEP-PH 0302077;%%},
<a name="cl-14572"></a>	Title = {Aspects of the color flavor locking phase of QCD in the Nambu-Jona Lasinio approximation},
<a name="cl-14573"></a>	Volume = {68},
<a name="cl-14574"></a>	Year = 2003,
