<a name="cl-13770"></a>@article{Beck:1996zs,
<a name="cl-13771"></a>	Author = {Beck, Rainer and Brandenburg, Axel and Moss, David and Shukurov, Anvar and Sokoloff, Dmitry},
<a name="cl-13772"></a>	Eref = {http://astro.annualreviews.org/cgi/content/abstract/34/1/155},
<a name="cl-13773"></a>	Journal = {Ann. Rev. Astron. Astrophys.},
<a name="cl-13774"></a>	Note = {{NORDITA-95-67-A}},
<a name="cl-13775"></a>	Pages = {155--206},
<a name="cl-13776"></a>	Slaccitation = {%%CITATION = ARAAA,34,155;%%},
<a name="cl-13777"></a>	Title = {Galactic magnetism: Recent developments and perspectives},
<a name="cl-13778"></a>	Volume = {34},
<a name="cl-13779"></a>	Year = {1996},
<a name="cl-13780"></a>	Abstract = {We discuss current observational and theoretical
<a name="cl-13781"></a>knowledge of magnetic fields, especially the
<a name="cl-13782"></a>large-scale structure in the disks and halos of spiral
<a name="cl-13783"></a>galaxies. Among other topics, we consider the
<a name="cl-13784"></a>enhancement of global magnetic fields in the interarm
<a name="cl-13785"></a>regions, magnetic spiral arms, and representations as
<a name="cl-13786"></a>superpositions of azimuthal modes, emphasizing a number
<a name="cl-13787"></a>of unresolved questions. It is argued that a turbulent
<a name="cl-13788"></a>hydromagnetic dynamo of some kind and an inverse
<a name="cl-13789"></a>cascade of magnetic energy gives the most plausible
<a name="cl-13790"></a>explanation for the regular galactic magnetic fields.
<a name="cl-13791"></a>Primordial theory is found to be unsatisfactory, and
<a name="cl-13792"></a>fields of cosmological origin may not even be able to
<a name="cl-13793"></a>provide a seed field for a dynamo. Although dynamo
<a name="cl-13794"></a>theory has its own problems, the general form of the
<a name="cl-13795"></a>dynamo equations appears quite robust. Finally,
<a name="cl-13796"></a>detailed models of magnetic field generation in
<a name="cl-13797"></a>galaxies, allowing for factors such as spiral
<a name="cl-13798"></a>structure, starbursts, galactic winds, and fountains,
<a name="cl-13799"></a>are discussed and confronted with observations.}}
<a name="cl-13800"></a>
<a name="cl-13801"></a>@article{Bedaque:1999nu,
