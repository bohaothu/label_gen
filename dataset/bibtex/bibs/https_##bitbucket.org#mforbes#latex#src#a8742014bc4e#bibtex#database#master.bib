<a name="cl-6284"></a>	Eprint = {0909.2684},
<a name="cl-6285"></a>	Journal = npb,
<a name="cl-6286"></a>	Keywords = {dark energy; cosmological constant; QCD},
<a name="cl-6287"></a>	Month = apr,
<a name="cl-6288"></a>	Number = {1-2},
