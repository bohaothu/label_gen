@Article{Jensen:1978:HED,
  author =       "E. Douglas Jensen",
  title =        "{Honeywell} experimental distributed processor --- an
                 overview",
  journal =      j-COMPUTER,
  volume =       "11",
  number =       "1",
  pages =        "28--38",
  month =        jan,
