@Article{Tax:00,
  author =       { D. M.J. Tax and M. van Breukelen and R.
                  P.W. Duin and J. Kittler},
  title =        {Combining multiple classifiers by averaging or by multiplying?},
  journal =      pr,
  year =         {2000},
  volume =       {33},
  number =       {9},
  pages =        {1475-1485},
  month =        {},
  note =         {},
  annote =       {In classification tasks it may be wise to combine
                  observations from different sources. Not only it
                  decreases the training time but it can also increase
                  the robustness and the performance of the
                  classification. Combining is often done by just
                  (weighted) averaging of the outputs of the different
                  classifiers. Using equal weights for all classifiers
                  then results in the mean combination rule. This
                  works very well in practice, but the combination
                  strategy lacks a fundamental basis as it cannot
                  readily be derived from the joint
                  probabilities. This contrasts with the product
                  combination rule which can be obtained from the
                  joint probability under the assumption of
                  independency. In this paper we will show differences
                  and similarities between this mean combination rule
                  and the product combination rule in theory and in practice.},
  pdf =          {file:/home/DOC/papers/pr33_kittler00.pdf},
  ident =        {[ Tax:00 ]}
}

