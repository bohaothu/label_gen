@Inproceedings{Ornburn:93,
        author             = {Ornburn, Stephen B. and LeBlanc Jr., Richard J.},
        booktitle          = {International Conference of Software Engineering},
        title              = {Building, Modifying and Using Component
		  Generators},
        year               = {1993},
        comment = {Present a technique for designing components that
		  can be used to construct applications that can be
		  represented by a finite-state machine.  Components
		  are combined by giving a path expression for the
		  composite program, where a path expression is like a
		  regular expression that describes the possible
		  execution paths.  Each component is described by
		  some code and a corresponding path expression.  The
		  path of the composite program can thus be projected
		  onto the path of each component to determine what
		  part of the component is needed, the corresponding
		  code is that merged with that of the all the other
		  components by reversing the projection.}
}
		  
