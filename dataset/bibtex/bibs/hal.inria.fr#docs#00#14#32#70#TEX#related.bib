@InProceedings{Clark-Tennenhouse:90, 
 author = "Clark, D.D. and Tennenhouse, D.L.",
 title = "Architectural Considerations for a New Generation of
   Protocols",
 pages = "200--208",
 descriptors = "{TCP/IP}; OSI; ISO PROTOCOL;
   PERFORMANCE IMPROVEMENT; PROTOCOL PERFORMANCE;",
 crossref = {sigcomm90},
 abstract = "The current generation of protocol architectures, such as
   {TCP/IP} or the ISO suite, seem successful at meeting the demands of
   todays networks. However, a number of new requirements have been
   proposed for the networks of tomorrow, and some innovation in
   protocol structuring may be necessary. In this paper, we review
   some key requirements for tomorrow's networks, and propose some
   architectural principles to structure a new generation of
   protocols. In particular, this paper identifies to new design
   principles, Application Level Framing and Integrated Layer
   Processing. Additionally, it identifies the presentation layer as a
   key aspect of overall protocol performance"
}

@Article{Clark-al:89, 
 author = "Clark, D. and Jacobson, V. and Romkey, J. and Salwen, M.", 
 title = "An analysis of {TCP} processing overhead", 
 publisher = "{IEEE}", 
 journal = "{IEEE} Communications Magazine", 
 volume = "27", 
 number = "6", 
 pages = "23--29", 
 year = "1989", 
 month = jun, 
 annote = "An implementation of TCP/IP is discussed, primarily Berkeley
   UNIX code on a 386 machine (but also references to work on 68020
   based Suns and a RISC workstation). They studied the length of the
   common, normal path through the code on both input and
   output. TCP/IP processing takes at most 300-400 instructions,
   depending upon architecture if implemented efficiently. The real
   bottleneck is in the operating system's implementation of buffers
   and timers. Contrary to popular belief, the most expensive part of
   the TCP protocol is the sender not the reciever.  The generation of
   the checksum, moving the data between kernel buffers and user
   memory and recovering of the TCB are all potentially more expensive
   than the core of the protocol itself. It is recommended that kernel
   buffers be forgotten and that the checksum be computed directly by
   the CPU whilst it is moving the data (no DMA).  The 96bits required
   to locate a TCB entry could be hashed to 8 bits. One can assume
   that each new packet uses the same TCB with a surprisingly high hit
   ratio even on heavily loaded machines. Using a 10MIPS CPU, it is
   predicted that TCP could handle 530Mbps. On a 32bit processor with
   250ns DRAM, the memory bandwidth is only 32Mbps so this can be the
   limiting factor. Protocol implementation in silicon is advised
   against as it prevents easy adjustments to the protocol
   implementation and design. Specialised outboard processors
   utilising the high speed sequential access mode of DRAMs may help,
   but buffering becomes more complex and possilby slower.",
 where = "1st {MultiG} Conference"
}


