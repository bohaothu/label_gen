<a name="cl-2176"></a>  author = {MH Davis and A Khotanzad and DP Flamig and SE Harms},
<a name="cl-2177"></a>  title = {A physics-based coordinate transformation for 3-{D} image matching.},
<a name="cl-2178"></a>  journal = {IEEE Trans Med Imaging},
