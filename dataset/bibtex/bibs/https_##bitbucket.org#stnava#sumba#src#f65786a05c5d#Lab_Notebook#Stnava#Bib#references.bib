<a name="cl-618"></a>  author = {Brian B Avants and Philip A Cook and Lyle Ungar and James C Gee and
<a name="cl-619"></a>	Murray Grossman},
<a name="cl-620"></a>  title = {Dementia induces correlated reductions in white matter integrity
<a name="cl-621"></a>	and cortical thickness: a multivariate neuroimaging study with sparse
<a name="cl-622"></a>	canonical correlation analysis.},
<a name="cl-623"></a>  journal = {Neuroimage},
