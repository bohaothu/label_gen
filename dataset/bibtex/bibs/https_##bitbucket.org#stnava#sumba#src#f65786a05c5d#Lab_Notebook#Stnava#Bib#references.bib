<a name="cl-2810"></a>@ARTICLE{Galant2006,
<a name="cl-2811"></a>  author = {Stanley P Galant and Tricia Morphew and Silvia Amaro and Otto Liao},
<a name="cl-2812"></a>  title = {Current asthma guidelines may not identify young children who have
<a name="cl-2813"></a>	experienced significant morbidity.},
<a name="cl-2814"></a>  journal = {Pediatrics},
<a name="cl-2815"></a>  year = {2006},
<a name="cl-2816"></a>  volume = {117},
