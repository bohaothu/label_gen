@InProceedings{regin-al-JFPLC-2002,
  author =		 {Jean-Charles Régin and Jean-François Puget
                  and Thierry Petit},
  title =		 {Representation of soft constraints by hard
                  constraints},
  booktitle =	 {11èmes Journées Francophones de Programmation
                  en Logique et avec Contraintes (JFPLC'02)},
  year =		 2002,
  address =		 {Nice, France},
  month =		 may,
  publisher =	 {Hermès},
  kind =		 {CDNCLA},
  note =		 {2-7462-0739-7},
  url =			 {http://www.emn.fr/x-info/tpetit/hard1.ps},
  abstract =	 {Most of the current algorithms dedicated to the
                  resolution of over-constrained problems, as
                  PFC-MRDAC, are based on the search for a support for
                  each value of each variable taken independently. The
                  semantics of soft constraints is only used to
                  speed-up such a search, but not to globally deduce
                  the existence or the absence of support. They do not
                  use the filtering algorithms associated with these
                  constraints. In this paper we present a new schema
                  which represents a soft constraints by an hard
                  constraint in order to automatically benefit from
                  its pruning performance and from the incrementality
                  mechanism provided with existing solvers. The
                  PFC-MRDAC (via the Satisfiabilty Sum constraint)
                  algorithm and the search for disjoint conflict sets
                  are then adapted to this new schema.},
}

