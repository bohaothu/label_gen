@Article{tounsi-david-ijait-2002,
  author =		 {Mohamed Tounsi and Philippe David},
  title =		 {{S}uccessive {S}earch {M}ethods for {S}olving
                  {C}onstraint and {O}ptimization {P}roblems},
  journal =		 {International Journal of Artificial Intelligence
                  Tools},
  year =		 2002,
  month =		 Sep,
  volume =		 11,
  number =		 3,
  kind =		 {RAICL},
  note =		 {ISSN 0218-2130},
  abstract =	 {In this paper we introduce a new method based on
                  Russian Doll Search ({DTH}) for solving optimization
                  problems expressed as Valued Constraint Satisfaction
                  Problems (VCSPs). The {DTH} method solves problems
                  of size n (where n is the number of variables) by
                  replacing one search by n successive searches on
                  nested subproblems using the results of each search
                  to produce a better lower bound. The main idea of
                  our method is to introduce the variables through the
                  successive searches not one by one but by sets of k
                  variables. We present two variants of our method:
                  the first one where the number k is fixed, noted
                  k{f}{DTH}; the second one, k{v}{DTH}, where k can be
                  variable. Finally, we show that our method improves
                  {DTH} on daily management of an earth observation
                  satellite.},
}

