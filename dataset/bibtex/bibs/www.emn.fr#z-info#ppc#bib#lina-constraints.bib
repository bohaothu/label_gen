@InProceedings{gueret-al-PMS-1998,
  author =		 {Christelle Guéret and Narendra Jussien and
                  Christian Prins},
  title =		 {Using intelligent backtracking to improve branch and
                  bound methods: an application to Open-Shop problems},
  booktitle =	 {6th International Workshop on Project Management and
                  Scheduling (PMS'98)},
  year =		 1998,
  address =		 {Istanbul, Turkey},
  month =		 jul,
  url =
                  {http://www.emn.fr/jussien/publications/gueret-PMS98.pdf},
  kind =		 {CDICLA},
  abstract =	 {Only two branch-and-bound methods have been
                  published so far for the Open-Shop problem. The best
                  one has been proposed by Brucker {\em et al.} But
                  some square problems from size 7 are still unsolved
                  by it.  We present an improving technique for
                  branch-and-bound methods applied to Brucker {\em et
                  al.} algorithm for Open-Shop problems. Our technique
                  is based on intelligent backtracking.  Intelligent
                  backtracking techniques involve the computation of
                  explanations when encountering a contradiction in
                  the search.  We present here an adaptation of a
                  generic explanation system we have initially
                  developed in the constraint programming scheme.  We
                  tested our approach on Open-Shop problems from the
                  literature (benchmarks of Taillard).  The search is
                  definitely improved : on some square problems of
                  size 10, the number of explored nodes is reduced by
                  more than 90\% and we solved an open problem of size
                  10.  We applied our technique on Open-Shop problems,
                  but it can easily be adapted to solve any other shop
                  problems with a Branch-and-Bound in which the
                  branching scheme consists in fixing disjunctions.},
hal = {hal-00362195},
}

@InProceedings{jussien-lhomme-SCAN-1998,
  author =		 {Narendra Jussien and Olivier Lhomme},
  title =		 {About avoidable computations in Interval methods},
  booktitle =	 {IMACS/GAMM international symposium on
                  Scientific Computing, Computer Arithmetic and
                  Validated Numerics (SCAN'98)},
  pages =		 66,
  year =		 1998,
  address =		 {Budapest, Hungary},
  month =		 sep,
  url =
                  {http://www.emn.fr/jussien/publications/jussien-SCAN98.pdf},
  kind =		 {CDICLA},
  abstract =	 {Interval methods for solving systems of nonlinear
                  equations typically split an interval vector or a
                  box each time the Newton operator $N(x, X)$ does not
                  lead to a sufficient narrowing. The drawback of such
                  a splitting procedure is that, when applied
                  recursively, it may generate many sub-boxes, and for
                  each sub-box, it is necessary to apply the Newton
                  operator from scratch: the Jacobian and its inverse
                  have to be computed, what is computationally
                  expensive. So, an exponential number of expensive
                  computations must be done. The behavior of the
                  overall Interval Newton method is generally much
                  better, because the Newton operator $N(x, X)$
                  generally performs well, so the number of splitting
                  is low. But in some very hard cases (see for example
                  the problem in [3]) a great number of sub-boxes are
                  really generated.  Two questions come to mind: How
                  can the number of generated sub-boxes be decreased?
                  and How can the computations over a sub-box be
                  reused for another sub-box?  We will address in this
                  paper both questions. The idea is to maintain a
                  dependency graph that allows to keep reusable
                  information when leaving a sub-box $I_1$ for another
                  one $I_2$.  When the information which is kept is
                  that the sub-box $I_2$ cannot contain any zero it is
                  easy to see that this will avoid $I_2$ to be tried,
                  so possible sub-boxes of $I_2$ are not generated.
                  This work is inspired by works done in Artificial
                  Intelligence: especially that of Ginsberg [1] and
                  Jussien and Lhomme [2].  [1] Matthew L. Ginsberg,
                  Dynamic Backtracking, Journal of Artificial
                  Intelligence Research volume 1, pages 25-46,1993.
                  [2] N. Jussien, O. Lhomme, Dynamic Domain Splitting,
                  Proceedings of ECAI'98. Forthcoming, 1998.  [3]
                  Van-Hentenryck, P., Puget, J-F., A Constraint
                  Satisfaction Approach to a Circuit Design
                  Problem. Journal of Global Optimization
                  (Forthcoming).},
}

@PhdThesis{debruyne-THESE-1998,
  author =		 {Romuald Debruyne},
  title =		 {Etude des consistances locales pour les
                  problèmes de satisfaction de contraintes de
                  grande taille},
  school =		 {LIRMM-Université de Montpellier II},
  month =		 dec,
  year =		 1998,
  kind =		 {DTH},
  url =			 {http://www.emn.fr/x-info/rdebruyn/these.zip},
  abstract =	 {Our aim is to know if on hard constraint networks
                  (CN), maintaining a pruningful local consistency can
                  outperform MAC and FC-CBJ, the most efficient search
                  algorithms. We study the local consistencies that
                  can beused on large CNs to know which is the most
                  promising. We make some recalls on the local
                  consistencies and we propose new ones. We propose
                  new algorithms to enforce the new local
                  consistencies too. A comparison shows which are the
                  local consistencies that could lead to better cpu
                  time performances for the search of a solution on
                  hard problems if t hey were maintained during
                  search. To prove the results of this comparison, we
                  propose a new search algorithm that maintains a
                  local consistency close to the most promising local
                  consistency studied. A comparison with the most
                  efficient search algorithms shows that this new
                  algorithm is the most efficient on hard problems
                  that have only few 3-cliques in their constraint
                  graph.  Notre objectif est de déterminer si sur
                  les réseaux de contraintes difficiles, le
                  maintien d'une consistance locale forte durant la
                  recherche peut {\^e}tre plus efficace que MAC et
                  FC-CBJ, les meilleurs algorithmes de recherche. Nous
                  étudions les consistances locales qui peuvent
                  {\^e}tre utilisées sur les grands réseaux de
                  contraintes afin de conna{î}tre celle qui semble la
                  plus appropriée. Nous rappelons quelles sont les
                  différentes consistances locales existantes et
                  nous en proposons de nouvelles.  Nous proposons de
                  nouveaux algorithmes pour réaliser la
                  consistance de chemin restreinte et la consistance
                  de chemin inverse. Nous proposons également des
                  algorithmes réalisant les nouvelles consistances
                  locales proposées. Une étude montre quelles
                  sont les consistances locales qui maintenues durant
                  la recherche d'une solution devraient permettre une
                  résolution plus rapide des probl{\è e}mes
                  difficiles. Afin de confirmer les résultats de
                  cette comparaison, nous proposons un nouvel
                  algorithme de recherche qui maintient une
                  consistance locale proche de la plus prometteuse des
                  consistances locales étudiées. Une étude
                  comparative avec les plus perfomants algorithmes de
                  recherche montre qu'il est le plus efficace sur les
                  problèmes difficiles qui ne comportent que peu
                  de 3-cliques dans leur graphe des contraintes.},
}

@InProceedings{jussien-gueret-JFPLC-1998,
  author =		 {Narendra Jussien and Christelle Guéret},
  title =		 {Procédures de Séparation-Évaluation -- Une
                  alternative au retour-arrière -- Application au
