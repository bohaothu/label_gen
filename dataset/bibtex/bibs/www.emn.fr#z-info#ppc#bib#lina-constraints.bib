@InProceedings{jussien-lhomme-JNPC-2002,
  author =		 {Narendra Jussien and Olivier Lhomme},
  title =		 {Vers une unification des algorithmes de
                  résolution de {CSP}},
  booktitle =	 {8èmes Journées nationales sur la
                  résolution pratique de problèmes NP-complets
                  (JNPC'02)},
  pages =		 {155-168},
