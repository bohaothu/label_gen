}

@TechReport{debruyne-bessiere-rr-LIRMM-98-037,
  author =		 {Romuald Debruyne and Christian Bessière},
  title =		 {Which local consistency has to used on large
                  constraint networks ?},
  institution =	 {LIRMM-Université de Montpellier II},
  address =		 {Montpellier, France},
  number =		 {98-037},
  year =		 1998,
  kind =		 {RI},
  type =		 {Technical Report},
  url =			 {http://www.emn.fr/x-info/rdebruyn/tr98037.pdf},
  abstract =	 {An algorithm looking for a solution on a constraint
                  network must use the suitable local consistency to
                  efficiently reduce the search space.  In the
                  eighties, some experimental evaluations of search
                  algorithms convinced the constraint reasoning
                  community that the limited local consistency used by
                  forward checking is the best choice and that pruning
                  more values is too expensive to be
                  cost-effective. However, these experiments were made
                  on small and easy constraint networks. More recent
                  works show that maintaining arc consistency during
                  search (MAC) outperforms forward checking on hard
                  and large constraint networks. It is conceivable
                  that on very difficult instances, maintaining an
                  even more pruningful local consistency may pay
                  off. In this paper we study the local consistencies
                  that can be used on large constraint networks,
                  namely those that do not change the constraints,
                  i.e. that only delete values in the domains.  In the
                  last three years, several new such local
                  consistencies have been proposed.  Maintaining one
                  of them during search would outperform MAC on large
                  constraint networks only if this local consistency
                  is much more pruningful than arc consistency while
                  requiring few additional time to be achieved. We
                  make an overview of the local consistencies that can
                  be used on large CNs and we propose new such local
                  consistencies. We highlight some relations between
                  these local consistencies and we compare them,
                  considering both their pruning efficiency and the
                  time required to enforce them.},
}

@TechReport{debruyne-rr-LIRMM-98-041,
  author =		 {Romuald Debruyne},
  title =		 {Removing more values than max-restricted path
                  consistency for the same cost},
  institution =	 {LIRMM-Université de Montpellier II},
  address =		 {Montpellier, France},
  number =		 {98-041},
  year =		 1998,
  kind =		 {RI},
  type =		 {Technical Report},
  url =			 {http://www.emn.fr/x-info/rdebruyn/tr98041.pdf},
  abstract =	 {Filtering techniques are essential to efficiently
                  look for a solution in a constraint network
                  (CN). They remove some local inconsistencies and so
                  reduce the search space. However, a given local
                  consistency has to be not too expensive if we want
                  to use it to efficiently prune the search tree
                  during search. Hence, for a long time it has been
                  considered that the best choice is the limited local
                  consistency achieved by forward checking
                  [GB65,HE80].  However, more recent works
                  [SF94,BR96,GS96] show that maintaining arc
                  consistency (which is a more pruningful local
                  consistency) during search outperforms forward
                  checking on hard and large constraint networks. It
                  is very likely that maintaining an even more
                  pruningful local consistency may pay off on very
                  hard problems.  A comparison of the local
                  consistencies more pruningful than arc consistency
                  that can be used on large CNs has been done in
                  [DebBes98]. The conclusion of this work is that
                  Max-restricted path consistency (Max-RPC,
                  [DebBes97]) is one of the most promising local
                  consistencies. It deletes far more values than arc
                  consistency with reasonable cpu time
                  requirements. For all the local consistencies more
                  pruningful than Max-RPC, there exists CNs on which
                  achieving them is really prohibitive.  In this paper
                  we show that Max-RPC is not the limit we have to do
                  not exceed to guarantee a reasonable cpu time. We
                  propose a new filtering algorithm, called
                  Max-RPCEn1, which prunes more values than an
                  algorithm achieving Max-RPC. It requires less
                  constraint checks than Max-RPC1, the Max-RPC
                  algorithm proposed in [DebBes97], and has almost the
                  same cpu time requirements.},
}

