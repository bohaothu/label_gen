@InProceedings{cambazard-jussien-JFPC-2005,
  author =		 {Hadrien Cambazard and Narendra Jussien},
  title =		 {Des explications pour reconna{î}tre et exploiter les
                  structures cachées},
  booktitle =	 {1ères Journées Francophones de Programmation
                  par Contraintes (JFPC'05)},
  year =		 2005,
  address =		 {Lens, France},
  month =		 jun,
  pages =		 {403-412},
  kind =		 {CDNCLA},
  url =
                  {http://www.emn.fr/jussien/publications/rochart-JFPC05.pdf},
hal = {inria-00000064},
  abstract =	 {L'identification de structures propres à un problème
                  est souvent une étape clef pour la conception
                  d'heuristiques de recherche comme la compréhension
                  de la complexité du problème.  De nombreuses
                  approches en Recherche Opérationnelle emploient des
                  stratégies de relaxations ou décompositions dès lors
                  que certaines structures idoines ont été
                  identifiées. L'étape suivante est la conception
                  d'algorithmes de résolution qui puisse intégrer à la
                  volée, pendant la résolution, ce type
                  d'information. Cet article propose d'utiliser un
                  solveur de contraintes à base d'explications pour
                  collecter de l'information pertinente sur les
                  structures dynamiques et statiques inhérentes au
                  problème. Par ailleurs, la reconnaissance de
                  relations spécifiques entre les variables suggère
                  l'adaptation d'algorithmes dédiés issus du monde de
                  la Recherche Opérationnelle au contexte de la
                  programmation par contraintes. Une telle adaptation
                  est discutée dans le cadre de la décomposition de
                  Benders.},
  keyword =		 {KEX},
}

