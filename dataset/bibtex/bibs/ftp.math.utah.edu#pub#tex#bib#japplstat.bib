@Article{Gill:2007:SAK,
  author =       "Paramjit S. Gill and Tim B. Swartz and Michael
                 Treschow",
  title =        "A Stylometric Analysis of {King Alfred}'s Literary
                 Works",
  journal =      j-J-APPL-STAT,
  volume =       "34",
  number =       "10",
  pages =        "1251--1258",
  year =         "2007",
  CODEN =        "????",
  DOI =          "http://dx.doi.org/10.1080/02664760701592992",
  ISSN =         "0266-4763 (print), 1360-0532 (electronic)",
  ISSN-L =       "0266-4763",
  bibdate =      "Wed Aug 25 11:41:55 MDT 2010",
  bibsource =    "http://www.tandf.co.uk/journals/routledge/02664763.html",
  abstract =     "For centuries, Alfred the Great was judged to have
                 translated several Latin texts into Old English. Many
                 scholars, however, have expressed doubt whether Alfred
                 could have done all of this work. With the availability
                 of the Old English Corpus in electronic form, it is
                 feasible to subject the texts to statistical
                 stylometric analysis. We approach the problem from a
                 Bayesian perspective where key words are identified and
                 frequencies of the key words are tabulated for seven
                 relevant texts. The question of authorship falls into
                 the general statistical problem of classification where
                 several simple innovations to classical agglomerative
                 procedures are introduced. Our results suggest that one
                 translation that has been traditionally attributed to
                 Alfred (The First Fifty Prose Psalms) tends to
                 distinguish itself from texts that are known to be
                 Alfredian.",
  acknowledgement = ack-nhfb,
  keywords =     "Agglomerative techniques; Bayesian methods;
                 classification; Dirichlet distribution; disputed
                 authorship; entropy; hierarchical clustering;
                 multinomial distribution; Old English",
}

