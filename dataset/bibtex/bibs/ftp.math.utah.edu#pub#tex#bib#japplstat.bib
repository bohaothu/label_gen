@Article{Almasri:2008:TCW,
  author =       "Abdullah Almasri and H{\aa}kan Locking and Ghazi
                 Shukur",
  title =        "Testing for climate warming in {Sweden} during
                 1850--1999, using wavelets analysis",
  journal =      j-J-APPL-STAT,
  volume =       "35",
  number =       "4",
  pages =        "431--443",
  month =        apr,
  year =         "2008",
  CODEN =        "????",
  DOI =          "http://dx.doi.org/10.1080/02664760701835011",
  ISSN =         "0266-4763 (print), 1360-0532 (electronic)",
  ISSN-L =       "0266-4763",
  bibdate =      "Wed Aug 25 11:41:55 MDT 2010",
  bibsource =    "http://www.tandf.co.uk/journals/routledge/02664763.html",
  abstract =     "This paper describes an alternative approach for
                 testing for the existence of trend among time series.
                 The test method has been constructed using wavelet
                 analysis which has the ability of decomposing a time
                 series into low frequencies (trend) and high-frequency
                 (noise) components. Under the normality assumption, the
                 test is distributed as $F$. However, using generated
                 empirical critical values, the properties of the test
                 statistic have been investigated under different
                 conditions and different types of wavelet. The Haar
                 wavelet has shown to exhibit the highest power among
                 the other wavelet types.\par

                 The methodology here has been applied to real
                 temperature data in Sweden for the period 1850--1999.
                 The results indicate a significant increasing trend
                 which agrees with the `global warming' hypothesis
                 during the last 100 years.",
  acknowledgement = ack-nhfb,
}

