@Article{Gordon:1978:DGP,
  author =       "Geoffrey Gordon",
  title =        "The development of the {General Purpose Simulation
                 System} ({GPSS})",
  journal =      j-SIGPLAN,
  volume =       "13",
  number =       "8",
  pages =        "183--198",
  month =        aug,
  year =         "1978",
  CODEN =        "SINODQ",
