@Article{Meissner:1975:EFC,
  author =       "Loren P. Meissner",
  title =        "On extending {Fortran} control structures to
                 facilitate structured programming",
  journal =      j-SIGPLAN,
  volume =       "10",
  number =       "9",
  pages =        "19--30",
  month =        sep,
  year =         "1975",
  CODEN =        "SINODQ",
  DOI =          "http://doi.acm.org/10.1145/987316.987320",
  ISSN =         "0362-1340",
  bibdate =      "Wed Jun 18 16:07:17 MDT 2008",
  bibsource =    "http://portal.acm.org/",
  abstract =     "A number of preprocessors and other extensions of
                 Fortran have been implemented recently, aimed at adding
                 'disciplined' control structures to the language. In
                 spite of wide variations in the details of these
                 dialects, many common features can be perceived. This
                 paper attempts to identify these common features, and
                 to draw some conclusions concerning their relation to a
                 possible future widely acceptable structured Fortran
                 language. Several different kinds of alternative and
                 iterative structures are described, along with some of
                 the ways they have been implemented. The use of
                 internal procedures for top-down refinement is noted.
                 Few, of the dialects include a general multiple level
                 exit, although in principle this feature is needed for
                 structured programming. Use of labels seems to be
                 preferable to any alternative way of implementing, this
                 feature in Fortran. Because of the importance of
                 compatibility with existing programs, and for other
                 reasons, it is unlikely that the GO TO statement will
                 disappear from Fortran in the forseeable future.
                 However. with the addition of new control structures it
                 should be possible to develop processors that warn
                 against certain particularly awkward usages of GO TO
                 statements. Further efforts are recommended, including
                 continued support of the ANSI X3J3 Fortran standards
                 committee, and the establishment of a separate group
                 charged with predicting trends and making timely
                 recommendations for future extensions to the
                 language.",
  acknowledgement = ack-nhfb,
  classification = "C6110 (Systems analysis and programming); C6140D
                 (High level languages)",
  corpsource =   "Lawrence Berkeley Lab., Univ. of California, Berkeley,
                 CA, USA",
  keywords =     "block structure; compatibility; control structures;
                 dialects; Fortran; FORTRAN; FORTRAN control structures
                 extension; GO TO statements; iterative structures;
                 language extensions; modular programming; multiple
                 level exit; portability; preprocessors; programming;
                 programming methodology; reducibility; standards;
                 statement labels; structured Fortran; structured
                 FORTRAN language; structured programming; top down
                 refinement",
  treatment =    "A Application; B Bibliography; P Practical",
}

