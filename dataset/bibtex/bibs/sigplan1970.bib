@Article{Lassagne:1975:RDI,
  author =       "Theodore D. Lassagne",
  title =        "Recursive definitions for iterative algorithms",
  journal =      j-SIGPLAN,
  volume =       "10",
  number =       "12",
  pages =        "26--38",
  month =        dec,
  year =         "1975",
  CODEN =        "SINODQ",
  DOI =          "http://doi.acm.org/10.1145/987686.987690",
  ISSN =         "0362-1340",
  bibdate =      "Wed Jun 18 16:07:45 MDT 2008",
  bibsource =    "http://portal.acm.org/",
  abstract =     "Since higher level languages first appeared, computer
                 practitioners have been seeking a language which will
                 permit them to define an algorithm to a computer
                 without unnecessary specification of the sequence in
                 which actions are to be carried out. Recursive
                 definitions meet this requirement, but a major obstacle
                 to their wider adoption has been the fact that so many
                 of our algorithms are iterative in nature, and it
                 typically has not been easy to convert an iterative
