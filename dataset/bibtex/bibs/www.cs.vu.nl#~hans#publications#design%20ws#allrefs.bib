@ARTICLE{Clarke2008,
  author = {Roger Clarke},
  title = {Web 2.0 as Syndication},
  journal = {Journal of Theoretical and Applied Electronic Commerce Research},
  year = {2008},
  volume = {3},
  pages = {30-43},
  number = {2},
  month = {aug},
  __markedentry = {[patricia]},
  comment = {http://www.jtaer.com/portada.php?agno=2008&numero=2#},
  keywords = {akmbook},
  owner = {patricia},
  pdf = {web 20 as syndication clarke_p3.pdf},
  review = {This paper assesses the available information about Web 2.0 to interpret
	its notion. 
	
	
	Though recognizing that the definition of Web 2.0 remains elusive,
	Clarke~\cite{Clarke2008} identifies four key aspects of Web 2.0 from
	the marketing perspective: syndication of (1) content, (2) advertising,
	(3) storage and (4) effort. There, {\em syndication} refers to the
	loose, unincorporated affiliation of people or companies, and the
	loose distribution of media material.
	
	
	In its essence, the central aspect of Web 2.0 is the transformation
	of the Web into a global platform for information sharing (corresponding
	to content syndication) coupled with the ability of both individuals
	and organizations to contribute new information and modify existing
	one. This led to cascading effects, like open access to any type
	of resources, new business models, and new privacy and ethical concerns.
	
	
	In Clarke's classification, effort syndication is especially relevant
	for AKM communities. He observes that individuals perceive themselves
	as participating to a community, whereas their efforts and behavious
	are part of an economy being used (either officially or unofficially)
	for profit. The communitarial perspective implies a fifth key aspect
	of Web 2.0 termed {\em social syndication}, this embracing "cross-leveraging
	and collaboration of content, effort and infrastucture".},
  timestamp = {13.03.2009}
}

