@INPROCEEDINGS{Klems2009,
  author = {Markus Klems and Jens Nimis and Stefan Tai},
  title = {Do Clouds Compute? A Framework for Estimating the Value of Cloud Computing},
  booktitle = {Seventh Workshop on E-Business (WeB2008)},
  year = {2009},
  series = {Lecture Notes in Business Information Processing (LNBIP)},
  publisher = {Springer Verlag, to appear},
  __markedentry = {[patricia]},
  abstract = {On-demand provisioning of scalable and reliable compute
	
	services, along with a cost model that charges consumers based on
	ac-
	
	tual service usage, has been an objective in distributed computing
	re-
	
	search and industry for a while. Cloud Computing promises to deliver
	
	on this objective: consumers are able to rent infrastructure in the
	Cloud
	
	as needed, deploy applications and store data, and access them via
	Web
	
	protocols on a pay-per-use basis. The acceptance of Cloud Computing,
	
	however, depends on the ability for Cloud Computing providers and
	con-
	
	sumers to implement a model for business value co-creation. Therefore,
	
	a systematic approach to measure costs and bene
ts of Cloud Comput-
	
	ing is needed. In this paper, we discuss the need for valuation of
	Cloud
	
	Computing, identify key components, and structure these components
	
	in a framework. The framework assists decision makers in estimating
	
	Cloud Computing costs and to compare these costs to conventional IT
	
	solutions. We demonstrate by means of representative use cases how
	our
	
	framework can be applied to real world scenarios.},
  comment = {to appear},
  keywords = {akmbook},
  owner = {patricia},
  pdf = {do-clouds-compute.pdf},
  review = {Both Grid Computing and Cloud Computing implement the more general
	model of Utility Computing~\cite{Klems2009}. However, while the first
	evolved in a scientific community as a means to solve computationally
	challenging tasks, the latter focuses on Web-scale applications and
	services, and introduces innovative business models. By building
	service on a Cloud Computing infrastructure, enterprises are suddently
	granted with potentially unlimited resources in terms of both storage,
	execution, services and application logics. This way they can solve
	increasignly pressing issues like scalability, availability, performance,
	flexibility, time to market; and they can realize promise of the
	Internet to offer same oppotunities to e.g. large organizations and
	startups.},
  timestamp = {04.03.2009}
}

