@ARTICLE{yelnik:neuroimage:2007,
   AUTHOR       = {Jérôme Yelnik and Eric Bardinet and Didier Dormont and 
      Grégoire Malandain and Sébastien Ourselin and Dominique Tandé and 
      Carine Karachi and Nicholas Ayache and Philippe Cornu and Yves Agid},
   JOURNAL      = {Neuroimage},
