@ARTICLE{simon:neuroimage:04,
   AUTHOR       = {Olivier Simon and Ferath Kherif and Guillaume Flandin and 
      Jean-Baptiste Poline and Denis Rivière and Jean-François Mangin and 
      Denis LeBihan and Stanislas Dehaene},
   JOURNAL      = {NeuroImage},
