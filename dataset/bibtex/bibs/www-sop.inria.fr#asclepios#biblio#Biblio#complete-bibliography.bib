@ARTICLE{Muthurangu:AJP:05,
   AUTHOR       = {V Muthurangu and D Atkinson and M Sermesant and 
      M E Miquel and S Hegde and R Johnson and R Andriantsimiavona and 
      A M Taylor and E Baker and R Tulloh and D Hill and R S Razavi},
   JOURNAL      = {Am J Physiol Heart Circ Physiol},
