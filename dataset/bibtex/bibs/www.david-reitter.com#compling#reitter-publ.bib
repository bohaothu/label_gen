	Pages = {8 pages},
	Pubtype = {refereed-ws},
	Title = {How teams benefit from communication policies: information flow in human peer-to-peer networks},
	Year = {2011},
	Bdsk-Url-1 = {http://www.david-reitter.com/compling/papers/reitter2011policies.pdf}}

