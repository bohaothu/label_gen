	Year = {2006},
	Bdsk-Url-1 = {http://www.david-reitter.com/compling/papers/reitter2006ccg.pdf}}

@inproceedings{reitter2006computational,
	Abstract = {Syntactic priming effects, modeled as increase in repetition probability shortly after a use of a syntactic rule, have the
  potential to improve language processing components.  We model priming of syntactic rules in annotated corpora of spoken dialogue, extending previous work that was confined to selected constructions. We find that speakers are more receptive to priming from their interlocutor in task-oriented dialogue than in sponaneous conversation. Low-frequency rules are more likely to show priming.},
