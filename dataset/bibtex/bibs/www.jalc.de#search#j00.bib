@article{jalc050201,
  author   = {Devillers, Raymond and Klaudel, Hanna and Pelz, Elisabeth},
  title    = {An Algebraic Box Calculus},
  journal  = jalc,
  year     = 2000,
  volume   = 5,
  number   = 2,
  pages    = {81--107},
  keywords = {Petri nets, algebraic specifications, synchronization, 
    compositional models},
  abstract = {A-nets, a high level class of labeled Petri nets 
    introduced in order to cope with structured data and typing, 
    are provided with an algebraic structure allowing to 
    compositionally construct complex systems from simple 
    building blocks. Standard Petri Box Calculus control flow 
    operations are complemented here by a generalized 
    synchronization mechanism based on parameter identification, 
    of which various forms are examined. Its usefulness is 
    illustrated in the semantics of a parallel program.}
}

@article{jalc050202,
  author   = {\'{E}sik, Zolt\'{a}n and Kuich, Werner},
  title    = {A Kleene Theorem for Lindenmayerian Algebraic Power Series},
  journal  = jalc,
  year     = 2000,
  volume   = 5,
  number   = 2,
  pages    = {109--122},
  keywords = {formal power series, Lindenmayer systems, Kleene Theorem},
  abstract = {We prove a Kleene theorem for Lindenmayerian algebraic 
    power series. The proof of this theorem uses general results 
    on the fixed point theory of continuous functions.}
}

@article{jalc050203,
  author   = {Honkala, Juha},
  title    = {On Formal Power Series Generated by Lindenmayer Systems},
  journal  = jalc,
  year     = 2000,
  volume   = 5,
  number   = 2,
  pages    = {123--144},
  keywords = {L system, formal power series, Lindenmayerian algebraic 
    series},
  abstract = {To study power series generated by Lindenmayer systems we 
    define L algebraic systems and series over arbitrary 
    commutative semirings. We establish closure and fixed point 
    properties of L algebraic series. We show how the framework 
    of L algebraic series can be used to define D0L, 0L, E0L, 
    DT0L, T0L and ET0L power series. We generalize for power 
    series the classical result stating that D0L languages are 
    included in CPDF0L languages.}
}

@article{jalc050204,
  author   = {Mateescu, Alexandru and Salomaa, Kai and Yu, Sheng},
  title    = {On Fairness of Many-Dimensional Trajectories},
  journal  = jalc,
  year     = 2000,
  volume   = 5,
  number   = 2,
  pages    = {145--157},
  keywords = {fairness, trajectories, shuffle},
  abstract = {Many-dimensional trajectories provide a method to define 
    and to investigate the parallel composition of $k$ words or 
    languages, where $k\geq 2$. We introduce natural fairness 
    conditions for the shuffle operation defined by a set of 
    many-dimensional trajectories. The decidability of the 
    fairness property for context-free sets of many-dimensional 
    trajectories is investigated. We show that both, the weak 
    and the strong problem to decide the fairness property for 
    context-free sets of many-dimensional trajectories are 
    decidable. Finally, we prove that the initial fairness 
    problem is also decidable for context-free sets of 
    many-dimensional trajectories.}
}

@article{jalc050205,
  author   = {W\"{a}tjen, Dietmar},
  title    = {Undecidability Results for Uniformly $k$-Limited 0L Systems},
  journal  = jalc,
  year     = 2000,
  volume   = 5,
  number   = 2,
  pages    = {159--167},
  keywords = {formal languages, $k$-limited 0L systems,
    uniformly $k$-limited 0L systems, decision questions},
  abstract = {In this paper we shall prove that it is undecidable 
    whether the intersection of two uniformly $k$-limited 
    0L languages is empty, finite, regular, context-free, a   
    $k$-limited or a uniformly $k$-limited language, whether the 
    language generated by a context-free grammar is a uniformly 
    $k$-limited 0L language or whether a uniformly $k$-limited 
    T0L language can be generated by a context-free grammar.
    Furthermore, it is undecidable whether the union of two 
    uniformly $k$-limited 0L languages is such a language again.}
}

