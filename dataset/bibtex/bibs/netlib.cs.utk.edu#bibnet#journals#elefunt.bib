  journal =      j-BIT-NUM-MATH,
  volume =       "41",
  number =       "3",
  pages =        "540--562",
  month =        jun,
  year =         "2001",
  CODEN =        "BITTEL, NBITAB",
  DOI =          "http://www.springerlink.com/openurl.asp?genre=article&id=doi:10.1023/A:1021971313412",
