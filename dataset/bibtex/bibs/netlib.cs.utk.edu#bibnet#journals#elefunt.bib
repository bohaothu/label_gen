@Article{Berrut:2004:APS,
  author =       "Jean-Paul Berrut and Hans D. Mittelmann",
  title =        "Adaptive Point Shifts in Rational Approximation with
                 Optimized Denominator",
  journal =      j-J-COMP-APPL-MATH,
  volume =       "164--165",
  number =       "??",
  pages =        "81--92",
  day =          "1",
