  title =        "Optimizing hardware function evaluation",
  journal =      j-IEEE-TRANS-COMPUT,
  volume =       "54",
  number =       "12",
  pages =        "1520--1531",
  month =        dec,
  year =         "2005",
  CODEN =        "ITCOB4",
  DOI =          "http://dx.doi.org/10.1109/TC.2005.201",
