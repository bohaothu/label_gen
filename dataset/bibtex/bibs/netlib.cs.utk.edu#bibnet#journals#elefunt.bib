@Article{VanDeun:2004:IAO,
  author =       "J. {Van Deun} and A. Bultheel",
  title =        "An Interpolation Algorithm for Orthogonal Rational
                 Functions",
  journal =      j-J-COMP-APPL-MATH,
  volume =       "164--165",
  number =       "??",
  pages =        "749--762",
