@INPROCEEDINGS{BPVR09,
  author = {Bucchiarone, A. and Pelliccione, P. and Vattani, C. and Runge, O.},
  title = {Self-Repairing Systems Modeling and Verification using AGG},
  booktitle = {Joint Working IEEE/IFIP Conference on Software Architecture 2009
	\& European Conference on Software Architecture (WICSA'09)},
  year = {2009},
  abstract = {Self-Repairing (or healing) systems are systems equipped with a mechanism
	that monitors the system behaviour to determine whether it behaves
	within prefixed parameters. If a deviation exists, then the system
	itself is in charge of adapting its configuration. In this paper
	we show how to model self-repairing systems by means of Dynamic Software
	Architectures (DSAs). DSAs are formalized as Typed (hyper) Graph
	Grammars (TGGs) and this formalization enables verification of correctness
	and completeness of self-repairing systems. DSAs are modeled and
	verified by using the Attributed Graph Grammar system (AGG). The
	overall approach is applied to a traffic light system case study.},
  owner = {Claudia},
  timestamp = {2009.08.31},
  url = {http://tfs.cs.tu-berlin.de/publikationen/Papers09/BPVR09.pdf}
}

