@ARTICLE{EP08,
  author = {Ehrig, H. and Prange, U.},
  title = {{Flattening Generalized Algebraic High-Level Nets}},
  journal = BEATCS,
  year = {2008},
  volume = {94},
  pages = {151--160},
  abstract = {The flattening is a well-known construction from high-level to low-level
	nets. Moreover, this construction is known to be a functor from the
	category AHLNets(SP;A) of algebraic high-level nets with fixed specification
	and algebra part to the category PTNets of place/transition nets.
	In this paper, we extend the flattening construction to generalized
	algebraic high-level nets, where the specification, the algebra part
	and the net structure may be changed by morphisms. The main result
	shows under which condition this generalized flattening construction
	preserves pushouts.},
