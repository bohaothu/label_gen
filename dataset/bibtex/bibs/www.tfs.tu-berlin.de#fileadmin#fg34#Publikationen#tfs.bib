@INPROCEEDINGS{TELW10,
  author = {Taentzer, G. and Ermel, C. and Langer, P. and Wimmer, M.},
  title = {Conflict Detection for Model Versioning Based on Graph Modifications},
  booktitle = {Proc. of Int. Conf. on Graph Transformations (ICGT'10)},
  year = {2010},
  volume = {6372},
  series = {LNCS},
  pages = {171--186},
  publisher = {Springer},
  abstract = {In model-driven engineering, models are primary artifacts and can
	evolve heavily during their life cycle. Therefore, versioning of
	models is a key technique which has to be offered by an integrated
	development environment for model-driven engineering. In contrast
	to text-based versioning systems we present an approach which takes
	abstract syntax structures in model states and operational features
	into account. Considering the abstract syntax of models as graphs,
	we define model revisions as graph modifications which are not necessarily
	rule-based. Building up on the DPO approach to graph transformations,
	we define two different kinds of conflict detection: (1) the check
	for operation-based conflicts, and (2) the check for state-based
	conflicts on results of merged graph modifications.},
  isbn = {ISSN 0302-9743},
  url = {http://tfs.cs.tu-berlin.de/publikationen/Papers10/TELW10.pdf}
}

