@INCOLLECTION{PK05,
  author = {Padberg, J. and Kreowski, H.-J.},
  title = {Loose Semantics of {P}etri Nets},
  year = {2005},
  pages = {370-384},
  abstract = {In this paper, we propose a new loose semantics for place/transition
	nets based on transition systems and generalizing the reachability
