}

@INPROCEEDINGS{BESW10,
  author = {Biermann, E. and Ermel, C. and Schmidt, J. and Warning, A.},
  title = {Visual Modeling of Controlled EMF Model Transformation using {Henshin}},
  booktitle = {Proc. 4th Intern. Workshop on Graph-Based Tools (GraBaTs'10)},
  year = {2010},
  pages = {1--14},
  abstract = {The tool HENSHIN is an Eclipse plug-in supporting visual modeling
	and execution of rule-based EMF model transformations. This paper
	describes the recent extensions of HENSHIN by control structures
	for controlled rule applications. The control structures comprise
	well-known imperative structures like sequences and conditions on
	rule applications. Moreover, application conditions for individual
	rules may now be arbitrarily nested and combined by logical connectors.
	We present the extension of the visual EMF model transformation environment
	Henshin to edit and perform controlled EMF model transformations
	along an example modeling a reactive Web service-based application
	(personal mobility manager).},
  isbn = {ISSN 1863-2122},
