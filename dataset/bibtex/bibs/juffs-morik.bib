@article{kapur_d-musser:1987a,
	author={Deepak Kapur and David R. Musser},
	title={Proof by Consistency},
	journal={Artificial Intelligence},
	year={1987},
	volume={31},
	number={2},
	pages={125--157},
	topic={reasoning-about-consistency;},
	contentnote={Abstract: Advances of the past decade in methods and computer programs for
		showing consistency of proof systems based on first-order equations have
		made it feasible, in some settings, to use proof by consistency as an alternative
		to conventional rules of inference. Musser described the method applied
		to proof of properties of inductively defined objects. Refinements of this
		inductionless induction method were discussed by Kapur, Goguen, Huet and
		Hullot, Huet and Oppen, Lankford, Dershowitz, Paul, and more recently by
		Jouannaud and Kounalis as well as by Kapur, Narendran and Zhang. This paper
		gives a very general account of proof by consistency and inductionless
		induction and shows how previous results can be derived simply from the
		general theory. New results include a theorem giving characterizations
		of an unambiguity property that is key to applicability of proof by consistency,
		and a theorem similar to the Birkhoff's Completeness Theorem for equational
		proof systems, but concerning inductive proof. }
}

