@article{kochen:1974a,
	author={Manfred Kochen},
	title={Representations and Algorithms for Cognitive Learning},
	journal={Artificial Intelligence},
	year={1974},
	volume={5},
	number={3},
	pages={199--216},
	topic={cognitive-architectures;learning;cognitive-modeling;},
	contentnote={Abstract: This is a report summarizing our progress towards a theory of
		cognitive learning. It is concerned with an algorithm that recognizes,
		selects and formulates in an internal language problems that arise in an
		external environment. This algorithm revises its representation of the
		environment and uses it to cope with self-selected problems. The algorithm
		depends on the formation of hypotheses and their use to select actions.
		The key ideas of this project are major new additions to a theory of representation
		of knowledge built on an inductive predicate logic. }
}

