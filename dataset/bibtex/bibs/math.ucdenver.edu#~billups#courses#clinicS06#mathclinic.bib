@INPROCEEDINGS{horn.nafpliotis.ea:niched,
  title = {A niched {P}areto genetic algorithm for multiobjective optmization},
  year = {1994},
  author = {Jeffrey Horn and Nicholas Nafpliotis and David Goldberg},
  booktitle = {First IEEE Conference on Evolutionary Computation, IEEE World Congress
	on Computational Intelligence, Volume 1},
  annote = {Many, if not most, optimization problems have multiple objectives.
	 Historically, multiple objectives have been combined ad hoc to
	fomr a scalar objective function, usually through a linear combination
	(weighted sum) of the multple attributes, or by turning objectives
	into constraints.  The genetic algorithm (GA), however, is readily
	modified to deal with multiple objectives by incorporating the concept
	of Pareto domination in its selection operator, and applying niching
	pressure to spread it population out along the Pareto optimal tradeoff
	surface.  This paper introduces the Niched Pareto GA as an algorithm
	for finding the Pareto optimal set.},
  owner = {Owner},
  timestamp = {2006.02.02},
}

@TECHREPORT{deb.agrawal.ea:fast,
  author = {Kalyanmoy Deb and Samir Agrawal and Amrit Pratap and T. Meyarivan},
  title = {A {F}ast {E}litist {N}on-{D}ominated {S}orting {G}enetic {A}lgorithm
	for {M}ulti-{O}bjective {O}ptimization},
  institution = {Kanpur Genetic Algorithms Laboratory},
  year = {2000},
  annote = {This paper proposes an approach to multi-objective optimization that
	uses genetic algorithms, non-domination sorting and elitism. The
	idea of non-domination sorting and elitism is that every offpring
	in the GA is compared to every parent and all other offspring to
	determine its fitness. If it is significantly less fit than its
	parent or other chromosomes, it is discarded and a new offspring
	is produced. This ensures that the next generation of offpsring
	will have parents more fit than the previous generation. This paper
	presents these ideas and applies the algorithm to certain problems,
	which demonstrates its relatively fast computation time $(O(mn^2))$,
	and success at finding optimality with fewer generations.},
  owner = {Owner},
  timestamp = {2006.02.02},
}

