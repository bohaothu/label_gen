@InProceedings{zachariasen.dam:tabu,
  author    = {Marin Zachariasen and Martin Dam},
  title     = {Tabu Search on the Geometric Traveling Salesperson Problem},
  booktitle = {Meta-heuristics: Theory and Applications},
  year      = {1996},
  editor =       {I.H. Osman and J.P. Kelly},
  publisher =    {Kluwar Academic Publishers},
  address =      {Boston},

    annote = {Several approximation algorithms and heuristics exist for obtaining near-optimal solutions to the TSP.  Most of these use a local search, transitioning from one solution to a neighboring solution at every step.  These simple transitions greatly limit the obtainable performance due to the small neighborhood of each solution, since there are often many solutions that cannot be improved by a single transition.  Zachariasen and Dam evaluate how a tabu search can benefit from using complex, stronger TSP transitions.  They show that it is advantageous to use strong transitions in tabu search without increasing running  time.
  }
}

@InProceedings{bansal.blum.ea:approximation,
  author    = {Nikhil Bansal and Avrim Blum and Shuchi Chawla and Adam Meyerson},
  title     = {Approximation Algorithms for Deadline-TSP and Vehicle Routing with Time Windows},
  booktitle = {Proceedings from the ACM Symposium on Theory of Computing, Chicago, June 2004},
  year      = {2004},
  address =      {Chicago},

    annote = {Given a graph G on n vertices, with a start vertex r and a deadline D(v) for each vertex v, the Deadline-TSP problem is to find a path starting at r that visits as many vertices as possible within their deadline.   Bansal et al. give an O(log n) approximation algorithm for the general case of the problem with arbitrary deadlines, which is an improvement over existing algorithms.  The authors also give an improved 3-approximation to the point-to-point orienteering problem and investigate the existence of a constant- factor approximation for the Deadline-TSP (to no avail).
  }
}

