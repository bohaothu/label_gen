@article{Randell92e,
   author = {Randell, D A and Cohn, A G},
   journal = {Computers and Mathematics with Applications},
   note = {Also appears in ``Semantic Networks'', ed. F. Lehmann,
		Pergamon Press, Oxford, pp. 459-476, 1992.},
   number = {6--9},
   pages = {459--476},
   title = {Exploiting Lattices in a Theory of Space and Time},
   volume = {23},
   year = {1992},
   abstract = {	  We outline an axiomatic  theory expressed in a first
		  order  predicate  logic  formalism.    The    theory
		  modifies and  extends  Bowman Clarke's  calculus  of
		  individuals.  The theory is primarily concerned with
		  a topological description  of space  and time.  From
		  this theory various classification hierarchies   are
		  singled  out and   expressed as lattice  structures.
		  The nodes  of these lattices correspond  to concepts
		  expressed in the theory as  monadic predicates.   We
		  concentrate upon that  part  of the theory  that  is
		  used  to  describe space. The logical  structure and
		  interrelationship of these    different      lattice
		  structures are discussed. Some attention is given to
		  how, by singling out such structures, the theory may
		  be effectively implemented within a resolution-based
		  automated reasoning setting.  } }

