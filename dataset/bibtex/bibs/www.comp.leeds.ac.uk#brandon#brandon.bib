@article{Sacerdoti74a,
   author = {Sacerdoti, E D},
   journal = {Artificial Intelligence},
   pages = {115-135},
   title = {Planning in a hierarchy of abstraction spaces},
   volume = {5},
   year = {1974},
   abstract = {	  A problem domain  can be represented as  a hierarchy
		  of  abstraction  spaces  in which  succesively finer
		  levels of detail are kintroduced. The problem solver
		  ABSTRIPS,  a modification  of STRIPS, can  define an
		  abstraction  space hierarchy     from   the   STRIPS
		  representation  of  a  problem  domain, and  it  can
		  utilize the  hierarchy in solving problems. Examples
		  of  the   systems  performance   are presented  that
		  demonstrate    the      significant    increases  in
		  problem-solving   power   that  this        approach
		  provides.  then   some  further implications of  the
		  hierarchical planning approach are explored.}, 
   owner = {john} 
}

@inproceedings{Sacerdoti75a,
   author = {Sacerdoti, E D},
   booktitle = {Proceedings IJCAI-75},
   pages = {206-214},
   title = {The nonlinear nature of plans},
   year = {1975},
   abstract = {
                                                                         
		  We usually think  of plans  as linear  sequences  of
		  actions. This is because  plans are usually executed
		  one  step  at a time.  But  plans themselves are not
		  constrained by limitations  of linearity. This paper
		  describes a  new  information  structure, called the
		  procedyral net, that represents a  plan as a partial
		  ordering  of  actions    with respect  to  time.  By
		  avoiding premature commitments to a particular order
		  for  achieving subgoals,  a  problem solving  system
		  using   this representation   can  deal easily   and
		  directly  with   problems that   are  otherwise very
		  difficult to solve.}, owner = {john} }

