@article{Kimbrough91a,
   author = {Kimbrough, S O and Hua, H},
   journal = {Minds and Machines},
   note = {Special edition on defeasible reasoning.},
   pages = {393-419},
   title = {On nonmonotonic reasoning with the method of sweeping
	presumptions},
   volume = {1},
   year = {1991},
   owner = {john},
   abstract = {
		  Reasoning  almost always   occurs  in the  face   of
		  incomplete      information.  Such  reasoning     is
		  nonmonotonic in the sense that conclusions drawn may
		  later  be withdrawn  when  additional information is
		  obtained.  There  is  an active   literature on  the
		  problem of  modelling  such nonmonotonic  reasoning,
		  yet  no category of  method  --- let  alone a single
		  method --- has been   broadly accepted as the  right
		  approach. This paper introduces a new method, called
		  {\em    sweeping   presumptions},  for     modelling
		  nonmonotonic reasoning.  The main goal of  the paper
		  is     to provide  an example-driven,  non-technical
		  introduction  to the method of sweeping presumtions,
		  and  thereby    make  it  plausible   that  sweeping
		  presumptions can be usefully applied to the problems
		  of  nonmonotonic  reasoning.  The paper discusses  a
		  representative sample of  examples tat have appeared
		  in   the literature on  nonmonotonic  reasoning, and
		  discusses them from  the point  of view of  sweeping
		  presumptions.}  }

