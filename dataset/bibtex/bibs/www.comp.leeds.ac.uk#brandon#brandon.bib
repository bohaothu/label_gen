@techreport{Lindstrom91a,
   author = {Lindstr\"{o}m, S},
   institution = {Department of Philosophy and Philosophy of
	 Science, Ume\aa Universitet, 901 87 Ume\aa,  Sweden},
   note = {Draft.},
   title = {A semantic approach to nonmonotonic reasoning:
 	inference operations and choice},
   year = {1991},
   abstract = {   This paper  presents a uniform semantic treatment of
		  nonmonotonic inference  operations  that   allow for
		  inferences  from  infinite  sets of  premises.   The
		  semantics is formulated  in terms  of {\em selection
		  functions}   and is a   generalization of   the {\em
		  preferential entailment} semantics of Shoham (1987),
		  (1988), Kraus,   Lehman,    and Magidor (1990)   and
		  Makinson (1990) (to  appear).  A selection functions
		  picks out  from  a  given  set  of  possible  states
		  (worlds, situations,  models) a subset consisting of
		  those states that   are, in  some sense,   the  most
		  preferred ones. A   proposition $\alpha$  is a  {\em
		  nonmonotonic  consequence} of  a set of propositions
		  $\Gamma$     iff   $\alpha$     holds  in   all  the
		  most-preferred  $\Gamma$-states.  In the  literature
		  on revealed preference theory, there are a number of
		  well-known  theorems concerning the representability
		  of   selection   functions,   satisfying     certain
		  properties,  in   terms  of   underlying  preference
		  relations.  Such  theorems  are  utilized e to  give
		  corresponding   representation        theorems   for
		  nonmonotonic   inference operations.  At  the end of
		  the   paper,    the connection  between nonmonotonic
		  inference  and  belief  revision,  in  the  sense of
		  Alchour\`{o}n,   G\"{a}rdenfors,   and Makinson,  is
		  explored. In this connection, {\em infinitary belief
		  revision operations}, that allow for the revision of
		  a theory    with   a   possibly  infinite   set   of
		  propositions, are introduced    and    characterized
		  axiomatically.  several   semantic    representation
		  theorems are proved  for operations of  this kind.},
		  owner = {john} }

