@inproceedings{Gotts93a,
   author = {Gotts, N M and Bonsall, P W},
   booktitle = {Proceedings of the 26th International Symposium on
		  Automotive Technology and Automation (ISATA)
		  Dedicated Conference on Advanced Transport
		  Telematics/Intelligent Vehicle-Highway Systems},
   pages = {451-458},
   publisher = { },
   title = {Route-choice and Route-following: Driver Response to a
		  Mock-up of an Automatic Route-guidance System},
   year = {1993},
   abstract = {   Eighteen drivers  were asked to    make a series  of
		  journeys in an unfamiliar  area. They were told they
		  were  taking    part in   testing   an  experimental
		  automatic route-guidance  system. On   all    trips,
		  drivers were  given  the opportunity  to plan  their
		  routes using a street-map, and making whatever notes
		  or sketches   they  chose.  On  certain   trips they
		  received advice via  a speech-synthesizer.  This was
		  supposedly from  the automatic guidance  system, but
		  in fact triggered by the experimenter.  As expected,
		  both guidance and  rudimentary  familiarity with the
		  network built  up   during the  study   resulted  in
		  reduced   journey  times,  and brought the  drivers'
		  routes closer  to the  recommendations given via the
		  speech  synthesizer. En-route halts and U-turns were
		  also    reduced. Observation  and drivers'  comments
		  indicated   that route-choice    criteria        are
		  context-dependent,    and     that      errors    in
		  route-following    cause     considerable  delay and
		  frustration to unguided drivers in unfamiliar areas.
		  } }

