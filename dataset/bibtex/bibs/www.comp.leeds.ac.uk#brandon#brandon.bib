@article{Shoham85a,
   author = "Shoham, Y",
   journal = {New Generation Computing},
   pages = {407-477},
   title = {Ten Requirements for a Theory of Change},
   volume = {3},
   year = {1985},
   abstract = {	  In order    to  focus the  discussion   of  temporal
		  reasoning in  Artificial Intelligence, we  propose a
		  list of requirements  that a rigorous theory of time
		  and  change  must meet.  These requirements refer to
		  the desired expressiveness of the formalisn (such as
		  representing continous change)  and to the potential
		  pitfalls   (such    as the  inter-    and intra-fram
		  problems)  that should be  avoided. We ``benchmark''
		  some of  the better  known temporal formalisms  that
		  have been  proposed  in Artificial  Intelligence  by
		  stating  which of the  requirements are  met, in our
		  opinion, by each formalism.  },
   owner = {john} 
}

@article{Shoham85b,
