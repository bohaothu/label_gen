@InProceedings{ BaKoSk02,
	author = "Baier, Georg and K{\"o}hler, Ekkehard and Skutella, Martin",
	title = "{On the $k$-splittable flow problem}",
	booktitle = "{Algorithms - ESA 2002. 10th annual European symposium, Rome, Italy, September 17-21, 2002. Proceedings}",
