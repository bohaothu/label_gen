@article{floyd-ecn,
  author =	 "Floyd, Sally",
  journal =	 ccr,
  month =	 oct,
  number =	 "5",
  pages =	 "10-23",
  title =	 "{TCP} and Explicit Congestion Notification",
  volume =	 "24",
  year =	 "1994",
  annote =	 {\begin{itemize} \firstitem{Promotes uses of ECN only
                  in RED routers. In this case, RED routers can be set
                  to mark the packet's ECN bit rather than dropping
                  it. Router sets bit, when receiver sees packet with
                  bit set, it sets the bit in the next outgoing ACK.}
                  \item{Includes LAN and WAN simulations of ECN
                  performance with RED.} \item{Describes some of the
                  reasoning behind recommendations about a sender's
                  response to ECN. More fleshed out later in
                  [\cite{ecn-rfc}], though.} \item{Section 4 -
                  ``Guidelines for TCP's response to ECN" is very
                  useful and contains a reminder that an indication of
                  congestion is not the same as an actual packet
                  loss.} \item{Section 8.2 - ``ECN with other versions
                  of TCP" foreseeing the interaction of ECN with TCP's
                  that predict congestion based on throughput or
                  delay. Floyd says that these TCP's will not diminish
                  the usefulness of ECN.} \end{itemize}}
}

