@misc{Xu1998b,
 author = {Xu, Feiyu},
 title = {Underspecified Treatment of Verb Phrase Ellipsis},
 year = {1998},
 howpublished = {ESSLLI Student Session},
 abstract = {This paper deals with the underspecified semantic
 representation of ellipsis and its resolution. We
 present an analysis to explain the distribution of
 stict and sloppy readings in the interaction of
 verb phrase ellipsis and anaphora. The analysis is
 integrated into a framwork that enables a unified
 and underspecified treatment of scope, anaphora,
 and VPE.},
