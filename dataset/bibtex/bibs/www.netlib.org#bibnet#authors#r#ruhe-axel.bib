@InCollection{Ruhe:1994:RKAa,
  author =       "Axel Ruhe",
  title =        "Rational {Krylov} algorithms for nonsymmetric
                 eigenvalue problems",
  crossref =     "Golub:1994:RAI",
  pages =        "149--164",
  year =         "1994",
  MRclass =      "65F15",
  MRnumber =     "1332748 (96a:65058)",
  bibdate =      "Tue Aug 24 22:44:41 2010",
  bibsource =    "ftp://ftp.math.utah.edu/pub/bibnet/authors/r/ruhe-axel.bib",
  abstract =     "The rational Krylov algorithm for the nonsymmetric
                 matrix pencil eigenvalue problem is described. It is a
                 generalization of the shifted and inverted Lanczos (or
                 Arnoldi) algorithm, in which several shifts are used in
                 one run. It computes an orthogonal basis and a small
                 Hessenberg pencil. The eigensolution of the Hessenberg
                 pencil, gives Ritz approximations to the solution of
                 the original pencil.\par

                 Rational Krylov is the natural alternative when
                 factorization of the matrix is not much more expensive
                 than solution, as e.g. when a multigrid algorithm is
                 used to solve systems.\par

                 In one variant several iterations with different shifts
                 are started on the same starting vector. Such
                 iterations can be performed in parallel, yielding a $p$
                 degree Krylov vector in one iteration on $p$
                 processors. An analogy to a method of experimentally
                 verifying stability of aircraft structures is
                 shown.\par

                 The algorithm is demonstrated on two applied test
                 problems.",
  acknowledgement = ack-nhfb,
  classmath =    "65F15 (Eigenvalues, eigenvectors (numerical linear
                 algebra)); 65Y05 (Parallel computation (numerical
                 methods))",
  keywords =     "Lanczos algorithm; Arnoldi algorithm; numerical
                 examples; parallel computation; rational Krylov
                 algorithm; nonsymmetric matrix pencil eigenvalue
                 problem; Hessenberg pencil; Ritz approximations;
                 multigrid algorithm",
  ZMnumber =     "0803.65045",
}

