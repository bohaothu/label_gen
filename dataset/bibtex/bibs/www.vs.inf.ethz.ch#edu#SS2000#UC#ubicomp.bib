@Article{ACMCS::Abowd1996,
  title =        "Software Engineering and Programming Language
                 Considerations for Ubiquitous Computing",
  author =       "Gregory D. Abowd",
  journal =      "ACM Computing Surveys",
  pages =        "190",
  month =        dec,
  year =         "1996",
  volume =       "28",
  number =       "4es",
  URL =          "http://www.acm.org/pubs/articles/journals/surveys/1996-28-4es/a190-abowd/a190-abowd.html",
  vskey =        "ubi:pgm",
  vsnote =       "Describes challenges in software engineering
                 ubiquitous computing systems. (1) The interface to an
                 ubiquitous service must be available everywhere. Since
                 interfaces depend on physical characteristics of
                 devices, it is important to adapt the interface
                 accordingly. This is called the 'interface scalability
                 problem'. (2) Ubiquity of information: service
                 components should be loosely coupled, data formats
                 should not prevent us from combining services. (3)
                 Integration of different communication techniques (WWW,
                 E-Mail, telephone, etc.). This paper concentrates very
                 much on the interactivity of ubicomp systems, i.e.
                 considers mainly human-usable systems. Nothing about
                 service integration, middleware, communication issues.
                 [hv]",
}

@Article{ibm_mark99,
  author =       "W. Mark",
  title =        "{Turning pervasive computing into mediated spaces}",
  journal =      "IBM Systems Journal",
  year =         "1999",
