@Article{Brustoloni:2006:LEN,
  author =       "Jos{\'e} Carlos Brustoloni",
  title =        "Laboratory experiments for network security
                 instruction",
  journal =      j-JERIC,
  volume =       "6",
  number =       "4",
  pages =        "5:1--5:??",
  month =        dec,
  year =         "2006",
