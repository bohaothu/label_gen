@InProceedings{Gentry:2008:THL,
  author =       "Craig Gentry and Chris Peikert and Vinod
                 Vaikuntanathan",
  title =        "Trapdoors for hard lattices and new cryptographic
                 constructions",
  crossref =     "ACM:2008:SPA",
  pages =        "197--206",
