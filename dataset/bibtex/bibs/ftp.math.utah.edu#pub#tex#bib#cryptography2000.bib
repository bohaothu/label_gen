@Article{Rafaeli:2003:SKM,
  author =       "Sandro Rafaeli and David Hutchison",
  title =        "A survey of key management for secure group
                 communication",
  journal =      j-COMP-SURV,
  volume =       "35",
  number =       "3",
  pages =        "309--329",
  month =        sep,
