@Article{Smith-Miles:2008:CDP,
  author =       "Kate A. Smith-Miles",
  title =        "Cross-disciplinary perspectives on meta-learning for
                 algorithm selection",
  journal =      j-COMP-SURV,
  volume =       "41",
  number =       "1",
  pages =        "6:1--6:25",
  month =        dec,
  year =         "2008",
  CODEN =        "CMSVAN",
  DOI =          "http://doi.acm.org/10.1145/1456650.1456656",
