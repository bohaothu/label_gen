@Article{Cheon:2008:PST,
  author =       "Jung Hee Cheon and Nicholas Hopper and Yongdae Kim and
                 Ivan Osipkov",
  title =        "Provably {Secure Timed-Release Public Key
                 Encryption}",
  journal =      j-TISSEC,
  volume =       "11",
  number =       "2",
