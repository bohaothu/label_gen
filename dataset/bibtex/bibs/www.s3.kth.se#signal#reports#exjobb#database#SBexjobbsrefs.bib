@MastersThesis{EX-0217,
  author = {M{\aa}rten Sander},
  title = {Bluetooth Personal Area Networking},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2002},
  note = {IR-SB-EX-0217},
  month = jul,
  company = {Ericsson},
  authorpublic = {yes},
  abstract = {Bluetooth is an open specification for short-range wireless communication, mainly intended to be a cable replacement technology. This master thesis shows how to add networking support for Bluetooth, using the Bluetooth Network Encapsulation Protocol (BNEP) and Personal Area Network (PAN) Profile. This added networking feature enables the use of the standard Internet Protocol (IP), an important step that increases the range of possible Bluetooth applications and provides an open interface to non-Bluetooth network resources. The study has also proven it is possible to develop embedded IP applications, running on the Bluetooth multi chip module itself, making Bluetooth a promising technology for providing network support in embedded systems.}
}

@MastersThesis{EX-0218,
  author = {Johan Einarsson},
  title = {Signalanalys och val av störsignal mot syntetisk apertur radar ({SAR})},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2002},
  note = {IR-SB-EX-0218},
  month = jul,
  company = {FOI},
  authorpublic = {yes},
  abstract = {Syntetisk apertur radar (SAR) tillhandahåller högupplösande markbilder oberoende av regn, dimma eller mörker. I militära scenarion används störsändare för att neutralisera SAR som en källa för strategisk information. Denna rapport tar upp ett tillvägagångssätt för att maximera störenergin i en SAR-mottagare med minimal utsänd störeffekt.  En snabb och noggrann signalanalys har utvecklats i syfte att undvika triggning på felaktiga signaler samt tillåta användning av en smalbandig störsignal. Simuleringar har genomförts för att utvärdera användandet av olika frekvensmätningsalgoritmer för signalanalysen. Ytterligare simuleringar och utvärderingar, med utgångspunkt från en SAR:s funktionalitet, har gjorts för att hitta en effektiv störform.  Resultat visar att störning av en SAR blir effektivare om den görs frekvenshoppande samt sker på frekvenser nära mittenfrekvensen i det momentana bandet. En signalanalys som använder en fasestimeringsalgoritm följd av en Goertzel algoritm tillåter användandet av en stegvist mer smalbandig störsignal, vilket medför en maximering av störenergin i en SAR-mottagare. Slutligen konstateras det att det bästa störalternativet i ett inledande stadie av signalanalysen, då ingen information om det momentana frekvensbandet finns, är repeterstörning.        }
}

