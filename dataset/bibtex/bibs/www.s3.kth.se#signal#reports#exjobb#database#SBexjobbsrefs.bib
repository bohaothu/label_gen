@MastersThesis{EX-0408,
  author = {Mathias Riback},
  title = {Downlink Beamforming in Wideband Systems Using OFDM},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2004},
  note = {IR-SB-EX-0408},
  month = mar,
  authorpublic = {yes},
  abstract = {In this work downlink beamforming in wideband systems is treated. Downlink beamforming is possible when the base-station has multiple transmit antennas. The transmitted energy is directed to the preferable user so that the interference to other users is minimized. The frequency selective wideband channel is divided into a series of flat fading narrowband channels using OFDM. By doing this the ISI introduced by the wideband channel is mitigated. Two main systems are treated. One using multi-tap beamformers and one using single-tap beamformers. The single-tap beamformer system uses a single-tap beamformer on every sub-carrier of the OFDM system while the multi-tap beamformer system uses one multi-tap beamformer on the wideband channel after the OFDM scheme is applied. The single-tap system applies the beamformer in the frequency domain and the multi-tap system applies the beamformer in the time domain. In both systems perfect channel knowledge at the base-station is assumed. The single-tap system is proven to perform better than the multi-tap system, both in theory and by simulations. Waterfilling for OFDM systems is treated and some special techniques for multi-user waterfilling is covered and a joint beamforming and waterfilling algorithm is developed. The system performance in both the single- and multi-tap system is increased by the waterfilling.}
}

