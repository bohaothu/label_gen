@MastersThesis{EX-0416,
  author = {Anders Nordell},
  title = {An investigation of the MR parallel imaging technique through sensitivity encoding (SENSE) and simulation of coil arrays designated for pediatric MR imaging},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2004},
  note = {IR-SB-EX-0416},
  month = jun,
  company = {Karolinska Institute},
  authorpublic = {yes},
  abstract = {This thesis describes the basics of Magnetic Resonance Imaging (MRI) and investigates a newer method for detecting the MR signals, called Sensitivity Encoding, SENSE. It also simulates detector hardware specially adapted for pediatric brain imaging. MRI is an imaging tool widely used in clinical diagnosis of disease processes throughout the body and is based on the phenomena magnetic resonance. MRI uses the property that when certain nuclei are placed inside a static magnetic field they start to precess at a resonance frequency and absorb energy in the radio frequency range. When the nuclei return to their original state, they re-emit this energy in a detectable signal. In order to make images from these signals, the object being imaged is spatially encoded to frequency with magnetic field gradients. The signals are sampled directly into the frequency domain and then the Inverse 2D Fourier transform is applied to acquire the spatial image. It takes time to encode these signals and the scan time for a set of images can vary a lot and are normally around 5 to 10 minutes. To reduce the scan time SENSE was proposed in 1997, being able to reduce scan time with up to a factor of 4. SENSE uses the acquiring technique, parallel imaging, which means that the same signal is acquired with more than one detector (receiver coil), from different positions. The signal is acquired simultaneously and stored separately. The SENSE reconstruction technique then makes it possible to gain time by sampling fewer points. Fewer sample points over the same frequency domain will result in an aliasing effect when the inverse Fourier transform is applied and image pixel values will be superimposed. To unfold this overlapped image a least square problem is solved with the information gathered from the multiple detectors. The SENSE technique reduces scan time and improves Signal to Noise Ratio (SNR) in the image. This thesis shows that SENSE is a highly effective way to reduce scan time. It also shows that receiver coils especially designed for pediatric imaging would improve image SNR, because SENSE receiver performance is highly dependent on the coil size, positioning and number of detectors used.}
}

