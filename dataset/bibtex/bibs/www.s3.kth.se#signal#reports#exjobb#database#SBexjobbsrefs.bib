@MastersThesis{EX-0115,
  author = {Fredrik Henriksson},
  title = {Link to System Interface for WCDMA},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2001},
  note = {IR-SB-EX-0115},
  month = oct,
  authorpublic = {yes},
  company = {Ericsson Research, Corporate Unit,},
  abstract = {Third generation mobile communication systems will support higher bit rates compared to second generation digital systems, such as GSM. WCDMA is the main third generation air interface and is optimized to support multiple high speed mobile multimedia services. It is necessary to understand the capabilities and the limitations of mobile communication systems to utilize the systems in an effcient way; this is achieved via simulations. System simulations are used to determine the capacity of cellular systems, while link simulations, model aspects like the performance of modulation, coding and the behavior of the fading channel. System simulators usually operate on slot level, whereas link simulators operate on symbol, chip or sub-chip level. A process that translates the behavior of the link from link level to system level, which then will form the physical layer model used in the WCDMA system simulator, is needed. The ideal solution would be to nd one single process that describes dierent services, different mobile speeds and different channel environments. This would decrease the number of necessary link simulations. The problem today is that link simulations has to be performed for each service, mobile speed and channel environment that are simulated in the system simulator. A physical layer model that separates the demodulator and the decoder is expected to handle different mobile speeds and channel environments. The idea is that characteristics like imperfections in the channel estimation, different mobile speeds and channel environments will be modeled in the demodulator model and different services will be modeled in the decoder model. Therefore, physical layer models of carrier and noise powers after the RAKE receiver are analytically derived on slot level for the uplink. These are used to calculate the signal-to-noise ratio (SNR) after the RAKE receiver, which is used to estimate the decoder performance. Modeling of link performance has been performed for different mobile speeds and different channel environments for a speech service with a data rate of 12.2 kbps. Different channel estimation schemes are investigated and the effect of power control is also investigated. Link simulations show that there is performance deterioration for the decoder at high mobile speeds with real channel estimation, which is not desirable for the physical layer model. Two different solutions to handle this deterioration are suggested. One solution is to have a single table with shift factors for different channels and mobile speeds and the other solution is to have a two-dimensional lookup table using both the mean and the variance. }
}

@MastersThesis{EX-0116,
  author = {Johan Hultell Andersson},
  title = {A Generalised Rake Receiver for a WCDMA TDD System},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2001},
  note = {IR-SB-EX-0116},
  month = oct,
  company = {Nokia},
  authorpublic = {yes},
  abstract = {The most important standard in the terrestrial air interface of the third generation of  mobile  communications  is  Wideband  Code  Division  Multiple  Access (WCDMA)  in  which  the  users  are  code  multiplexed.  Even  though  orthogonal Walsh codes are  utilised multipath propagation implies  that  the  desired  signal  is affected  of  multiuser  interference  which  result  in  a  loss  of  performance.  By suppressing the interference the performance of the system  is improved resulting in that additional users can be added to the system. In this report, interference suppression by utilising a Generalised Rake receiver in both  downlink  and  uplink  transmission  for  a  WCDMA  TDD  system  is investigated. The main  difference between  the conventional and  the Generalised Rake  receiver  is  that  the  conventional  Rake  receiver  models  the  overall interference  as  zero-mean  white  Gaussian  noise  while  the  Generalised  Rake receiver  takes  the  colour  of  the  overall  interference  into  account.  Hence  the Generalised  Rake  receiver  may  utilise  more  fingers  resulting  in  different combining weights for the Generalised Rake receiver. It is found that the Generalised Rake receiver improves the performance compared with  the  conventional  Rake  receiver.  The  size  of  the  improvement  is  mainly dependent on the type of channel, the number of  interfering  users, the spreading factor, the Signal-to-Noise ratio, the direction of transmission and the number of utilised Rake fingers and their placement. }
}

