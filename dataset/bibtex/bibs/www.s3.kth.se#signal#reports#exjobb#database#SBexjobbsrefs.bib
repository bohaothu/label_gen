@MastersThesis{EX-0430,
  author = {Mathias Vesterlund},
  title = {Analys av Hjärtfrekvensvariabilitet i Matlab},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2004},
  note = {IR-SB-EX-0430},
  month = oct,
  company = {D-Tools},
  authorpublic = {yes},
  abstract = {Analysis of the beat-to-beat variations in heart rate (HRV-analysis) has since the early 80's been a hot research area to nuerologist as well as diabetes researchers. Through different provocative reflex tests, the state of the autonomic nervous system can be assesssed. Since the autonomic regulation of the heart is a nonlinear process, people have started to take an interest in certain nonlinear analysis methods from the area of chaos theory.      This thesis will comprise the building of a prototype for the diagnosis of autonomic neuropathy with the aid of three cardiovascular reflextests. These tests try to provoke a response from the heart through different manoeuvres while the heart rate is measured. Through the use of HRV-analysis it is then possible to assess whether the patient is suffering the disease, and if so, how far it has progressed.      The measurement of the heart rate is done with a simple heart rate monitor from the field of sports medicine and the prototype is implemented in Matlab. }
}

