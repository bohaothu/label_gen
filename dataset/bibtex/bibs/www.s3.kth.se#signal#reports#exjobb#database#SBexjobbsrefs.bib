@MastersThesis{EX-0227,
  author = {Johan Bolin},
  title = {Content Activity Based Video Summarization},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2002},
  note = {IR-SB-EX-0227},
  month = dec,
  company = {Ericsson Research MediaLab},
  authorpublic = {yes},
  abstract = {Distribution of video clips, such as news and commercials, are expected to be an important ingredient in the future systems for mobile communication. Key frames are a low bandwidth still picture representation of a video. This thesis presents an add-on algorithm and new implementations in an off-line video summarization system included in the content structuring standard MPEG-7. The original algorithm was developed by Ericsson and EPFL, and is based on feature point image representation and stochastic modeling of the video content. The new implementation saves up to 20% of the processing time in video content of  talking head  character and features several new possibilities to customize the result. The report also discusses a number of issues within the area of video summarization and content structuring within MPEG-7.}
