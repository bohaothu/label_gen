@MastersThesis{EX-0322,
  author = {Niklas Lithammer},
  title = {Speech on HS-DSCH},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2003},
  note = {IR-SB-EX-0322},
  month = dec,
  company = {Ericsson Research},
  authorpublic = {yes},
  abstract = {The third generation mobile communication system, using WCDMA, is being deployed around the world. In the latest specification, Release 5, the support for packet data is significantly improved. This is achieved by introducing the High-Speed Downlink Shared Channel, HS-DSCH. This channel offers increased capacity, reduced delays and high peak rates, made possible thanks to fast adjustments of the transmission parameters. The main principles are channel dependent scheduling, fast link adaptation and fast hybrid ARQ with soft combining. The HS-DSCH delivers particularly good performance for best-effort data services, however some of the high-speed benefits can also be used to provide a speech service, called HS-speech. Instead of using a high bit rate, the channel-dependent scheduling and fast link adaptation are used to provide a quality of service, on a best-effort channel. This work will examine the possibilities with the HS-speech approach and measure how this would affect data traffic throughput, which the HS-DSCH normally is used for. The point is not to host only speech but to investigate the possibility to transmit speech in conjunction with data traffic on a shared downlink channel. Transmitting speech on the HS-DSCH is more power efficient than speech services on dedicated channels. On average the power consumption can nearly be halved, but still delivering the same or even better speech quality. These benefits are under the assumption that the associated DPCHs are excluded for the HS-speech. When the communication system is used both for speech and web traffic, the speech users will consume resources leading to a capacity degradation for the web users. This degradation will be comparable regardless of the channel type used to carry the voice service. }
}

@MastersThesis{EX-0401,
  author = {Julie Yeo},
  title = {Impact of Channel Characteristics on Wireless Multi-user MIMO Systems},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2004},
  note = {IR-SB-EX-0401},
  month = feb,
  authorpublic = {yes},
  abstract = {Multiple user multiple input multiple output (MIMO) wireless systems have attracted interest lately due to their ability to allow for high data rate transmissions without increasing the available bandwidth. However in most studies, the channels between the different transmitters and receivers are assumed to be independent realizations of the same random distribution, which is not a true reflection of reality. This thesis studies the impact of channel characteristics on wireless multi-user MIMO systems. Orthogonal frequency division multiplexing (OFDM), which offers the possibility of combating multi-path fading but with lower complexity compared to conventional equalization techniques, is used here in conjunction with SINR- based beamforming for joint allocation in space and frequency. The channel is modelled according to a Kronecker structure that has been proposed in literature. Results from simulations of different channel characteristics obtained via changing the correlation properties are presented here. It is found that correlation on the transmitter side have as large effects as correlation on the receiver side. Also, the larger the correlation, the higher the outage probability. Results suggest that at  high correlation values, the outage probability can be as high as a system without spatial multiplexing. In addition results from simulations performed on a set of measured channels, with the aim of seeing how well the theoretical values coincided with reality, are also presented. The outage probability in this case corresponds to that of theoretical values with medium to high correlation. However, it should be mentioned that the results obtained based on the measured data are somewhat pessimistic due to the limitations posed by the small amount of measured data available.}
}

@MastersThesis{EX-0402,
  author = {Svante Bergman},
  title = {Performance Analysis and Loading Algorithm for Spatial Multiplexing using Partial Channel State Information},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2004},
  note = {IR-SB-EX-0402},
  month = feb,
  authorpublic = {yes},
  abstract = {Using information about the channel at the transmitter can improve the performance of Multi-input Multi-output (MIMO) communication systems. When perfect Channel State Information (CSI) is available at the transmitter, data can be multiplexed and optimized over independent spatial channels. However, when the CSI is partial or imperfect, crosstalk between the spatial channels is inevitable which complicates the optimization of the data transmission. In this work, an approximate formula for the bit error rate (BER) of the spatial channels is derived for two types of CSI; channel mean CSI or spatial correlation CSI. The formula is applied to a bit and power loading algorithm that approximately maximizes the uncoded bit rate given a BER constraint and total power constraint. Numerical experiments demonstrate the accuracy of the BER formula and the performance of the loading algorithm.}
}

@MastersThesis{EX-0403,
  author = {{\AA}sa Carlstr{\"o}m},
  title = {Code Tracking Algorithms for WCDMA in Third Generation Mobile Networks},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2004},
  note = {IR-SB-EX-0403},
  month = feb,
  authorpublic = {yes},
  abstract = {In Europe and in Asia UMTS, Universal Mobile Telecommunication Systems, has been chosen as the third generation mobile network standard. To be able to maintain communication between a user and the system a concurrent synchronisation is necessary, since the system is asynchronous and does not have a global time reference. In this study two different synchronisation algorithms are compared regarding robustness to user movement, noise and interference. One algorithm calculates an average estimate of the signal delay during a specific time span for the synchronisation. The other algorithm is more complex and synchronises continuously.   The algorithms are realised in Matlab and simulations are done to analyse how the algorithms react to white noise, interference and a quickly changing environment. The more complex algorithm, the algorithm that synchronises continuously, performs better with regards to a changing environment but both algorithms performances degrade when interference is added to high noise levels. }
}

