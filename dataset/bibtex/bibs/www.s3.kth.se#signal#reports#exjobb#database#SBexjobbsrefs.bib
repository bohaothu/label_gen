@MastersThesis{EX-0228,
  author = {Lars Ljung},
  title = {Markm{\aa}lf{\"o}ljning med multihypotesassociation},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2002},
  note = {IR-SB-EX-0228},
  month = dec,
  company = {SAAB Aerospace},
  authorpublic = {yes},
  abstract = {Target tracking is the process of following the position of a moving target by using observations from some kind of sensor. The association of observations with the right target is an important issue in target tracking. This problem gets harder when ground targets are tracked; this is due to the large amount of clutter delivered by the sensor. A promising method to address this issue is Multi Hypothesis Tracking (MHT). This method &#64257;nds a solution by forming several hypotheses and then investigates which of these that get support from later measurements. To prevent the number of hypotheses from growing exponentially in course of time the most unlikely ones must me deleted in a suitable manner. A multi hypothesis tracker and a simple simulation environment have been implemented. A couple of different methods to reduce the amount of hypotheses and possible tracks have been investigated to evaluate how they a&#64256;ect the ability to track targets. In this report it is shown that by using a divided gate the processing time can be reduced without seriously affecting the performance. It is also shown that by using CJPDA (Cheap Joint Probabilistic Data Association) in the MHT algorithm it is possible to use a larger depth in time. Further it is shown that it is not necessary to form all possible hypotheses since even a smaller subset gives a good result.}
}

@MastersThesis{EX-0301,
  author = {Fredrik {\"O}hman},
  title = {Link Adaptation Techniques for {HIPERLAN} Type 2},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2003},
  note = {IR-SB-EX-0301},
  month = jan,
  authorpublic = {yes},
  abstract = {Hiperlan/2 is a high speed radio communication system that operates in the 5 Ghz range. A Hiperlan/2 system can adjust the data rate depending on the quality of the current radio link, using link adaptation.  In this master thesis a new link adaptation technique for Hiperlan/2 is suggested. The algorithm makes its decisions based not only on the mean noise level on the channel, but also on the behavior of the channel for different frequencies.  The performance of the link adaptation technique is evaluated using simulations of a Hiperlan/2 system. A more conventional adaptation algorithm is also implemented for comparisons.  The simulations show that the adaptation technique suggested in this thesis will give higher throughput than the conventional type. }
}

@MastersThesis{EX-0302,
  author = {Mikael Gr{\"o}nblad},
  title = {Dual-Mode Multiuser Detectors for {DS-CDMA}},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2003},
  note = {IR-SB-EX-0302},
  month = jan,
  authorpublic = {yes},
  abstract = {In this master's thesis two types of dual-mode multiuser detectors is introduced that either dynamically switches its detection mode between matched-filter and decorrelator or matched-filter and successive interference cancellation (SIC). The reason for looking into these detectors is that in some cases the difference in performance between matched-filter and decorrelator and SIC is not enough to motivate the increase in complexity. When the background interferences dominates compared to the multiple access interference (MAI), the decorrelator performs worse than the matched-filter, and the faster matched-filter should be used. The SIC receiver is sensitive to poor channel estimates. When the channel is fast varying it is harder to obtain good estimates and the SIC will suffer a severe performance degradation under these conditions. This leads to that a dual-mode design can switch to the faster matched-filter without losing much in performance.  We have succeeded in creating a dual-mode detector design that reduces the overall computational requirement while maintaining similar performance as that of the single-mode detector, SIC. This design was compared to the already existing dual-mode detector that switches between matched-filter and decorrelator.  The evaluation was performed using MATLAB simulations and assuming a multiuser case with a slow, random and fast varying Rayleigh distributed channel coefficients. The performance metric is average BER, versus average SNR or number of active users. There are two types of dual-mode detectors in this thesis, type 1 is matched-filter and decorrelator, type 2, the new design, is matched-filter and SIC.  The results showed that type 1 detects 47 % of the bits in the faster matchedfilter mode if the case was a slow varying channel and still maintain a performance that is in between the single-mode matched-filter and decorrelator. For type 2 in the random varying channel case it detects 46 % of the bits in matched-filter and still has a performance in terms of BER that is close to that of the single-mode SIC detector. }
}

@MastersThesis{EX-0303,
  author = {Linus Falk},
  title = {Lab Equipment for System Identification},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2003},
  note = {IR-SB-EX-0303},
  month = jan,
  authorpublic = {yes},
  abstract = {The objective is to implement a system identification application on a digital signal processor, that can be used as part of a future laboratory exercise. Gen- eral issues in system identification such as the choice of sampling frequency and input signal properties are discussed. System identification algorithms are analyzed by there performance and implementation suitability. For estimation validation the Akaike information criteria is tested. Methods for conversion of discrete-time system models to continuous-time are viewed.  The resulting application has a quite good ability to estimate the frequency response of the investigated system, although the estimate in rare cases is un- stable. The estimated systems pole-zero placement and order does not always match the stated coefficients on the investigated system well.  }
}

