@MastersThesis{EX-0105,
  author = {Henrik Back},
  title = {Performance Evaluations for MIMO Antennas Applied to HIPERLAN/2},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2001},
  note = {IR-SB-EX-0105},
  month = mar,
  authorpublic = {yes},
  company = {Ericsson Radio Systems},
  abstract = {Future wireless systems demand high bitrates to provide the users with more and more advanced services. A wireless local area network that already shows great performance and high bitrates is HIPERLAN/2. A potential application to improve the peak bit rate in HIPERLAN/2 is to introduce more than one antenna at the receiver and transmitter and apply some kind of space-time coding. One space-time technique that has shown enormous spectral efficiency and great theoretical possibilities to increase the bitrate is the BLAST technique. The BLAST technique transmits different information on different transmit antennas and detect them jointly in the receiver. In this thesis a combined OFDM and BLAST technique has been implemented and simulated for different channel properties in typical urban macrocell environment. The channel model used is a very realistic model called COST259. Simulations show that it is possible to double the bitrate and still get a FER=1% by adding one antenna at the receiver and transmitter and apply the V-BLAST technique. In the case when the distance between the antennas in the transmitter as well as the receiver is half a wavelength the transmit power from each transmit antenna has to be increased by 9 dB to keep the above performance. If the receiver antenna distance is increased by a factor of 6 the power only needs to be increased with 2 dB. This indicates that the V-BLAST technique suffers greatly when the channels are highly correlated.}
}

