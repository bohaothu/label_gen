@MastersThesis{EX-0211,
  author = {Fredrik Fredriksson},
  title = {Evaluation of Methods for {MIMO} Capacity Enhancement},
  school = {Royal Institute of Technology (KTH)},
  address = {Dept. of Signals, Sensors \& Systems, Signal Processing},
  year = {2002},
  note = {IR-SB-EX-0211},
  month = may,
  authorpublic = {yes},
  abstract = {In recent years, with the evolution of wireless LAN (WLAN) standards such as IEEE 802.11a and Hiperlan/2, the possibilities of providing wireless wideband communication services have increased dramatically. These standards allow data rates in the range of 5-50 Mbps, which is in line with what most wired office networks provide today. However, judging from the evolution of communications in recent years, it will not be long before such data rates are insufficient to meet public demands. Consequently, extensive research is performed to develop methods of increasing the potential data rates of such wireless systems, without increasing the required bandwidth. <p> Operating in the 5 GHz frequency band, the previously mentioned WLAN standards are highly suitable for for different kinds of multiple-input multiple-output (MIMO) techniques. Using antenna arrays at both ends of the wireless link, these techniques exploit the spatial independence of the channels to provide diversity, thus increasing the signal-to-noise ratio (SNR) at the receiving end. Consequently, the potential performance gain of MIMO schemes will decrease with increased correlation between adjacent antenna elements. Furthermore, MIMO schemes require channel information, making them sensitive to errors in the channel estimates. <p> This thesis evaluates the performance of four different MIMO techniques and compares them to the common single-input single-output (SISO) and single-input multiple-output (SIMO) techniques used today. The evaluation is performed using MATLAB simulations and assuming dual transmit and receive antennas and frequency non-selective, slowly-fading channel with Rayleigh distributed channel coefficients. Furthermore, the spatial correlation of the channel is modelled using statistics obtained from measurements in an indoor office-like environment. The performance metric is average throughput, measured in bits/s/Hz, versus average SNR, given the expected correlation between true and estimated channel coefficients. <p> The evaluated MIMO schemes are: Space-time block codes, beamforming, waterfilling and a hybrid of beamforming and space-time block codes. The first scheme is based on the theory of orthogonal designs. The second and third schemes are based on the singular-value decomposition (SVD) theorem and the final one, being a hybrid of the first two schemes, is based on both the SVD theorem and orthogonal designs.  }
}

