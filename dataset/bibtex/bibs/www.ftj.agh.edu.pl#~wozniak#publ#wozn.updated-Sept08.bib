@Article{Adamczak:1996:ACS,
  author =       "A. Adamczak and M. P. Faifman and L. I. Ponomarev and
                 V. I. Korobov and V. S. Melezhik and R. T. Siegel and
                 J. Wozniak",
  affiliation =  "Inst. of Nucl. Phys., Cracow, Poland.",
  title =        "Atlas of cross sections for scattering of muonic
                 hydrogen atoms on hydrogen isotope molecules",
  journal =      "Atomic Data \& Nuclear Data Tables",
  volume =       "62",
  number =       "2",
  pages =        "255--344",
  publisher =    "",
  year =         "1996",
  keywords =     "",
  url =          "http://www.sciencedirect.com/science?_ob=ArticleURL&_udi=B6WBB-45N4PXF-T&_user=10&_handle=V-WA-A-W-B-MsSAYVA-UUW-U-AACBCYBCWA-AACAAZVBWA-ECVDEBDUE-B-U&_fmt=summary&_coverDate=03%2F31%2F1996&_rdoc=2&_orig=browse&_srch=%23toc%236706%231996%23999379997%23307537!&_cdi=6706&view=c&_acct=C000050221&_version=1&_urlVersion=0&_userid=10&md5=2f230055e19ad257695c6e11e3b18817",
  abstract =     "The total cross sections of the elastic, spin-flip,
                 and charge-exchange processes for the scattering of
                 muonic hydrogen isotope atoms (p$\mu$ , d$\mu$ , t$\mu$ ) in
                 the ground state on hydrogen isotope molecules (H$_2$, 
                 D$_2$, T$_2$, HD, HT, DT) are calculated. The
                 scattering cross sections of muonic hydrogen isotope
                 atoms on hydrogen isotope nuclei obtained earlier in
                 the multichannel adiabatic approach are used in the
                 calculations. Molecular effects (electron screening,
                 rotational and vibrational excitations of target
                 molecules, etc.) are taken into account. The spin
                 effects of the target molecules and of the incident
                 muonic atoms are included. The cross sections are
                 averaged over the Boltzmann distribution of the
                 molecule rotational states and the Maxwellian
                 distribution of the target molecule kinetic energies
                 for temperatures 30, 100, 300, and 1000 K. The cross
                 sections are given for kinetic energies of the incident
                 muonic atoms ranging from 0.001 to 100 eV in the
                 laboratory frame. (45 References).\par",
  accessionnumber = "005243313",
  ovidsource =   "Atomic Data \& Nuclear Data Tables, vol.62, no.2,
                 March 1996, pp.255-344. Publisher: Academic Press,
                 USA.",
}

