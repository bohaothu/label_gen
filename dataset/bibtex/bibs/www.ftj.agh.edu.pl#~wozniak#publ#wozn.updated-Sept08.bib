@InProceedings{Bulgakov:1996:NRC,
  author =       "T. Bulgakov and Vit. Bystritskii and G. Mesyats and
                 A. Sinebryukhov and V. Sinebryukhov  and V. Bystritsky and
                 V. Grebenyuk and S. Parzhitski and B. Sabirov and
                 V. Stolupin and A. Luchinsky and S. Chaikovsky and
                 V. Makhrin  and N. Ratakhin and S. Sorokin and A. Gula and
                 M. Filipowicz and J. Wozniak",
  affiliation =  "",
  title =        "Nuclear reactions cross section
                 measurement using {Z}-pinch technology",
  booktitle =      "Proc. 11-th  Int.   Conf.   on High Power
                 Particle Beams 'Beams'96'",
  volume =       "2",
  number =       "",
  pages =        "917",
  publisher =    "",
  year =         "1996",
  note =     "[June 10-14, 1996, Prague,  Czech  Republic]",
  abstract =     {Direct experimental estimate of the upper limit of the $d +
d$ -> $^3$He + $n$ cross section at deuteron energies below the keV region is
obtained for the first time. The experiment was performed at the Pulsed Ion
Beam Accelerator of the High-Current Electronics Institute in Tomsk, using
high intensity, radially converging deuteron beams, generated during
implosion of liner plasma. A two-jet liner made of 17\%D$_2$ + 83\%N$_2$
gas, was used, with the inner jet serving as a target. The $dd$-fusion
neutrons were detected by time-of-flight scintillator spectrometers and
BF$_3$ detectors of thermal neutrons placed in polyethylene moderator. The
upper limit obtained for the $d+d$ -> $^3$He + $n$ cross section for deuteron
energy 440 eV is &sigma < 2$\cdot 10^{-34}$ cm$^2$ at 90\% confidence level.
The result demonstrates that liner implosion technique can be used in the
investigations of nuclear reactions between light nuclei at infra low
energies, previously not accessible in experiments with classical beam
