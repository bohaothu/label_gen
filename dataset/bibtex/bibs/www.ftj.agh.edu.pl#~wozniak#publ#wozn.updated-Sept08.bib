}

@Article{Belyaev:1995:IIB,
  author =       "V. B. Belyaev and Vyach. M. Bystritsky and
                 V. M. Grebenyuk and O. I. Kartavtsev and S. S.
                 Parzhitsky and B. M. Sabirov and V. T. Sidorov and S. G.
                 Stetsenko and V. A. Stolupin and T. L. Bulgakov and G. A.
                 Mesyats and A. A. Sinebryukhov and Vit. M. Bystritsky and W.
                 Czaplinski and M. Filipowicz and A. Gula and E. Lacki and
                 J. Wozniak and F. E. Zyazula and S. K. Anrukhovich and A. V.
