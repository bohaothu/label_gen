@InCollection{Morstin:1977:HCF,
  author =       "K. Morstin and J. Wozniak",
  title =        "Hydrogen in Coal. {F}easibility of Determination by Neutron
                 Methods",
  booktitle =     "Nuclear Techniques and Mineral Resources 1977",
  year =         "1977",
  publisher =      "IAEA, Vienna",
  pages =        "119-133",
	abstract    = {An increasing interest in coal problems, such as
processing, underground  gasification, and combating the methane danger,
leeds to the need for techniques to determine rapidly the hydrogen content
in coal seams. Neutron methods seem to be particularly useful for this
purpose. In order to test their usefulness, neutron parameters of coal
versus its hydrogea content were calculated. Next, the stationary neutron
fields were considered using some different theoretical approaches. Strong
dependences are observed, thus confirming the feasibility of the proposed
determinations, especially with applying the epithermal-neutron gauges,
since the appreciable matrix effects can be expected in the case of thermal
neutron detection. The interferring influence of the borehole was considered
using the experimentally verified theoretical approach.
	  }
}

