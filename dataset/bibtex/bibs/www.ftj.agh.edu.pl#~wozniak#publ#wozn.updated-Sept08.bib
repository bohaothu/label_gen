@Article{Bystritsky:MHD,
  author = 	 {V.M. Bystritsky and V.V. Gerasimov and J. Wozniak},
  title = 	 {Measurement of helium-3 and deuterium stopping power ratio for negative muons},
  journal = 	 {Eur. Phys. J. D},
  year = 	 {2007},
  OPTkey = 	 {},
  volume = 	 {42},
  OPTnumber = 	 {},
  pages = 	 {79--84},
  OPTmonth = 	 {},
  OPTnote = 	 {},
  abstract =     {The measurement method and results measuring of the stopping power ratio of helium-3 and deuterium atoms for muons slowed down in the D/3He mixture are presented. Measurements were performed at four values of pure 3He gas target densities, $\varphi_{\rm He} = $0.0337, 0.0355, 0.0359, 0.0363 (normalized to the liquid hydrogen density) and at a density 0.0585 of the D/3He mixture. The experiment was carried out at PSI muon beam $\mu$E4 with the momentum $P\mu =$ 34.0 MeV/c. The measured value of the mean stopping ratio $S_{^3\rm\!He/D}$ is $1.66
+- 0.04$.
},
  OPTannote = 	 {}
}

@Article{Dudkin:ESR,
  author = 	 {G.N. Dudkin and B.A. Nechaev and V.N. Padalko and V.M. 
Bystritsky and J. Wozniak and V.I. Veretelnik and E.G. Furman},
  title = 	 {Energy Supply to the Region of Colliding Plasma Flows with Opposite
Polarization Fields},
  journal = 	 {Technical Physics Letters},
  year = 	 {2007},
  OPTkey = 	 {},
  volume = 	 {33},
  number = 	 {3},
  pages = 	 {235--238},
  OPTmonth = 	 {},
  note = 	 {(engl. transl from {\it Pisma v JTF}, {\bf 33}, 6 (2007) 15--22) },
  abstract =     {The interaction of two colliding deuterium plasma flows
   propagating in opposite directions across the magnetic field
   and having opposite directions of the polarization fields has
   been experimentally studied. The plasma flows with densities up
   to 10(16) cm(-3) stop and a velocity of up to 2 x 10(7) stop
   cm/s were formed using discharges initiated in crossed E x H
   fields. Each discharge operated at an electric power of up to
   300 mW and a discharge current up to 100 kA. The results of
   measurements of the equivalent capacitance of polarized plasma
   flows were used to estimate the transverse permittivity of
   plasma. The rate of depolarization in colliding flows was
   determined. The frequency and energy characteristics of a
   plasma LC circuit formed by colliding flows were estimated. The
   temporal modulation of plasma density in the flows was
   measured.},
  OPTannote = 	 {}
}
