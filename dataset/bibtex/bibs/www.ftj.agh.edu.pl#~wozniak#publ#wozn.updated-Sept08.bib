@Article{Belyaev:1995:NPI,
  author =       "V. B. Belyaev and A. Bertin and V. M. Bystritsky and
                 V. M. Bystritsky and A. Gula and O. I. Kartavtsev and
                 A. V. Kravtsov and A. V. Luchinsky and G. A. Mesyats
                 and L. A. Rivkis and N. A. Rotakhin and A. A.
