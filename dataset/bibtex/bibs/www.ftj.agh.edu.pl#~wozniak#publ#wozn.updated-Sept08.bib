@Article{Knowles:2001:ESM,
  author =       "P. E. Knowles and V. M. Boreiko and V. M. Bystritsky
                 and M. Filipowicz and O. Huot and F. Mulhauser and V.
                 N. Pavlov and F. M. Penkov and C. Petitjean and N. P.
                 Popov and V. G. Sandukovsky and L. A. Schaller and H.
                 Schneuwly and V. A. Stolupin and J. Wozniak",
  affiliation =  "Fribourg Univ., Switzerland.",
  title =        "Experimental search for $\mu$ d $^3${H}e fusion",
  journal =      "Hyperfine Interactions",
  volume =       "138",
  number =       "1-4",
  pages =        "289--94",
  publisher =    "",
  year =         "2001",
  keywords =     "",
  url  =         "http://springerlink.metapress.com/content/m016p9122t6p6h37/?p=c7c6ce8eedec4881be07cb3a03afa062&pi=45",
  abstract =     "The vast majority of muon catalyzed fusion research
                 has been concerned with muonic molecules of hydrogen
                 isotopes only, since the dynamics of higher-Z muonic
                 atoms in general preclude the formation of molecular
                 systems. In the specific case of hydrogen-helium
                 mixtures, bound muonic molecular states can exist, and
                 thus it is possible to search for the reaction d$\mu$ 
                 $^3$He ->  $\mu$ + &alpha (3.66 MeV) +
                 p (14.64 MeV). Until recently, the theoretical
                 predictions for the nuclear fusion rate in the d$\mu$ 
                 $^3$He molecule, &lambda$_f$ , ranged over one
                 order of magnitude, from 10$^5$ to 10$^6$ per
                 second. An experimental upper limit has been measured
                 for &lambda$_f$ in HD + 3He giving a value
                 < 6$\times 10^4$ s$^{-1}$. We
                 report on the analysis of an experiment in D$_2$ +
                 3He which has shown a signal coming either from
                 the muon catalyzed reaction, or from the fusion in
                 flight of 3He's formed from d$\mu$ d fusion. (7
                 References).\par",
  note = "[RIKEN Conference on Muon Catalyzed Fusion and
                 Related Exotic Atoms (MuCFO1). Shimoda, Japan. 22-26
                 April 2001]",
  accessionnumber = "007496413",
  ovidsource =   "Kluwer Academic Publishers. Hyperfine Interactions,
                 vol.138, no.1-4, 2001, pp.289-94. Netherlands.",
}

