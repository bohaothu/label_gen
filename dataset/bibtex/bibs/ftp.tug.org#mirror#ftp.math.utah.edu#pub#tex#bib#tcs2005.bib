@Article{Blaser:2005:BAS,
  author =       "Markus Bl{\"a}ser",
  key =          "Associative algebra; Lower bound; Multiplicative
                 complexity",
  title =        "Beyond the {Alder--Strassen} bound",
  journal =      j-THEOR-COMP-SCI,
  volume =       "331",
  number =       "1",
  pages =        "3--21",
  day =          "15",
  month =        feb,
  year =         "2005",
  CODEN =        "TCSCDI",
  ISSN =         "0304-3975 (print), 1879-2294 (electronic)",
  ISSN-L =       "0304-3975",
  bibdate =      "Thu Jul 7 14:28:15 MDT 2005",
  bibsource =    "http://www.sciencedirect.com/science/journal/03043975",
  abstract =     "We prove a lower bound of $\frac{5}{2}n^2$ for the
                 multiplicative complexity of $n\times n$-matrix
                 multiplication over arbitrary fields. More general, we
                 show that for any finite dimensional semisimple algebra
                 $A$ with unity, the multiplicative complexity $C(A)$ of
                 the multiplication in A is bounded from below by
                 $\frac{5}{2}\dim A - 3(n_1 + \cdots + n_\tau)$ if the
                 decomposition of $A \cong A_1 \times \cdots \times
                 A_\tau$ contains only noncommutative factors, that is,
                 the division algebra $D_\tau$ is noncommutative or
                 $n_\tau \geq 2$.\par

                 We also deal with the complexity of multiplication in
                 algebras with nonzero radical. We present an example
                 that shows that our methods in the semisimple case
                 cannot be applied directly to this problem. We exhibit
                 lower bound techniques for $C(A)$ that yield bounds
                 still significantly above the Alder--Strassen bound.
                 The main application is the lower bound for the
                 multiplicative complexity of multiplication in the
                 algebra $T_n(k)$ of upper triangular $n\times
                 n$-matrices.",
  acknowledgement = ack-nhfb,
  fjournal =     "Theoretical Computer Science",
}

