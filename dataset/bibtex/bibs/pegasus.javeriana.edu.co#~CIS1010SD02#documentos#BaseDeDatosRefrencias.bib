@INPROCEEDINGS{Min2009,
  author = {Min, Huaqing and Zeng, Jiaan and Luo, Ronghua},
  title = {Fuzzy CMAC with automatic state partition for reinforcementlearning},
  booktitle = {GEC '09: Proceedings of the first ACM/SIGEVO Summit on Genetic and
	Evolutionary Computation},
  year = {2009},
  pages = {421--428},
  address = {New York, NY, USA},
  publisher = {ACM},
  abstract = {Most of reinforcement learning (RL) algorithms use value function
	to seek the optimal policy. In large or even continuous states, function
	approximation approaches must be used to represent value function.
	The structures of function approximators influence the learning performance
	greatly. However, the design of structures relies too much on human
	designer and inappropriate design can lead to poor performance. In
	this paper, we propose a novel function approximator called Fuzzy
	CMAC (FCMAC) with automatic state partition (ASP-FCMAC) to automate
	the structure design for FCMAC. Based on CMAC (also known as tile
	coding), ASP-FCMAC employs fuzzy membership function to lower the
	computation load, and analyzes Bellman error as well as learning
	weights to partition the state automatically so as to generate the
	structure of FCMAC. Empirical results in both mountain car and RoboCup
	Keepaway domains demonstrate that ASP-FCMAC can automatically generate
	the structure of FCMAC and agent using it can learn efficiently.},
  doi = {http://doi.acm.org/10.1145/1543834.1543891},
  isbn = {978-1-60558-326-6},
  location = {Shanghai, China},
  owner = {Pacho},
  timestamp = {2010.02.08}
}

