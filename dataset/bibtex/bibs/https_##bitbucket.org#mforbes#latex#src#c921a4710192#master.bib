<a name="cl-16259"></a>	Pages = 120403,
<a name="cl-16260"></a>	Title = {Condensation of Pairs of Fermionic Atoms Near a Feshbach Resonance},
<a name="cl-16261"></a>	Volume = 92,
<a name="cl-16262"></a>	Year = 2004}
