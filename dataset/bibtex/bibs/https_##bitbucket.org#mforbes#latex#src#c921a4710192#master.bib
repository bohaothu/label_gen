<a name="cl-9981"></a>	Journal = {JCAP},
<a name="cl-9982"></a>	Keywords = {dark matter, x-rays, Chandra},
<a name="cl-9983"></a>	Number = 01,
<a name="cl-9984"></a>	Pages = {023},
<a name="cl-9985"></a>	Slaccitation = {%%CITATION = ASTRO-PH/0611506;%%},
<a name="cl-9986"></a>	Title = {Diffuse X-Rays: Directly Observing Dark Matter?},
<a name="cl-9987"></a>	Volume = 0801,
<a name="cl-9988"></a>	Year = 2008,
