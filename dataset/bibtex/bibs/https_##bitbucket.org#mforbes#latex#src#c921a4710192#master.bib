<a name="cl-4206"></a>	Pages = {282-295},
<a name="cl-4207"></a>	Title = {Positron annihilation radiation from the inner galaxy},
<a name="cl-4208"></a>	Volume = 559,
<a name="cl-4209"></a>	Year = 2001}
