<a name="cl-6723"></a>@article{Beane:2003da,
<a name="cl-6724"></a>	Abstract = {The two-nucleon sector is near an infrared fixed
<a name="cl-6725"></a>point of QCD and as a result the S-wave scattering
<a name="cl-6726"></a>lengths are unnaturally large compared to the
<a name="cl-6727"></a>effective ranges and shape parameters. It is usually
<a name="cl-6728"></a>assumed that a lattice QCD simulation of the
<a name="cl-6729"></a>two-nucleon sector will require a lattice that is
<a name="cl-6730"></a>much larger than the scattering lengths in order to
<a name="cl-6731"></a>extract quantitative information. In this paper we
<a name="cl-6732"></a>point out that this does not have to be the case:
<a name="cl-6733"></a>lattice QCD simulations on much smaller lattices
<a name="cl-6734"></a>will produce rigorous results for nuclear physics.},
<a name="cl-6735"></a>	Author = {Beane, S. R. and Bedaque, P. F. and Parreno, A. and Savage, M. J.},
<a name="cl-6736"></a>	Eprint = {hep-lat/0312004},
