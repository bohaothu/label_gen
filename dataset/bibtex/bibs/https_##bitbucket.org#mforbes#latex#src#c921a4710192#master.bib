<a name="cl-3924"></a>	Eprint = {arXiv:cond-mat/0505374},
<a name="cl-3925"></a>	Journal = prl,
<a name="cl-3926"></a>	Keywords = {fermion; thermodynamics; superfluidity; Bose-Einstein condensation},
<a name="cl-3927"></a>	Number = 9,
