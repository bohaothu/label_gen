<a name="cl-10413"></a>	Journal = prd,
<a name="cl-10414"></a>	Month = apr,
<a name="cl-10415"></a>	Number = {07},
<a name="cl-10416"></a>	Pages = {4023},
<a name="cl-10417"></a>	Title = {Domain walls and theta dependence in {QCD} with an effective Lagrangian approach},
<a name="cl-10418"></a>	Volume = {59},
