<a name="cl-8741"></a>	Pages = {205-376},
<a name="cl-8742"></a>	Slaccitation = {%%CITATION = HEP-PH 0402234;%%},
<a name="cl-8743"></a>	Title = {NJL model analysis of quark matter at large density},
<a name="cl-8744"></a>	Volume = 407,
<a name="cl-8745"></a>	Year = 2005}
<a name="cl-8746"></a>
<a name="cl-8747"></a>@article{Buballa:2004sx,
<a name="cl-8748"></a>	Abstract = {A schematic NJL-type model is employed to
