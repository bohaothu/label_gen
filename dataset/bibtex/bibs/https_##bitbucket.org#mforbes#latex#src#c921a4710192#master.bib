<a name="cl-10445"></a>	Slaccitation = {%%CITATION = PRLTA,84,4814;%%},
<a name="cl-10446"></a>	Title = {Can theta vacua be created in heavy ion collisions?},
<a name="cl-10447"></a>	Volume = {84},
<a name="cl-10448"></a>	Year = {2000}}
<a name="cl-10449"></a>
<a name="cl-10450"></a>@article{Kharzeev:2007tn,
<a name="cl-10451"></a>	Abstract = {We examine the recent suggestion that {P}- and {CP}-odd
<a name="cl-10452"></a>effects in {QCD} matter can induce electric charge
