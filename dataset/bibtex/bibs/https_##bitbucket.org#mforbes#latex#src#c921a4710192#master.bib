<a name="cl-2247"></a>	Journal = {Phys.~Rept.},
<a name="cl-2248"></a>	Keywords = {universality; Large scattering length; Renormalization group; three-body; Efimov effect; Limit cycle; Discrete scale invariance; Hyperspherical formalism; Radial laws; Effective field theory},
