<a name="cl-607"></a>	Group = {Neutron Star},
<a name="cl-608"></a>	Keywords = {neutron matter; neutron star; lattice},
<a name="cl-609"></a>	Title = {On the relativistic Thomas-Fermi treatment of compressed atoms and compressed massive nuclear density cores},
<a name="cl-610"></a>	Year = {2009},
<a name="cl-611"></a>	Bdsk-File-1 = {}}
