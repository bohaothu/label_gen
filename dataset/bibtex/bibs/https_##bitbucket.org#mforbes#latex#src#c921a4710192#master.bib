<a name="cl-12367"></a>	Journal = {Annu. Rev. Astron. Astrophys.},
<a name="cl-12368"></a>	Pages = {37--64},
<a name="cl-12369"></a>	Title = {A critical review of galactic dynamos},
<a name="cl-12370"></a>	Volume = {37},
<a name="cl-12371"></a>	Year = {1999}}
<a name="cl-12372"></a>
