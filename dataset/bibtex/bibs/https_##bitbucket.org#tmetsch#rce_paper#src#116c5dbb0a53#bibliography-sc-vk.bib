<a name="cl-256"></a>@INPROCEEDINGS{ForkertKSSW01,
<a name="cl-257"></a>  author = {Tomas Forkert and Hans-Peter Kersken and Andreas Schreiber and Martin
<a name="cl-258"></a>        Strietzel and Klaus Wolf},
<a name="cl-259"></a>  title = {The Distributed Engineering Framework {TENT}},
<a name="cl-260"></a>  booktitle = {Vector and Parallel Processing -- VECPAR 2000: 4th International
<a name="cl-261"></a>        Conference},
<a name="cl-262"></a>  year = {2001},
<a name="cl-263"></a>  editor = {Palma, J.M.L.M.; Dongarra, J.; Hern�ndez, V.},
<a name="cl-264"></a>  volume = {1981},
<a name="cl-265"></a>  series = {Lecture Notes in Computer Science},
<a name="cl-266"></a>  pages = {38-46},
<a name="cl-267"></a>  address = {Berlin / Heidelberg},
<a name="cl-268"></a>  publisher = {Springer},
<a name="cl-269"></a>  abstract = {The paper describes TENT, a component-based framework for the integration
<a name="cl-270"></a>        of technical applications. TENT allows the engineer to design, automate,
<a name="cl-271"></a>        control, and steer technical workflows interactively. The applications
<a name="cl-272"></a>        are therefore encapsulated in order to build components which conform
<a name="cl-273"></a>        to the TENT component architecture. The engineer can combine the
<a name="cl-274"></a>        components to workflows in a graphical user interface. The framework
<a name="cl-275"></a>        manages and controls a distributed workflow on arbitrary computing
<a name="cl-276"></a>        resources within the network. Due to the utilization of CORBA, TENT
<a name="cl-277"></a>        supports all state-of-the-art programming languages, operating systems,
<a name="cl-278"></a>        and hardware architectures. It is designed to deal with parallel
<a name="cl-279"></a>        and sequential programming paradigms, as well as with massive data
<a name="cl-280"></a>        exchange. TENT is used for workflow integration in several projects,
<a name="cl-281"></a>        for CFD workflows in turbine engine and aircraft design, in the modeling
<a name="cl-282"></a>        of combustion chambers, and for virtual automobile prototyping.},
<a name="cl-283"></a>  owner = {maas},
<a name="cl-284"></a>  timestamp = {2007.12.22},
<a name="cl-285"></a>  url = {http://springerlink.metapress.com/content/c17v1kra7m649raj/?p=8edfe88911f243749cd82bdf99b4e552&amp;pi=2}
<a name="cl-286"></a>}
<a name="cl-287"></a>
<a name="cl-288"></a>@INPROCEEDINGS{ForkertKKS04,
<a name="cl-289"></a>  author = {Tomas Forkert and Guy K. Kloss and Christian Krause and Andreas Schreiber},
<a name="cl-290"></a>  title = {Techniques for Wrapping Scientific Applications to {CORBA} Components},
<a name="cl-291"></a>  booktitle = {9th International Workshop on High-Level Programming Models and Supportive
<a name="cl-292"></a>        Environments (HIPS 2004)},
<a name="cl-293"></a>  year = {2004},
<a name="cl-294"></a>  pages = {100-108},
<a name="cl-295"></a>  publisher = {IEEE Computer Society},
<a name="cl-296"></a>  abstract = {This paper presents wrapping techniques that we have developed in
<a name="cl-297"></a>        order to integrate applications into the scientific workflow management
<a name="cl-298"></a>        system TENT. These techniques take into account the varying degrees
<a name="cl-299"></a>        of controllability found for each application and utilize various
