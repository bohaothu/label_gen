<a name="cl-864"></a>  author = {Armin Rigo and Samuele Pedroni},
<a name="cl-865"></a>  title = {{PyPy&#39;s} Approach to Virtual Machine Construction},
<a name="cl-866"></a>  booktitle = {OOPSLA &#39;06: Companion to the 21st ACM SIGPLAN symposium on Object-oriented
<a name="cl-867"></a>        programming systems, languages, and applications},
<a name="cl-868"></a>  year = {2006},
