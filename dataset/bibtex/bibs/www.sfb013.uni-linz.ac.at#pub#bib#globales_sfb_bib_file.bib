@Article{RISC2930,
   author = {E. Kartashova and V. L'vov},
   title = {{A model of intra-seasonal oscillations in the Earth atmosphere}},
   language = {english},
   abstract = {We suggest a way of rationalizing an intra-seasonal oscillations (IOs) of the Earth atmospheric flow fields as a few specific, resonantly interacting triads of planetary waves, isolated from the system of all the rest planetary waves. In the meteorologically significant domain we found (in each Hemisphere) four such triads, localized in narrow latitudinal belts around 19, 28 and 34 grads. Our model is independent of the topography (mountains, etc.) and can give a natural explanation of IOs both in the North and South Hemispheres.},
   journal = {Physical Review Letters},
   volume = {98},
   number = {198501},
   pages = {1--4},
