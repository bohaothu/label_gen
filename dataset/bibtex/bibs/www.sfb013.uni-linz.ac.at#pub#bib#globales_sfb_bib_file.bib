@Unpublished{RISC3238,
   author = {Burkhard Zimmermann},
   title = {Definite Symbolic Integration and Summation of Special Functions},
   language = {english},
   abstract = {Abstract: Fasenmyer's algorithm computes a re\-cur\-ren\-ce for the sum $\sum_{k=0}^\infty f(n,k)$ where $f(n,k)$ is a given hypergeometric term. It was generalized by Wilf and Zeilberger to the ($q$-)case and to multisums. The talk presents a new generalization of (Wilf and Zeilberger's generalization of) Fasenmyer's algorithm to a larger class of special functions. This class consists of $\partial$-finite functions satisfying certain technical conditions. It includes, in particular, the holonomic functions, and enjoys various closure properties. Applications to certain sums and integrals involving orthogonal polynomials are given, using our Mathematica implementation of our algorithm.},
   year = {2004},
   month = {July},
   note = {Contributed talk at Summer School On Orthogonal Polynomials And Special Functions},
