}

@inproceedings { ISI:000245459700311,
	unique-id = {ISI:000245459700311},
	isbn = {978-7-03-017974-6},
	abstract = {Both axiomatic design and TRIZ have their advantage and disadvantage in
   product design. Axiomatic design has advantage in producing conceptual
   design, and TRIZ has advantage in solving design contradiction. A
   method of integrating axiomatic design and TRIZ to solve design problem
   is proposed. Firstly axiomatic design is used to produce conceptual
   design. Then the potential contradiction in the design is examined and
   the technical contradiction or physical contradiction is determined.
   Subsequently proper contradiction solving law is adopted based on
   contradiction type. Finally new design parameter is redesigned to
   eliminate contradiction. A case study of hydraulic work-over rig is
   given to demonstrate the proposed method.},
