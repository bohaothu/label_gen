@article { ISI:000249857300008,
	unique-id = {ISI:000249857300008},
	issn = {0020-7543},
	doi = {10.1080/00207540701450179},
	abstract = {As our society becomes more environmentally conscious, manufacturers
   must incorporate `environmental qualities' into products. This paper
   proposes a general design methodology to effectively support
   environmentally consciousness design of products. The methodology
   employs three tools: LCA ( life cycle assessment), QFDE ( quality
   function deployment for environment), and TRIZ ( theory of inventive
   problem solving). QFDE is a tool developed by modifying and extending
   QFD, and is extended further in this paper. In addition, connecting two
   consecutive tools is realized by established relations among those
   tools. The presented methodology is proved through application to a
   hair dryer to effectively support the product planning and conceptual
   design stages. For instance, designers could utilize one of the LCA
   results that the product has a high impact on global warming through
   energy consumption during its use phase to define a requirement
   objectively in QFDE, `reduce the energy consumption' with a high
   weighting. TRIZ allowed designers to generate four improvement
   solutions. The most highly evaluated was using resonance frequency on
   the motor. This was obtained from a QFDE result that `dry quickly' and
   `dry quietly' have a contradiction. The methodology has a larger
   benefit than is obtained from utilizing those three tools independently.},
	pages = {4143-4162},
	number = {18-19},
	volume = {45},
	year = {2007},
	journal = {INTERNATIONAL JOURNAL OF PRODUCTION RESEARCH},
	title = {A QFD-centred design methodology for environmentally conscious product
   design},
	author = {Sakao , Tomohiko}
}

@inproceedings { ISI:000254108200031,
	unique-id = {ISI:000254108200031},
	abstract = {Conceptual design is the most critical stage in the whole design
   process because its outputs, i.e., concepts, will directly determine
   the success of the whole design. In the conceptual design stage, it
   would be greatly beneficial if there was a tool to help the creative
   thinking process such that designers could represent, manipulate,
   re-use, and share mental models to easily generate novel ideas.
   Mind-mapping, a method frequently used in corporations, presents the
   possibility of building and manipulating innovation processes. Existing
   approaches in building mind maps, however, only play a role in
   documenting, planning, and organizing thinking processes; proper
   guidance to create new ideas is limited. This paper presents the
   concept of incorporating semantics into a mind map. By considering
   different creative thinking methods such as the brainstorming,
   historical examination, TRIZ method, and etc., we explored the
   applications of these semantics to guide the growth of a mind map, and
   built a software system that can help users easily create new ideas and
   re-use existing knowledge in creative conceptual design. Several design
   examples are illustrated in detail in this paper to show the
   applications of the semantic mind map. We believe the newly defined
   semantic mind map has the potential to help users to conduct creative
   conceptual design, build interconnection among different concepts, and
   re-use existing design knowledge effectively.},
	note = {International Conference on Mechanical Engineering and Mechanics, Wuxi,
   PEOPLES R CHINA, NOV 05-07, 2007},
	pages = {143-149},
	year = {2007},
	booktitle = {PROCEEDINGS OF THE INTERNATIONAL CONFERENCE ON MECHANICAL ENGINEERING
   AND MECHANICS 2007, VOLS 1 AND 2},
	title = {Applying semantic mind map to creative conceptual design},
	editor = {Batra, RC and Qian, LF and Li, XN and Zhou, KD and Dresig, H and Morita, Y and Cheung, E},
	author = {Li , Ho-Ying and Yao , Zhiyang}
}

