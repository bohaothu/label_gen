@inproceedings { ISI:000252881800001,
	unique-id = {ISI:000252881800001},
	abstract = {Innovation has become the business of business. It distinguishes the
   leaders from the laggards, the successful from the unsuccessful. As
   important as innovation is, its several dimensions and perspectives are
   not well defined or understood. And, without such understanding, it is
   difficult to establish what it is, what it implies for the organization
   and its clients, and when it has been achieved. In this paper the
   various perspectives on innovation are explored. First, the verb-noun
   confusion - is innovation a process or a result? Second, the primary
   forms of innovation - from product to service to business model, and
   variations among these. Particular emphasis is given to service
   innovation as most organizations operate in, and/or compete among,
   service-based economies. Finally, some of the issues associated with
   creating a culture of innovation are discussed. The cultural aspect
   will, for many, be their most difficult obstacle to overcome.},
	note = {China Private Economy Innovation International Forum, Taizhou, PEOPLES
   R CHINA, AUG 11-12, 2007},
	pages = {3-17},
	year = {2007},
	booktitle = {PROCEEDING OF CHINA PRIVATE ECONOMY INNOVATION INTERNATIONAL FORUM},
	title = {Perspectives on innovation},
	editor = {Sun, L and Wang, CB},
	author = {Welke , Richard J.}
}

@inproceedings { ISI:000250810200015,
