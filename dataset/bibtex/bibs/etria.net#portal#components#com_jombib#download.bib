	pages = {145-154},
	year = {2007},
	booktitle = {Future of Product Development},
	title = {Trends of evolutions and patent analysis: An application in the
   household appliances field},
	editor = {Krause, FL},
	author = {Crotti , A. and Ghitti , M. and Regazzoni , D. and Rizzi , C.}
}

@inproceedings { ISI:000252721200022,
	unique-id = {ISI:000252721200022},
	isbn = {978-7-5629-2610-8},
	abstract = {Technical innovation is the motivation of high-tech enterprises'
   critical capability. Customer value is the starting point for analyzing
   innovation patterns. From the point of view, a technical innovation
   pattern for high-tech enterprises is constructed based on TRIZ and QFD
   (Quality Function Development). TRIZ is considered the most
   comprehensive and systemic theory for technical innovation today. And
   customer requirements can well represented by QFD method. Methodology
   of robust design is integrated into the innovation pattern on the basis
   of technical innovation and customer driven. The route of the pattern
   begins with understanding customer requirements with QFD. Then
   innovative design concept and process concept are defined with the
   combination of TRIZ. Product parametric value and specifications are
   determined by making use of robust design. The results would be
   insensitive to variation. Finally, above technical innovation pattern
   is formed with the target of customer value. A total cost optimizing
   model is put forward based on the whole life cycle of product in order
   to translate the pattern from theoretic study into application. With
   the model, outputs of this innovation pattern can be effectively
   evaluated. Applying basements are provided for implementing it.},
	note = {4th International Conference on Innovation and Management, Ube, JAPAN,
   DEC 05-06, 2007},
	pages = {116-119},
	year = {2007},
	booktitle = {PROCEEDINGS OF THE 4TH INTERNATIONAL CONFERENCE ON INNOVATION \&
   MANAGEMENT, VOLS I AND II},
	title = {Construction of technical innovation pattern and optimizing model of
   high-tech enterprises based on TRIZ and Quality Function Development},
	editor = {Kaminishi, K and Duysters, G},
	author = {Aiguo , Wang and Zhihong , Zhang}
}

