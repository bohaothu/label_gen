@Article{Asselin:1994:OCS,
  author =       "Daniel Asselin and Henri-H. Arsenault and Donald
                 Prevost",
  title =        "Optical circular sampling system for translation and
                 rotation invariant pattern recognition",
  journal =      j-OPT-COMM,
  volume =       "110",
