  title =        "Document image compression using document analysis and
                 block-class-specific data compression methods",
  journal =      "Proceedings of SPIE --- The International Society for
                 Optical Engineering",
  volume =       "2186",
  pages =        "146--155",
  year =         "1994",
  CODEN =        "PSISDG",
