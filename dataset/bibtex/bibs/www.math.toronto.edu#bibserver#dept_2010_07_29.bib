@article{chan_regularity_2008,
	Abstract = {We consider the fractional Burgers' equation on {\${\textbackslash}R{\textasciicircum}N\$} with the critical dissipation term. We follow the parabolic {De-Giorgi's} method of Caffarelli and Vasseur {{\textbackslash}cite{Driftdiffusion}} and show existence of smooth solutions given any initial datum in {\$L{\textasciicircum}2({\textbackslash}R{\textasciicircum}N)\$.}},
