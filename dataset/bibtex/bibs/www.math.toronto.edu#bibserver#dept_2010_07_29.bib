@article{bulut_stability_2009,
	Abstract = {In this paper we establish a complete local theory for the energy-critical nonlinear wave equation {(NLW)} in high dimensions \${{\textbackslash}mathbb R} {\textbackslash}times {{\textbackslash}mathbb R}{\textasciicircum}d\$ with \$d {\textbackslash}geq 6\$. We prove the stability of solutions under the weak condition that the perturbation of the linear flow is small in certain space-time norms. As a by-product of our stability analysis, we also prove local well-posedness of solutions for which we only assume the smallness of the linear evolution. These results provide essential technical tools that can be applied towards obtaining the extension to high dimensions of the analysis of Kenig and Merle {\textbackslash}cite{keme06} of the dynamics of the focusing {(NLW)} below the energy threshold. By employing refined paraproduct estimates we also prove unconditional uniqueness of solutions for \$d{\textbackslash}ge 5\$ in the natural energy class. This extends an earlier result by Furioli, Planchon and Terraneo {{\textbackslash}cite{FPT03}} in dimension \$d=4\$.},
	Author = {Aynur Bulut and Magdalena Czubak and Dong Li and Nata{\"o}a Pavlovic and Xiaoyi Zhang},
	Journal = {0911.4534},
	Keywords = {Mathematics - Analysis of {PDEs}},
	Month = nov,
	Title = {Stability and Unconditional Uniqueness of Solutions for Energy Critical Wave Equations in High Dimensions},
	Url = {http://arxiv.org/abs/0911.4534},
	Year = {2009},
	Bdsk-Url-1 = {http://arxiv.org/abs/0911.4534}}

