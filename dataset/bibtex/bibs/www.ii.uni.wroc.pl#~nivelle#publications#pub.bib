@INPROCEEDINGS{deNivelle2002a,
  AUTHOR       = {Hans de Nivelle},
  EDITOR       = {Julian Bradfield},
  TITLE        = {Extraction of Proofs from the Clausal Normal Form 
                  Transformation},
  BOOKTITLE    = {Proceedings of the 16th International Workshop on Computer 
                  Science Logic (CSL 2002)},
  PADDRESS     = {Berlin, Germany},
  ADDRESS      = {Edinburgh, Scotland, UK},
  PUBLISHER    = {Springer Verlag},
  MONTH        = {September},
  SERIES       = {Lecture Notes in Artificial Intelligence},
  VOLUME       = {2471},
  ISBN         = {3-540-44240-5},
  YEAR         = {2002},
  PAGES        = {584--598},
  ABSTRACT     = {The paper discusses three problems that arise
                  when one wants to construct a clausal normal form of 
		  a first-order formula, and simultaneously construct 
		  a proof that this clausal normal form is correct. 
		  We assume that the resulting proofs are in type theory. 
		  First the problem of non-equivalence preserving
		  transformations is discussed. It turns out that 
		  all such transformations are second order provable
		  if one assumes choice axioms. 
		  Second, the problem of the size of the resulting proofs
		  is discussed. It can be handled by using introduction of 
		  definitions, and by using proof normalization.
		  The third problem discussed, is the problem 
		  of handling strong and optimized Skolemization.
		  We show that both types of Skolemization can be
		  reduced to standard Skolemization, so that only
		  one type of choice axioms is needed.}, 
}


