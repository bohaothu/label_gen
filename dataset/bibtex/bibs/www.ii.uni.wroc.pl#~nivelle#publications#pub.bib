


@MASTERSTHESIS{deNivelle1991,
   AUTHOR      = {Hans de Nivelle},
   TITLE       = {Complexity of Interpretation Problems in Truth Maintenance 
                  Systems},
   SCHOOL      = {Technische Universiteit Delft (the Netherlands)}, 
   YEAR        = 1991,
   MONTH       = {August},
   ABSTRACT    = {A Truth Maintenance System is a system that maintains 
                  {\em beliefs} using {\em non-monotonic justifications}.
                  A non-monotonic justification is a rule of form
                  $ A_1, \ldots, A_p, \mbox{abs}(B_1), \ldots, \mbox{abs}(B_q) 
                   \rightarrow C, $ where $ \mbox{abs}(B_j) $ means that the
                  belief $ B_j $ have to be absent. 
                  Mostly, there are more than one interpretation possible:
                  For example, the set 
                  $ \abs(A) \rightarrow B, \ 
                    \abs(B) \rightarrow A $ 
                  would allow two belief sets, believing $ \{ A \}, $ 
                  and believing $ \{ B \}. $ 
                  The general problem of computing belief sets is
                  NP-hard. If one allows $ 3 $-valued beliefs
                  (by introducing dontknow), the problem becomes
                  quadratic. This is called {\em skeptical semantics}.
                  Unfortunately, for most interesting rule sets, nearly
                  all beliefs will have the status 'dontknow', which
                  is not very useful. 
                  I showed in the thesis, that the skeptical semantics
                  is the best possible tractable semantics. 
               }
}


