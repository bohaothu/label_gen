@InProceedings{torrens97jcl-aaai,
  Author         = {Torrens, Marc and Weigel, Rainer and Faltings,
                   Boi V.},
  Title          = {Java Constraint Library: bringing constraints
                   technology on the Internet using the Java
                   language},
  Comment        = {Java Constraint Library, JCL},
  BookTitle      = {Constraints \& Agents -- Papers from the AAAI
                   Workshop at the 14th National Conference on
                   Artificial Intelligence (AAAI'97), Providence,
                   Rhode Island, USA, 27.--31. Juli 1997, Technical
                   Report WS-97-05},
  Annote         = {Distributed problem solving on the web is becoming
                   more and more important. Client-server
                   architectures are often confronted with performance
                   problems due to server overload. In this paper we
                   will present a methodology that tries to minimise
                   the server load and thus can allow certain
                   application to be put on the Internet. The idea is
                   to formalise the problem as a constraint
                   satisfaction problem (CSP). This formalization
                   supports a natural decomposition of the task into
                   two subtasks. Namely the generation of the CSP that
                   needs to be done on the server, because in this
                   phase a database might have to be accessed and a
                   second task to solve the CSP that can be done
                   locally on the client. For solving the CSP
                   generated by the server we provide the Java
                   Constraint Library (JCL) that allows the problem to
                   be solved on any Internet capable computer.},
  Editor         = {Freuder, Eugene C.},
  Publisher      = {AAAI Press},
  Address        = {Menlo Park, California, USA},
  Year           = 1997,
  Note           = {Erweiterte Fassung: {\citep{torrens97jcl-cp}}},
  ISBN           = {1-57735-032-4},
  URL            = {\biburl{http://liawww.epfl.ch/Publications/Archive/Torrens1997.pdf}},
  URLDate        = {14. Februar 2005},
  xURL           = {http://liawww.epfl.ch/cgi-bin/Pubs/single_entry?bibtex_key=Torrens1997a},
  xURL           = {http://www.marctorrens.com/EN/publications.html},
  xURL           = {http://www.aaai.org/Press/Reports/Workshops/ws-97-05.html},
  xURL           = {http://www.aaai.org/Conferences/National/1997/aaai97.html},
}

