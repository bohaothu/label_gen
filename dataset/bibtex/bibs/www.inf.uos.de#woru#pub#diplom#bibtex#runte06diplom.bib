@InProceedings{rossi90equivalence,
  Author         = {Rossi, Francesca and Petrie, Charles J. and
                   Dhar, Vasant},
  Title          = {On the Equivalence of Constraint Satisfaction
                   Problems},
  BookTitle      = {Proceedings of the 9th European Conference on
                   Artificial Intelligence (ECAI'90), Stockholm,
                   Sweden, 6.--10. August 1990},
  Abstract       = {A solution of a Constraint Satisfaction Problem
                   (CSP) is an assignment of values to all its
                   variables such that all its constraints are
                   satisfied. Usually two CSPs are considered
                   equivalent if they have the same solution set. We
                   find this definition limiting, and develop a more
                   general definition based on the concept of mutual
                   reducibility. In this extended scheme it is
                   reasonable to consider a pair of CSPs equivalent
                   even if they have different solutions. The basic
                   idea behind the extended scheme is that two CSPs
                   can be considered equivalent whenever they contain
                   the {"}same information{"}, in the sense that it is
                   possible to obtain the solution of one from the
                   solution of the other, and viceversa. In this
                   scheme, both constraint and variable redundancy are
                   allowed in CSPs belonging to the same equivalence
                   class, since they do not affect the information
                   content of the problem. By using this new
                   definition of equivalence, we prove that binary and
                   non-binary CSPs are equivalent. Such a result was
                   known before, but never formally proved due to the
                   inadequacy of the available technical tools. This
                   is the first confirm that our new definition of
                   equivalence is both reasonable and useful.},
  Editor         = {Aiello, {Luigia Carlucci}},
  Publisher      = {Pitman Publishing},
  Address        = {London, Boston},
  Pages          = {550--556},
  Month          = {6.--10. } # aug,
  Year           = 1990,
  Note           = {Ausführliche Version:
                   {\citep{rossi89equivalence}}},
  ISBN           = {0-273-08822-X},
  xURL           = {http://www.eccai.org/ecai-proceedings.shtml},
  xURL           = {http://www.informatik.uni-trier.de/~ley/db/conf/ecai/ecai90.html},
  Comment        = {defines a new vision of CSP equivalence (extended
                   CSP equivalence)},
}

