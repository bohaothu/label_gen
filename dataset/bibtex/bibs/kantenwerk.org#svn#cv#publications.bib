	Keywords = {semiBlog, Semantic Blogging, DERI},
	Local-Url = {file://localhost/Users/knud/Projekte/sw.org/knud/papers/semiBlogISD2005Word/semiBlogISD2005print.pdf},
	Month = {August},
	Pages = {855--866},
	Title = {{semiBlog --- Semantic Publishing of Desktop Data}},
