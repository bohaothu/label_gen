@inproceedings{Ambrus:2009it,
	Abstract = {The FAST environment [1] allows users to graphically compose intelligent, i.e. semantically annotated gadgets from predefined building blocks and deploy them on various mashup platforms, thus enabling the interconnection of different systems and services. Since different parties involved in the creation of gadgets and building blocks conceptualise knowledge in different ontologies, using ontology matching as a means to reconcile differences in the various conceptualisations is a crucial issue. This paper discusses first steps in our effort to integrate ontology matching in an end-user-oriented environment such as FAST, using an e-commerce-related scenario. We evaluate a number of tools and approaches for solving different levels of complexity in ontology matching and define the direction of integrating ontology matching into the FAST environment.},
	Author = {Oszkar Ambrus and Knud M{\"o}ller and Siegfried Handschuh},
	Booktitle = {Workshop on User-generated Services (UGS2009) at ICSOC2009, Stockholm, Sweden},
	Date-Added = {2010-02-04 17:18:01 +0000},
	Date-Modified = {2010-02-04 17:18:01 +0000},
	Read = {Yes},
	Title = {Towards Ontology Matching for Intelligent Gadgets},
	Year = {2009}}

@inproceedings{hoyer2009fast,
	Abstract = {The transfer of the mashup paradigm in corporate environments needs additional capabilities beyond those typically associated with consumer mashups. In this paper, we present the architecture of the FAST platform which allows creating enterprise-class and multi-channel visual building blocks (so called gadgets) in an ad-hoc manner. The design of complex enterprise-class gadgets is supported by an integrated semantic concept which hides the complexity from the actual users. The architectural components of the platform, a technical life cycle model for enterprise mashups, and the FAST gadget ontology are presented. By means of a cross-organizational real-world scenario from the marketing/ promotion event area, we demonstrate the value and potential of the FAST platform.},
	Author = {Volker Hoyer and Till Janner and Ivan Delchev and Javier L{\'o}pez and Sebastian Ortega and Rafael Fern{\'a}ndez and Knud Hinnerk M{\"o}ller and Ismael Rivera and Marcos Reyes and Manuel Fradinho},
	Booktitle = {The 7th International Joint Conference on Service Oriented Computing (ICSOC2009), Stockholm, Sweden.},
	Date-Added = {2010-02-04 17:17:53 +0000},
	Date-Modified = {2010-02-04 17:17:53 +0000},
	Month = {November},
	Title = {The {FAST} Platform: An Open and Semantically-enriched Platform for Designing Multi-Channel and Enterprise-Class Gadgets},
	Year = {2009},
	Bdsk-File-1 = {}}

@article{moeller2009ontology_soft_skills,
	Abstract = {In this paper we argue for the importance of soft ontology factors, or ``ontology soft skills'' as an important aspect for the quality of an ontology. Ontology soft skills are those features of an ontology which relate less to technical and structural issues, but rather to how well an ontology manages to attract new users and implementers and how well it manages to achieve adoption and uptake. We present a set of five guidelines for good ontology soft skills and illustrate them with concrete examples from the Semantic Web Conference Ontology (SWC). We argue that greater adoption and uptake mean greater sharedness, and therefore ultimately a more ``beautiful'' ontology.},
	Author = {Knud M{\"o}ller and Sean Bechhofer and Tom Heath and Siegfried Handschuh},
	Date-Added = {2010-02-04 17:17:46 +0000},
	Date-Modified = {2010-02-04 17:55:19 +0000},
	Journal = {Applied Ontology},
	Read = {Yes},
	Title = {Ontology Soft Skills --- The {Semantic Web Conference Ontology}},
	Volume = {Special Edition on ``Beautiful Ontologies''},
	Year = {2010},
	Bdsk-File-1 = {}}

