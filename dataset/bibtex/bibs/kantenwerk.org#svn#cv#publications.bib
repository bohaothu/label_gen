@inproceedings{moeller2005semiBlog,
	Abstract = {The Semantic Web (SW) still faces a chicken-and-egg problem: until a substantial amount of data becomes available, many people will not see a use for the SW; but as long as people do not see its use, they may not be willing to spend the time and effort required to generate data in a SW-conformant fashion. In this paper, we propose that the recent phenomenon of blogging, combined with a tool to easily generate SWdata from existing formal desktop data, can result in a form of semantic blogging which would help to overcome the aforementioned problem and make the SW vision a reality. We discuss how semantic metadata could be represented in a blog, and present a prototype of the semiBlog editor, which was created with the purpose of semantic blogging in mind. We argue that such a semantic blog editor should integrate tightly with a user's desktop environment, as this would make integration of existing data into the blog as easy as possible and would also increase the motivation to add to the Semantic Web.},
	Address = {Karlstad, Sweden},
	Author = {Knud M{\"o}ller and John G. Breslin and Stefan Decker},
	Booktitle = {14th Conference on Information Systems Development (ISD2005)},
	Date-Added = {2008-05-08 17:44:37 +0100},
