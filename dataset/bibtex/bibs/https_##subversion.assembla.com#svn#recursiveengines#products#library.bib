journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Adult,Cellular Phone,Cellular Phone: statistics \& numerical data,Communication,Data Collection,Data Collection: methods,Factor Analysis, Statistical,Friends,Humans,Observation,Social Behavior,Students},
month = sep,
