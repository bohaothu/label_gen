file = {:C$\backslash$:/Papery/pdf/Onnela et al. - Structure and tie strengths in mobile communication networks. - 2007.pdf:pdf},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Cellular Phone,Communication,Humans,Interpersonal Relations,Probability},
month = may,
number = {18},
