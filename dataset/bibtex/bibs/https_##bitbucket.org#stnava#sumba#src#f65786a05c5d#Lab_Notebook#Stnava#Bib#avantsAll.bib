<a name="cl-992"></a>  pmid = {19150502},
<a name="cl-993"></a>  timestamp = {2009.04.02},
<a name="cl-994"></a>  url = {http://dx.doi.org/10.1016/j.neuroimage.2008.12.016}
<a name="cl-995"></a>}
