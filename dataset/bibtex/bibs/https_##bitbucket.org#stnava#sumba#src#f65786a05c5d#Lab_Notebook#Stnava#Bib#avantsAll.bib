<a name="cl-1310"></a>  author = {Junghoon Kim and Brian Avants and Sunil Patel and John Whyte and
<a name="cl-1311"></a>	Branch H Coslett and John Pluta and John A Detre and James C Gee},
<a name="cl-1312"></a>  title = {Structural consequences of diffuse traumatic brain injury: A large
<a name="cl-1313"></a>	deformation tensor-based morphometry study.},
<a name="cl-1314"></a>  journal = {Neuroimage},
