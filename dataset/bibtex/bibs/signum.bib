@Article{Higginbotham:1986:AF,
  author =       "T. F. Higginbotham",
  title =        "Another factor of $(10^{31} -1)/9$",
  journal =      j-SIGNUM,
  volume =       "21",
  number =       "3",
  pages =        "12--12",
  month =        jul,
