@INPROCEEDINGS{APL-011,
  author = {G. G. Borg and J. H. Harris and N. M. Martin and D. Thorncraft and R. Milliken and D. Miljak and B. Kwan and J. Kircher},
  title = {An Investigation of the Properties and Applications of Plasmas Antennas (Paper 0271)},
  booktitle = {Millenium Conference on Antennas and Propagation},
  year = {2000},
  address = {Davos, Switzerland},
  month = {9-14 apr},
  abstract = {Recent experimental results
	 [1] have demonstrated that plasma can achieve comparable electrical
	 performance to metal in conventional antenna applications. In these
	 experiments plasma was formed and contained in an evacuated cylindrical
	 tube filled with 10 mTorr - 2 Torr of noble gas mixtures such as a
	 fluorescent lighting tube. Adequate efficiencies (up to 50) for most
	 applications, predictable radiation patterns and low base-band noise for HF
	 and VHF communications have been demonstrated. Such antennas may
	 therefore offer a viable alternative to metal in existing applications
	 when overall technical requirements are considered. The electrical
	 properties of plasma can be varied in both space and time. Thus plasma
	 antennas can be rapidly reconfigured for directionality and frequency
	 agility or turned off to eliminate current ringing. When the plasma
	 is extinguished it no longer scatters radiation. As a result, it
	 has a significantly reduced radar cross-section. Unlike metals, the
	 plasma electrical properties are dependent on frequency. Below the
	 plasma frequency, the plasma behaves like a metal. Above the plasma
	 frequency, the plasma behaves like a dielectric. A plasma antenna can
	 therefore be employed for communications at HF while having a reduced
	 cross-section to microwave radar. Extinguishing the antenna eliminates mutual
	 interference with neighbouring antennas suggesting applications in antenna
	 arrays and antenna farms. The RF Hub of the Australian Defence Science
	 and Technology Organisation and The Plasma Research Laboratory of
	 the Australian National University have been collaborating on the
	 development of plasma antenna technology. The goals of the program
	 have been to investigate the efficacy of plasma antennas, establish
	 their potential to replace metal antennas in some circumstances, and
	 investigate the possibility of creating novel antenna structures that
	 overcome some of the limitations of current antenna technologies.
	 In this paper we describe the progress of this work. We review the
	 electromagnetic properties of plasma that are relevant for antenna
	 applications and we review our main experimental results. A theory of
	 cylindrical plasma antenna elements has been developed [2] and it will be
	 compared with experiment. The performance of plasma antennas in certain
	 telecommunications and radar applications will also be presented.},
}

