@ARTICLE{Beddoe2006,
  author = {G. Beddoe and S. Petrovic},
  title = {Selecting and Weighting Features using a Genetic Algorithm in a Case-Based
	Reasoning Approach to Personnel Rostering},
  journal = {European Journal of Operational Research},
  year = {2006},
  volume = {175},
  pages = {649--671},
  abstract = { Personnel rostering problems are highly constrained resource allocation
	prob- lems. Human rostering experts have many years of experience
	in making rostering de- cisions which reect their individual goals
	and objectives. We present a novel method for capturing nurse rostering
	decisions and adapting them to solve new problems using the Case-Based
	Reasoning (CBR) paradigm. This method stores examples of previously
	encountered constraint violations and the operations that were used
	to repair them. The violations are represented as vectors of feature
	values.We investigate the problem of select- ing and weighting features
	so as to improve the performance of the case-based reasoning approach.
	A genetic algorithm is developed for o-line feature selection and
	weighting us- ing the complex data types needed to represent real-world
	nurse rostering problems. This approach signicantly improves the
	accuracy of the CBR method and reduces the number of features that
	need to be stored for each problem. The relative importance of dierent
	features is also determined, providing an insight into the nature
	of expert decision making in personnel rostering. Keywords: Scheduling,
	Knowledge Based Systems, Case-Based Reasoning, Meta-heuristics, Feature
	Selection and Weighting},
  date-modified = {2007-01-18 11:46:32 +0000},
  url = {http://www.asap.cs.nott.ac.uk/publications/pdf/sxp_EJORNEW.pdf}
}

