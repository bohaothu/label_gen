@ARTICLE{Aickelin2004,
  author = {U. Aickelin and P. White},
  title = {Building Better Nurse Scheduling Algorithms},
  journal = {Annals of Operations Research},
  year = {2004},
  volume = {128},
  pages = {159--177},
  abstract = {The aim of this research is twofold: Firstly, to model and solve a
	complex nurse scheduling problem with an integer programming formulation
	and evolutionary algorithms. Secondly, to detail a novel statistical
	method of comparing and hence building better scheduling algorithms
	by identifying successful algorithm modifications. The comparison
	method captures the results of algorithms in a single figure that
	can then be compared using traditional statistical techniques. Thus,
	the proposed method of comparing algorithms is an objective procedure
