IEEE Catalog Number: 07TH8963C},
)

@inproceedings(Chuang:2007:cec,
title={Integrated Radiation Optimization: Inspired by the Gravitational Radiation in the Curvature of Space-Time}, 
author={Cheng-Long Chuang and Joe-Air Jiang},
  pages = {3157--3164},
  booktitle = "2007 IEEE Congress on Evolutionary Computation",
  year = 2007,
  editor = "Dipti Srinivasan and Lipo Wang",
