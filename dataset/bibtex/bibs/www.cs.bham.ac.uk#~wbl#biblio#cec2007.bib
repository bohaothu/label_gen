@inproceedings(Dol&#237;vka:2007:cec,
title={Using the Differential Evolution Algorithm for the Multi-Objective Optimization of a Switched-Current Circuit}, 
author={Luk\'a\v{s} Dol\'ivka and Ji\v{r}\'i Hospodka},
  pages = {1351--1358},
  booktitle = "2007 IEEE Congress on Evolutionary Computation",
  year = 2007,
