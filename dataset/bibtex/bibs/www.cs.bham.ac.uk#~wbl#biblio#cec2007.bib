IEEE Catalog Number: 07TH8963C},
)

@inproceedings(Davendra:2007:cec,
 title={Enhanced Differential Evolution Hybrid Scatter Search for Discrete Optimization}, 
author={Donald Davendra and Godfrey Onwubolu},
 pages = {1156--1162},
