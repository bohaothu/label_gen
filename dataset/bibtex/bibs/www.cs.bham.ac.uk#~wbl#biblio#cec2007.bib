IEEE Catalog Number: 07TH8963C},
)

@inproceedings(Petalas:2007:cec,
title={Entropy.Based Memetic Particle Swarm Optimization for Computing Periodic Orbits of Nonlinear Mappings}, 
author={Y. G. Petalas and K. E. Parsopoulos and M. N. Vrahatis},
  pages = {2040--2047},
  booktitle = "2007 IEEE Congress on Evolutionary Computation",
  year = 2007,
  editor = "Dipti Srinivasan and Lipo Wang",
