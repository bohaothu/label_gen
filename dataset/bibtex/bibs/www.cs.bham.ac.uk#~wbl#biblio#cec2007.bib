@inproceedings(Shir:2007:cec,
title={The Application of Evolutionary Multi-Criteria Optimization to Dynamic Molecular Alignment}, 
author={Ofer M. Shir and Michael Emmerich and Thomas B\"ack and Marc J. J. Vrakking},
  pages = {4108--4115},
  booktitle = "2007 IEEE Congress on Evolutionary Computation",
  year = 2007,
  editor = "Dipti Srinivasan and Lipo Wang",
  address = "Singapore",
