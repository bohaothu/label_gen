IEEE Catalog Number: 07TH8963C},
)

@inproceedings(Goth:2007:cec,
title={Initial Results with EpiSwarm, A Swarm-Based System for Investigating Genetic Epistasis}, 
author={Thomas Goth and Chia-Ti Tsai and Fu-Tien Chiang and Clare Bates Congdon},
  pages = {3855--3861},
  booktitle = "2007 IEEE Congress on Evolutionary Computation",
  year = 2007,
  editor = "Dipti Srinivasan and Lipo Wang",
