IEEE Catalog Number: 07TH8963C},
)

@inproceedings(Machwe:2007:cec,
title={Enabling Generative Behavior within an Interactive Evolutionary Design System Using a Component-Based Representation}, 
author={Azahar T. Machwe and Ian C. Parmee},
  pages = {2964--2971},
  booktitle = "2007 IEEE Congress on Evolutionary Computation",
