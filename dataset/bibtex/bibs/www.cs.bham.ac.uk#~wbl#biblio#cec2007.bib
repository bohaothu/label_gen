@inproceedings(Shaker:2007:cec,
title={Investigating Hybrids of Evolutionary Search and Linear Discriminant Analysis for Authorship Attribution}, 
author={Kareem Shaker and David Corne and Richard Everson},
  pages = {2071--2077},
  booktitle = "2007 IEEE Congress on Evolutionary Computation",
  year = 2007,
  editor = "Dipti Srinivasan and Lipo Wang",
  address = "Singapore",
