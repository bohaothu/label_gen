author={A. S. Ramkumar and S. G. Ponnambalam and N. Jawahar},
  pages = {4005--4011},
  booktitle = "2007 IEEE Congress on Evolutionary Computation",
  year = 2007,
  editor = "Dipti Srinivasan and Lipo Wang",
  address = "Singapore",
