@inproceedings(Cazangi:2007:cec,
title={An Evolutionary Approach for Autonomous Robotic Tracking of Dynamic Targets in Healthcare Environments}, 
author={Renato R. Cazangi and Craig Feied and Michael Gillam and Jonathan Handler and Mark Smith and Fernando J. Von Zuben},
  pages = {3654--3661},
  booktitle = "2007 IEEE Congress on Evolutionary Computation",
