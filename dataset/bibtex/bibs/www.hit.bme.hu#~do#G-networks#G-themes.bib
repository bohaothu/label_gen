@INPROCEEDINGS{Gelenbe2007e,
author={Gelenbe, E. and Sakellari, G. and D'Arienzo, M.},
booktitle={Self-Adaptive and Self-Organizing Systems, 2007. SASO '07. First International Conference on}, title={Controlling Access to Preserve QoS in a Self-Aware Network},
