}

@ARTICLE{Gelenbe1996b,
author={Gelenbe, E. and Yutao Feng and Krishnan, K.R.R.},
journal={Proceedings of the IEEE},
title={Neural network methods for volumetric magnetic resonance imaging of the human brain},
year={1996},
volume={84},
number={10},
