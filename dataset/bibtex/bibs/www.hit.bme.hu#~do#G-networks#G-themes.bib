}

@article{Hey20083822,
title = "Reduced complexity algorithms for cognitive packet network routers",
journal = "Computer Communications",
volume = "31",
number = "16",
pages = "3822 - 3830",
year = "2008",
note = "Performance Evaluation of Communication Networks (SPECTS 2007)",
issn = "0140-3664",
