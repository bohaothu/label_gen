	Institution = {Sealife Project: EU  Reference IST-2006-027269},
	Keywords = {web-services and bioinformatics and accessibility and data-types and wsdl and interfaces and use-cases and molecular-biology},
	Title = {Specification of available services},
	Year = {2007}}
