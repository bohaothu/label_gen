@Article{Agerwala:1977:SES,
  author =       "Tilak Agerwala",
  title =        "Some Extended Semaphore Primitives",
  journal =      j-ACTA-INFO,
  volume =       "8",
  number =       "3",
  pages =        "201--220",
  day =          "9",
  month =        aug,
