@Article{Ernst:1977:RIP,
  author =       "George W. Ernst",
  title =        "Rules of Inference for Procedure Calls",
  journal =      j-ACTA-INFO,
  volume =       "8",
  number =       "2",
  pages =        "145--152",
  day =          "24",
  month =        may,
  year =         "1977",
  CODEN =        "AINFA2",
  ISSN =         "0001-5903 (print), 1432-0525 (electronic)",
  ISSN-L =       "0001-5903",
  bibsource =    "ftp://ftp.ira.uka.de/pub/bibliography/Compiler/bevan.bib;
                 ftp://ftp.ira.uka.de/pub/bibliography/Misc/HBP/ACTAI.bib",
  abstract =     "The semantics of procedures and parameters were
                 formalized by Hoare \cite{Hoare:soal:1971} in terms of
                 axioms and rules of inference. Igarashi et al.
                 \cite{Igarashi:London:Luckham:acta:1975} reformulated
                 Hoare's system. This paper extends their rule of
                 inference for procedure calls to allow
                 \begin{enumerate} \item actual variable parameters to
                 occur in actual value parameters; \item the body of a
                 procedure to contain global variables that do not occur
                 in assignment positions; \item post-conditions and
                 ``internal'' assertions of a procedure to refer to the
                 initial values of variable parameters. \end{enumerate}
                 None of these are allowed in Hoare's system or the
                 system of Igarashi, London and Luckham. An attractive
                 feature of these extensions is that they do not
                 increase the complexity of the rules of inference.",
  checked =      "19940503",
  refs =         "5",
}

@Article{Munch-Andersen:1977:SAJ,
