@Article{Eve:1977:CTC,
  author =       "J. Eve and R. Kurki-Suonio",
  title =        "On computing the transitive closure of a relation",
  journal =      j-ACTA-INFO,
  volume =       "8",
  number =       "4",
  pages =        "303--314",
  day =          "25",
  month =        nov,
  year =         "1977",
  CODEN =        "AINFA2",
  ISSN =         "0001-5903 (print), 1432-0525 (electronic)",
  ISSN-L =       "0001-5903",
  bibsource =    "ftp://ftp.ira.uka.de/pub/bibliography/Compiler/bevan.bib;
                 ftp://ftp.ira.uka.de/pub/bibliography/Misc/allison.bib;
                 ftp://ftp.ira.uka.de/pub/bibliography/Misc/HBP/ACTAI.bib",
  abstract =     "An algorithm is presented for computing the transitive
                 closure of an arbitrary relation which is based upon
                 Tarjan's algorithm \cite{Tarjan:sicomp:1972} for
                 finding the strongly connected components of a directed
                 graph. A new formulation, justifying a somewhat
                 simplified statement of the latter, characterises
                 weaker restrictions on the form of the graph traversal
                 than Tarjan's depth first conditions and reveals
                 aspects of the behaviour of this algorithm which have
                 been obscure hitherto. If $V$ is the number of vertices
                 in the directed graph representing the relation then
                 the worst case behaviour, $O(V^3)$ is inferior to
                 existing algorithms
                 \cite{Artazarov:Dinic:Kronrod:Faradzev:1970,Furman:1970}
                 which require $O(V^{3}/\log V)$ and $O(V^{\log _{2} 7}
                 \log V)$ operations respectively. The best case
                 performance, $O(V^2)$ operations, is better. Viewed in
                 this way, it is similar to algorithms
                 \cite{Purdom:bit:1970,Syslo:Dzikiewicz:1975,Warshall:jacm:1962}
                 but it combines the improved efficiency in the presence
                 of strongly connected components which characterises
                 the algorithms
                 \cite{Purdom:bit:1970,Syslo:Dzikiewicz:1975} with the
                 advantages of Warshall's algorithm
                 \cite{Warshall:jacm:1962}, namely, succinctness, a
                 single traversal of the directed graph and ability to
                 exploit the availability of Boolean vector
                 operations.",
  annote =       "Improves on Tarjan algorithm $O(V^3)$ worst case worse
                 than existing algorithms but $O(V^2)$ best cases better
                 than others.",
  checked =      "19940503",
  keywords =     "algorithm; component; directed graph; graph; relation;
                 strongly connected component; transitive closure",
  refs =         "8",
}

