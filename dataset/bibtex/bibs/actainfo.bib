@Article{Watt:1977:PPA,
  author =       "David A. Watt",
  title =        "The Parsing Problem for Affix-Grammars",
  journal =      j-ACTA-INFO,
  volume =       "8",
  number =       "1",
  pages =        "1--20",
  day =          "29",
  month =        mar,
  year =         "1977",
  CODEN =        "AINFA2",
  ISSN =         "0001-5903 (print), 1432-0525 (electronic)",
  ISSN-L =       "0001-5903",
  bibsource =    "ftp://ftp.ira.uka.de/pub/bibliography/Compiler/AG.bib;
                 ftp://ftp.ira.uka.de/pub/bibliography/Compiler/bevan.bib;
                 ftp://ftp.ira.uka.de/pub/bibliography/Misc/HBP/ACTAI.bib",
  abstract =     "Affix grammars are an extension of context-free
                 grammars which retain most of their advantages and
                 eliminate most of their limitations with respect to the
                 definition of programming languages and the
                 specification of their translators. The extension
                 allows definition of context-sensitive syntax features,
                 and also allows semantics to be linked to syntax. In
                 this paper, the parsing problem for affix grammars is
                 explored and shown to be closely related to the parsing
                 problem for context-free grammars. This enables a
                 standard context-free parser constructor to be
                 generalised to a constructor for affix grammars,
                 essentially by addition of a preprocessor. The
                 resulting constructors are compared with previously
                 implemented or proposed constructors.",
  checked =      "19940503",
  keywords =     "affix grammars; divers.affix; parsing",
  refs =         "18",
  sjb =          "The introduction explains that affix grammars (AGs) of
                 Koster \cite{Koster:a68i:1972} provide a unified
                 notation for defining phrase structure and
                 context-sensitive constraints. Each nonterminal in an
                 AG may have associated with it a fixed number of
                 affixes. These represent attributes of the
                 corresponding language constructs. The affixes may be
                 passed as parameters to primitive predicates. These are
                 arbitrary functions which impose the necessary
                 context-sensitive constrains on the affixes. [The
                 predicates are defined separately from the AG; thus the
                 formalisation of the AG is only partial] A distinction
                 is made between derived affixes and inherited ones. It
                 is the usual distinction made about attributes in
                 attributed grammars. [Is there really a difference
                 between affix grammars and attribute grammars?] The
                 differences between AGs and TLGs is that the latter
                 makes no distinction between inherited and derived
                 attributes. Also in TLGs the predicates are defined
                 grammatically [Whatever that means]. As shown in Watt's
                 Ph.d \cite{Watt:phd:1974} TLGs do not lend themselves
                 to automatic construction of efficient parsers.
                 \subsubsection*{An example of an Affix Grammar} To
                 handle simple define before use. For example the
                 following would be valid :- \begin{example} dcl x use x
                 end \end{example} whereas the following would be
                 invalid :- \begin{example} dcl x use y end dcl x dcl x
                 use x end \end{example} The BNF for this simple
                 language is :- \begin{example} program \(\rightarrow\)
                 list seq 'end' list \(\rightarrow\) 'dcl' tag list
                 \(\rightarrow\) list 'dcl' tag seq \(\rightarrow\) seq
                 \(\rightarrow\) seq 'use' tag tag \(\rightarrow\) 'x'
                 tag \(\rightarrow\) 'y' tag \(\rightarrow\) 'z'
                 \end{example} To cope with the context sensitive stuff,
                 affixes are added :- \begin{example} program :
                 list\(\uparrow\)SET, set\(\downarrow\)SET,'end'.
                 list\(\uparrow\)SET: 'dcl',
                 tag\(\uparrow\)TAG,declare\(\downarrow\)\emptyset\(\downarrow\)TAG\(\uparrow\)SET.
                 list\(\uparrow\)SET: list\(\uparrow\)SET1, 'dcl',
                 tag\(\uparrow\)TAG,declare\(\downarrow\)SET1\(\downarrow\)TAG\(\uparrow\)SET.
                 seq\(\downarrow\)SET: seq\(\downarrow\)SET:
                 seq\(\downarrow\)SET, 'use',
                 tag\(\uparrow\)TAG,identity\(\downarrow\)SET\(\downarrow\)TAG.
                 tag\(\uparrow\)x: 'x' tag\(\uparrow\)y: 'y'
                 tag\(\uparrow\)z: 'z' \end{example} The syntax of the
                 BNF has now changed slightly. The ``\(\rightarrow\)''
                 has changed to ``:'', items are now separated by ``,''
                 rather than whitespace and each production ends with
                 ``.''. The ``\(\uparrow\)'' and ``\(\downarrow\)''
                 signify the direction of the affix i.e. derived and
                 inherited respectively. The definition contains
                 reference to two primitive functions. These are defined
                 as follows (in no particular format) :- \begin{example}
                 declare(s\(\sb{1}\), t, s\(\sb{2}\)) = if t \(\in\)
                 s\(\sb{1}\) then \(false\) else s\(\sb{2}\) =
                 s\(\sb{1}\) \(\cup\) \{t\} identify(s, t) = t \(\in\) s
                 \end{example} \subsubsection*{Definition of Affix
                 Grammars} Two pages of dense definitions. e.g. an affix
                 grammar is an 8 tuple \ldots.
                 \subsubsection*{Well-formed Affix Grammars} Equally
                 mind numbing stuff about what it is to be a well-formed
                 affix grammar. \subsubsection*{Parsing Strategy} How
                 you actually parse these things. Consider parsing :-
                 \begin{example} dcl x use x end \end{example} This
                 initially matches the rule :- \begin{example}
                 list\(\uparrow\)SET: 'dcl',
                 tag\(\uparrow\)TAG,declare\(\downarrow\)\emptyset\(\downarrow\)TAG\(\uparrow\)SET.
                 \end{example} In a top down parse ``SET'' will be
                 initially undefined. The ``dcl'' will be parsed by the
                 rule and then the ``x'' will be dealt with by the rule
                 :- \begin{example} tag\(\uparrow\)x: 'x' \end{example}
                 Thus the value of ``TAG'' is now ``x''. The function
                 ``declare'' is then called and thus the affix ``SET''
                 becomes \{x\}. One way of actually implementing the
                 above is to have an affix stack. The rest of the paper
                 then goes on to discuss the stack method.",
}

@Article{Luckham:1977:PTW,
  author =       "David C. Luckham and Norihisa Suzuki",
  title =        "Proof of Termination within a Weak Logic of Programs",
  journal =      j-ACTA-INFO,
  volume =       "8",
  number =       "1",
  pages =        "21--36",
  day =          "29",
  month =        mar,
  year =         "1977",
  CODEN =        "AINFA2",
  ISSN =         "0001-5903 (print), 1432-0525 (electronic)",
  ISSN-L =       "0001-5903",
  bibsource =    "ftp://ftp.ira.uka.de/pub/bibliography/Compiler/bevan.bib;
                 ftp://ftp.ira.uka.de/pub/bibliography/Compiler/semantics.bib;
                 ftp://ftp.ira.uka.de/pub/bibliography/Misc/HBP/ACTAI.bib;
                 ftp://ftp.ira.uka.de/pub/bibliography/Theory/dershowitz.bib",
  abstract =     "A weak logic of programs is a formal system in which
                 statements that mean ``the program halts'' cannot be
                 expressed. In order to prove termination, we would
                 usually have to use a stronger logical system. In this
                 paper we show how we can prove termination of both
                 iterative and recursive programs within a weak logic by
                 augmenting the programs with counters and adding bound
                 assertions on the counters as loop invariants and entry
                 conditions. Thus, most of the existing verifiers which
                 are based on weak logic of programs can be used to
                 prove termination of programs without any modification.
                 We give examples of proofs of termination and of upper
                 bounds on computation time that were obtained using a
                 Pascal program verifier. the use of the method to
                 locate causes of non-termination is also illustrated.",
  checked =      "19940503",
  keywords =     "termination; weak logic",
  refs =         "16",
}

