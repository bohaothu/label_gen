  title =        "The relaxed min-max heap. {A} mergeable double-ended
                 priority queue",
  journal =      j-ACTA-INFO,
  volume =       "30",
  number =       "3",
  pages =        "215--231",
  month =        may,
  year =         "1993",
  CODEN =        "AINFA2",
  ISSN =         "0001-5903 (print), 1432-0525 (electronic)",
  ISSN-L =       "0001-5903",
  MRclass =      "68P05 (68P20)",
  MRnumber =     "94h:68038",
  bibdate =      "Sat Oct 9 09:56:22 MDT 1999",
  bibsource =    "ftp://ftp.ira.uka.de/pub/bibliography/Misc/HBP/ACTAI.bib;
                 ftp://ftp.ira.uka.de/pub/bibliography/Theory/heap.bib;
                 MathSciNet database",
  abstract =     "A data structure that implements a mergeable
                 double-ended priority queue, namely the {\em relaxed
                 min-max heap}, is presented. A relaxed min-max heap of
                 $n$ items can be constructed in $O(n)$ time. In the
                 worst case, operations {\em find\_min}() and {\em
                 find\_max}() can be performed in constant time, while
                 each of the operations {\em merge}(), {\em insert}(),
                 {\em delete\_min}(), {\em delete\_max}(), {\em
                 decrease\_key}(), and {\em delete\_key}() can be
                 performed in $O(\log n)$ time. Moreover, {\em insert}()
                 has $O(1)$ amortized running time. If lazy merging is
                 used, {\em merge}() will also have $O(1)$ worst-case
                 and amortized time. The relaxed min-max heap is the
                 first data structure that achieves these bounds using
                 only two pointers (plus one bit) per item.",
  acknowledgement = ack-nhfb,
  refersto =     "\cite{Atkinson$^+$86}, \cite{Brown78},
                 \cite{Carlsson87-1}, \cite{Driscoll$^+$88},
                 \cite{Floyd64}, \cite{Fredman$^+$86},
                 \cite{Fredman$^+$87-1}, \cite{Gambosi$^+$91},
                 \cite{Hasham$^+$87}, \cite{Knuth73},
                 \cite{Olariu$^+$91-2}, \cite{Sack$^+$85},
                 \cite{Sleator$^+$86}, \cite{Vuillemin78},
                 \cite{Williams64}, \cite{Weiss92}",
}

