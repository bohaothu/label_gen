@article{SimonTaylorKiss:10,
	Affiliation = {E{\"o}tv{\"o}s Lor{\'a}nd University Budapest Institute of Mathematics Budapest Hungary},
	Author = {Simon, P{\'e}ter and Taylor, Michael and Kiss, Istvan},
	Date-Added = {2011-01-31 14:18:55 +0000},
