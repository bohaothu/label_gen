	Keywords = {Carotid bifurcation; MRI; atherosclerosis; 3D ultrasound; wall shear stress; haemodynamics; method comparison},
	Keywords-Plus = {computational fluid-dynamics; wall shear-stress; blood flow; artery bifurcation; pulsatile flow; steady flow; hemodynamics; models; atherosclerosis; reproducibility},
	Language = {{English}},
