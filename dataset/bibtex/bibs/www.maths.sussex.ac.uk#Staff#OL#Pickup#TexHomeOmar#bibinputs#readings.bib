	Keywords = {intracranial aneurysms, morphological characterization, 3-D Zernike moments, image recomposition, computational medicine, medical imaging},
	Keywords-Plus = {Unruptured Cerebral Aneurysms; Aspect Ratio; Natural-History; Saccular Aneurysms; Risk-Factors; Rupture; Recognition; Shape; Size; Hemodynamics},
	Language = {English},
