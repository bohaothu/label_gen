@techreport{BlomkerJentzen:10,
	Author = {Bl{\"o}mker, Dirk and Jentzen, Arnulf},
	Date-Added = {2011-05-15 17:35:43 +0100},
	Date-Modified = {2011-05-15 17:40:17 +0100},
	Institution = {Universit{\"a}t Augsburg (DE) and "Wolfgang Goethe" Universit{\"a}t Frankfurt am Mainz (DE)},
	Keywords = {Galerkin approximations, stochastic partial differential equation, stochastic heat equation, stochastic reaction diffusion equation, stochastic Burgers equation, strong error criteria.},
	Mrclass = {60H15, 35K90},
	Title = {Galerkin approximations for the stochastic Burgers equation},
	Type = {Preprint},
	Year = {2010},
	Abstract = {Existence and uniqueness for semilinear stochastic evolution equations with additive noise by means of finite dimensional Galerkin approximations is established and the convergence rate of the Galerkin approximations to the solution of the stochastic evolution equation is estimated.
These abstract results are applied to several examples of stochastic partial differential equations (SPDEs) of evolutionary type including the stochastic heat equation, stochastic reaction diffusion equations and the stochastic Burgers equation. The estimated convergence rates are illustrated by numerical simulations.
The main novelty in this article is to estimate the difference of the finite dimensional Galerkin approximations and of the infinite dimensional SPDE uniformly in space, i.e. in the L∞-topology, instead of the usual Hilbert-space estimates in the L2-topology, that were shown before.},
	Bdsk-File-1 = {}}

