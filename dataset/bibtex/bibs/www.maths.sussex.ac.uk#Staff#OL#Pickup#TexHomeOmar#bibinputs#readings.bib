	Keywords = {3D ultrasound; carotid artery; plaque; quantification; reconstruction; stenosis},
	Keywords-Plus = {3-dimensional ultrasound; power-mode; duplex ultrasonography; velocity-measurements; endarterectomy trial; surgery trial; color Doppler; stenosis; accuracy; flow},
	Language = {English},
