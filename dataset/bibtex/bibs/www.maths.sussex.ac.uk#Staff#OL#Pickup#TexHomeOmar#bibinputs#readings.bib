	Keywords-Plus = {WALL SHEAR-STRESS; NONPLANAR CAROTID BIFURCATION; CLINICAL ULTRASOUND SCANNER; ANATOMICAL FLOW PHANTOMS; FREEHAND 3D ULTRASOUND; VULNERABLE PLAQUES; DOPPLER ULTRASOUND; BLOOD-FLOW; VELOCITY; PRESSURE},
	Language = {English},
