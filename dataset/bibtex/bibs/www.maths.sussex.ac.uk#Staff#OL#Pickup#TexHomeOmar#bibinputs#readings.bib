	Journal-Iso = {{ESAIM-Math. Model. Numer. Anal.-Model. Math. Anal. Numer.}},
	Keywords = {Finite element method; energy norm; aposteriori error estimation; hydro-mechanical coupling; poroelasticity},
	Keywords-Plus = {{BIOTS CONSOLIDATION PROBLEM; FINITE-ELEMENT METHODS; RECONSTRUCTION; EQUATIONS}},
	Language = {{English}},
	Month = {{MAR-APR}},
	Number = {{2}},
