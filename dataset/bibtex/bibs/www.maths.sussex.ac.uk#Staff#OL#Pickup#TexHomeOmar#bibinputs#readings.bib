@article{dziuk-elliott:08,
	Author = {Dziuk, G. and Elliott, C. M.},
	Date-Added = {2009-01-21 17:57:36 +0000},
	Date-Modified = {2009-01-21 17:59:42 +0000},
	Fjournal = {Interfaces and Free Boundaries. Mathematical Modelling, Analysis and Computation},
	Issn = {1463-9963},
	Journal = {Interfaces Free Bound.},
	Local-Url = {file://localhost/Users/omar/Literature/Author/DziukElliott-2008-A.pdf},
	Mrclass = {65M60 (35K20 35K35)},
	Mrnumber = {MR2383539 (2009a:65250)},
	Mrreviewer = {Christoph Ortner},
	Number = {1},
	Pages = {119--138},
	Title = {Eulerian finite element method for parabolic {PDE}s on implicit surfaces},
	Volume = {10},
	Year = {2008},
	Annote = { The article is concerned with the formulation of finite element methods for second- and fourth-order parabolic equations on implicitly defined surfaces.

A hypersurface is defined as a level set of a map $\Phi$. In contrast to methods where the PDE is solved directly on a triangulation of the surface, the idea is to solve it on all level sets simultaneously by discretizing a suitable variational formulation by a finite element method. This approach is advantageous, for example, when a surface is given as a level set of the solution of a coupled equation, or when topological changes occur.

After a thorough introduction to differential calculus on level sets, several PDEs on surfaces are introduced and variational formulations are derived which lead to natural Galerkin (semi-) discretizations by $H^1$-conforming finite element methods. Fourth-order equations are treated by splitting them into second-order systems. The implicit Euler scheme is used for time discretization. A wide range of numerical experiments are presented, including experimental orders of convergence. Optimal first-order convergence rates in $L^2(H^1)$-type norms and second-order convergence rates in $L^\infty(L^2)$-type norms are observed.
--- Reviewed by Christoph Ortner},
	Bdsk-File-1 = {}}

