	Author = {Hatzopoulos, Vasilis and Taylor, Michael and Kiss, Istvan Z. and Simon, P{\'e}ter L.},
	Date-Added = {2010-09-07 21:41:32 +0100},
	Date-Modified = {2010-09-24 09:10:37 +0100},
	Institution = {Department of Mathematics, University of Sussex and Institute of Mathematics, Eotvos Lorand University},
	Keywords = {epidemiology, networks, graph theory, applied mathematics, mathematical biology},
	Month = {September},
	Number = {15},
	Title = {Multiple sources and routes of information transmission: implications for epidemic dynamics},
	Type = {preprint},
	Year = {2010},
	Abstract = {In a recent paper, we proposed and analyzed a compartmental ODE-based model describing
the dynamics of an infectious disease where the presence of the pathogen also triggers the 
diffusion of information about the disease. In this paper, we extend this previous work by 
presenting results based on pairwise and simulation models that are better suited for capturing 
the population contact structure at a local level. We use the pairwise model to examine the 
potential of different information generating mechanisms and routes of information 
transmission to stop disease spread or to minimize the impact of an epidemic. The 
individual-based simulation is used to better differentiate between the networks of disease 
and information transmission and to investigate the impact of different basic network 
topologies and network overlap on epidemic dynamics. The paper concludes with an
individual-based semi-analytic calculation of R0 at the non-trivial disease free equilibrium.},
	Bdsk-File-1 = {}}

@techreport{DekelLeviatan:04,
	Address = {6 Hamasger St., Or-Yehuda 60408, Israel, shai.dekel@turboimage.com and Tel Aviv University, Tel Aviv 69978, Israel, leviatan@math.tau.ac.il},
	Author = {Dekel, S. and Leviatan, D.},
	Date-Added = {2010-09-06 21:30:51 +0100},
	Date-Modified = {2010-12-18 12:09:00 +0000},
	Institution = {RealTimeImage and School of Mathematical Sciences},
	Keywords = {Bramble-Hilbert Lemma, Multivariate Nonlinear Approximation, finite element method},
	Mrclass = {41A10, 41A25, 41A63, 65M60},
	Title = {The {Bramble--Hilbert Lemma} for convex domains},
	Type = {online preprint},
	Year = {2004},
	Abstract = {The Bramble-Hilbert lemma is a fundamental result on multivariate polynomial ap- proximation. It is frequently applied in the analysis of Finite Element Methods (FEM) used for numerical solutions of PDEs. However, this classical estimate depends on the geometry of the domain and may `blow-up' for simple examples such as a sequence of triangles of equivalent diameter that become thinner and thinner. Thus, in FEM applications one usually requires that the mesh has `quasi-uniform' geometry. This assumption is perhaps too restrictive when one tries to obtain estimates of nonlinear approximation methods that use piecewise polynomials.
Our main result that improves upon this point is the following. Let Ω ⊂ Rn be a bounded convex domain and let $g\in W_p^m(\Omega), m\in N, 1\leq p \leq\infty$, where $W_p^m(\Omega)$ is the Sobolev space. Then there exists a polynomial $P$ of total degree $m-1$ for which
$|g−P|_{k,p}\leqC(n,m)\operatorname{diam}\Omega^{m-k}|g|_{m,p},k=0,1,\dotsc,m,$ where $|\cdot|_{k,p} := \sum_{|\alpha|=k}∥D^\alpha\cdot ∥_{L^p (\Omega)}$ is the Sobolev semi-norm of order $k$. As a consequence we get that for $f\in L_p(\Omega)$,
$m Em−1(f,Ω)p≈Km f,diamΩ	p,
where Em−1(f,Ω)p := infP∈Πm−1 ∥f − P∥Lp(Ω)$, is the error of polynomial approximation of degree $m-1$ and $K_m( , )_p$ is the K-functional associated with the pair $(L_p(\Omega),W_p^m(\Omega))$, and where the constants of equivalence depend only on $m$ and $n$.
For the case of convex domains (elements) this extends a recent result for $p = 2$, and for $m = 1$ and $2 < p\leq\infty$. This also improves previous results where the constant in the estimate further depends on the geometry of the domain, or where there is a constraint $p > n(\geq 2)$.},
