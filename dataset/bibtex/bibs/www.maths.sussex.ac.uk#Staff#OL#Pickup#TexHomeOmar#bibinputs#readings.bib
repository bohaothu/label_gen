@article{evans:80,
	Author = {Evans, Lawrence C.},
	Coden = {HJMADZ},
	Date-Added = {2009-03-27 10:29:42 +0000},
	Date-Modified = {2009-03-27 10:34:53 +0000},
	Fjournal = {Houston Journal of Mathematics},
	Issn = {0362-1588},
