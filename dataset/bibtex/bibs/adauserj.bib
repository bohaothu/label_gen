@Article{Zweiacker:1997:MCA,
  author =       "M. Zweiacker",
  title =        "Making {CORBA} applications fault tolerant using
                 checkpointing and recovery",
  journal =      j-ADA-USER-J,
  volume =       "18",
  number =       "2",
  pages =        "78--91",
  month =        jun,
  year =         "1997",
  CODEN =        "AUJOET",
  ISSN =         "1381-6551",
  bibdate =      "Fri Dec 26 08:32:34 MST 1997",
  bibsource =    "http://www.adauk.org.uk/pubs/default.htm",
  abstract =     "Fault tolerance is an issue of high importance to
                 distributed systems, a fact that is well identified in
                 the ISO/ITU Reference Model of Open Distributed
                 Processing (ODP) by the inclusion of failure
                 transparency. The Persistent Object Group Service
                 (POGS) described in this article keeps Object Group
                 Service (POGS) described in this article keeps track of
                 the state of a distributed application as far as global
                 checkpoint consistency is concerned. Application
                 objects take checkpoints of their own in a
                 non-coordinated fashion, using the POGS to detect
                 global state inconsistencies. As a consequence of
                 consulting POGS, objects take additional checkpoints
                 that would not have occurred otherwise, but which are
                 necessary to ensure global state consistency. The
                 advantage of the POGS approach lies in the fact that
                 global checkpoint consistency control is in the fact
                 that global checkpoint consistency control is separated
                 from the objects that actually do the checkpointing.
                 This is a necessary step on the way to integrating
                 fault tolerance mechanisms in a late stage of the
                 software development process. A prototype of the POGS
                 has been implemented using CORBA as a standard
                 distributed systems technology.",
  acknowledgement = ack-nhfb,
  classification = "C6110B (Software engineering techniques); C6110J
                 (Object-oriented programming); C6150G (Diagnostic,
                 testing, debugging and evaluating systems); C6150G
                 (Diagnostic, testing, debugging and techniques); C6150N
                 (Distributed systems software)",
  corpsource =   "Swiss Telecom PTT Res. and Dev., Berne, Switzerland",
  keywords =     "distributed application state monitoring; distributed
                 processing; distributed systems technology; failure
                 transparency; fault-tolerant CORBA applications; global
                 checkpoint consistency control; global state
                 consistency; ISO standards; ISO/ITU Reference Model of
                 Open Distributed Processing; object-oriented
                 programming; Persistent Object Group Service; POGS;
                 software development process; software engineering;
                 software fault tolerance; software standards; system
                 monitoring; system recovery; system recovery software
                 standards",
  pubcountry =   "UK",
  treatment =    "P Practical",
}

@Article{Ward:1997:RRE,
  author =       "W. A. {Ward, Jr.}",
  title =        "A retrospective on the {Rational Environment}",
  journal =      j-ADA-USER-J,
  volume =       "18",
  number =       "3",
  pages =        "121--143",
  month =        sep,
  year =         "1997",
  CODEN =        "AUJOET",
  ISSN =         "1381-6551",
  bibdate =      "Fri Dec 26 08:32:34 MST 1997",
  abstract =     "The Rational Environment was a software engineering
                 environment designed to facilitate the development of
                 large-scale software systems written in Ada. This
                 article is intended to be a comprehensive survey of
                 available information on the Environment as well as a
                 description of information on the Environment as well
                 as a description of its capabilities. The article
                 begins by presenting the historical context in which
                 the Rational Environment was developed; this includes
                 background information on the so-called software crisis
                 and the US Department of Defense response. So that its
                 use may be more clearly understood by those who have
                 never used software development aids of this type, a
                 brief general discussion of software engineering
                 environments is also provided. This is followed by a
                 description of the Rational Environment itself,
                 emphasizing description of the Rational Environment
                 itself, emphasizing the layered structure which allows
                 tight integration of additional tools. Several such
                 tools are then described, noting the phase of the
                 software life cycle that each addresses. The next major
                 section reports the experiences of various Environment
                 users. The article concludes with some remarks
                 regarding the Environment's strengths and
                 limitations.",
  acknowledgement = ack-nhfb,
  classification = "C6110B (Software engineering techniques); C6115
                 (Programming support); C6140D (High level languages)",
  corpsource =   "Sch. of Comput. and Inf. Sci., Univ. of South Alabama,
                 Mobile, AL, USA",
  keywords =     "Ada; computer aided software engineering; large-scale
                 software system development; layered structure;
                 programming environments; Rational Environment;
                 reviews; software crisis; software engineering
                 environment; software life cycle; software tools;
                 software tools environments",
  pubcountry =   "UK",
  treatment =    "B Bibliography; P Practical",
}

