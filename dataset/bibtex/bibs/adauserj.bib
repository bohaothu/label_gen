@Article{Betz:1997:EAC,
  author =       "H. Betz and B. Holzmuller and R. Koschke",
  title =        "Experiences in Adjusting a Compiler Toolkit to
                 Generate {Ada 95} Code",
  journal =      j-ADA-USER-J,
  volume =       "18",
  number =       "1",
  pages =        "29--40",
  month =        mar,
  year =         "1997",
  CODEN =        "AUJOET",
  ISSN =         "1381-6551",
  bibdate =      "Fri Dec 26 08:32:34 MST 1997",
  bibsource =    "http://www.adauk.org.uk/pubs/default.htm",
  abstract =     "Cocktail is a compiler toolkit originally developed by
                 the ``Gesellschaft fur Mathematik und Datenverarbeitung
                 (GMD)'' at Karlsruhe in Germany. It supports all phases
                 of compiler construction by providing generators for
                 scanners, parsers, abstract syntax trees, attribute
                 evaluators, traversers of abstract syntax trees,
                 attribute evaluators, traversers of abstract syntax
                 trees, and back ends. The original toolkit generates C
                 and Modula-2 code. The authors adjusted the tools
                 (other than the code generator) to generate Ada 83 code
                 before Ada 95 became standardised and any Ada 95
                 compiler was available. The generated Ada 83 code is
                 quite similar to generated Modula-2. When the Ada 95
                 compiler, Gnat, was released, they took advantage of
                 the object-oriented features of Ada 95 for code
                 generation. This led to a major simplification of the
                 generated abstract syntax a major simplification of the
                 generated abstract syntax tree. Nevertheless, in taking
                 advantage of the possibility to define dispatching
                 operations, they observed an explosion of the generated
                 code, which led to tremendously longer compile times
                 compared to the former Ada 83 code. Finally, they
                 realised that the object-oriented features of Ada 95
                 were not sufficient to support the code generation as
                 conveniently as they had expected.",
  acknowledgement = ack-nhfb,
  classification = "C6115 (Programming support); C6140D (High level
                 languages); C6150C (Compilers, interpreters and other
                 processors)",
  corpsource =   "Dept. of Comput. Sci., Stuttgart Univ., Germany",
  keywords =     "abstract syntax tree traversers; abstract syntax
                 trees; Ada; Ada 95 code generation; application
                 generators; attribute evaluators; back ends; Cocktail
                 compiler toolkit; compile times; compiler construction;
                 compiler generators; dispatching operations; Gnat;
                 object-oriented features; object-oriented languages;
                 parsers; scanners; software tools; software tools
                 oriented languages",
  pubcountry =   "UK",
  treatment =    "P Practical",
}

