@Article{Sanden:1996:UTC,
  author =       "B. I. Sanden",
  title =        "Using tasks to capture problem concurrency",
  journal =      j-ADA-USER-J,
  volume =       "17",
  number =       "1",
  pages =        "25--36",
  month =        mar,
  year =         "1996",
