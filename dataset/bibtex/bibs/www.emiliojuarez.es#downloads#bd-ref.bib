@ARTICLE{Alkorta2000Comparison131,
  author = {Alkorta, I. and Barrios, L. and Rozas, I. and Elguero, J.},
  journal = {J. Mol. Struct.},
  title = {Comparison of models to correlate electron density at the bond critical
	point and bond distance},
  year = {2000},
  volume = {496},
  pages = {131--137},
  timestamp = {2008-11-01+14:28:38},
  abstract = {Two models proposed in the literature to correlate the electron density
	at the bond critical point with the bond distance have been compared.
	The first one, proposed independently by Knop-Boyd and Destro, considers
	an implicit double logarithmic relationship between these two properties.
	We propose a second model, which uses a logarithmic relationship.
	Van der Waals and hydrogen bond interactions as well as traditional
	covalent bonds have been considered in order to have a broader range
	of the compared properties. The results indicate that both models
	provide good results when individual bond types are correlated, although
	the logarithmic model seems to be slightly better than the double
	logarithmic one. Finally, a unique equation corresponding to model
	2 has been devised to correlate all the H-X or C-X bonds.},
  file = {Alkorta2000Comparison131.pdf:BIBLIOGRAFIA/adf-dft/Alkorta2000Comparison131.pdf:PDF},
  keywords = {Hydrogen bonds, Bond lengths, Bond critical points},
