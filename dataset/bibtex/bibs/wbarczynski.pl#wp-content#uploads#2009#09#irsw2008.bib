@INPROCEEDINGS{loeser2008,
  author = {Alexander Loeser and Wojciech Barczynski and Falk Brauer},
  title = {What's the intention behind your query? A few observations from a
	large developer community},
  booktitle = {1st International Workshop on Identity and Reference on the Semantic
	Web (IRSW2008)},
  year = {2008},
  month = {06},
  abstract = {We study common query intentions in a software developer network with
	more than one million users. Based on a large query log analysis
	we could identify typical search intentions and identify common entities.
	For resolving most frequent query intentions and to identify entities
	and relationships from relevant pages we recommend state-of-the-art
	information extraction technologies.},
