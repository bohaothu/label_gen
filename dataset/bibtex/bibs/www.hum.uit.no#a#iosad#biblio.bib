    @article{rubach-booij2001,
     jstor_articletype = {primary_article},
     title = {Allomorphy in {O}ptimality {T}heory: {P}olish iotation},
     author = {Rubach, Jerzy and Booij, Geert E.},
     journal = {Language},
     volume = 77,
     number = 1,
     jstor_formatteddate = {Mar., 2001},
     pages = {26--60},
     abstract = {This article discusses iotation, a process that has been analyzed in generative phonology as a palatalization rule. We argue that optimality theory predicts the treatment of this process in terms of allomorphy, which in fact is desirable for a synchronic analysis. The consequence is that, with regard to iotation effects, the task of phonology is to account for the distribution of allomorphs rather than to derive them from a single underlying representation. While, as a result of diachronic changes, the allomorphs are arbitrary, their distribution is not. It follows from the interaction of universal phonological and morphological constraints, and from the considerations of segment markedness.},
     year = 2001,
    }

