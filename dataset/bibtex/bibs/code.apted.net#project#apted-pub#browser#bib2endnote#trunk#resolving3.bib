    <table class="code"><thead><tr><th class="lineno" title="Line numbers">Line</th><th class="content"> </th></tr></thead><tbody><tr><th id="L1"><a href="#L1">1</a></th><td>This file was created with JabRef 1.4.</td></tr><tr><th id="L2"><a href="#L2">2</a></th><td></td></tr><tr><th id="L3"><a href="#L3">3</a></th><td>@STRING{AIMS = {Workshop on Artificial Intelligence in Mobile Systems (AIMS)}}</td></tr><tr><th id="L4"><a href="#L4">4</a></th><td></td></tr><tr><th id="L5"><a href="#L5">5</a></th><td>@STRING{LNCS = {Lecture Notes in Computer Science (LNCS)}}</td></tr><tr><th id="L6"><a href="#L6">6</a></th><td></td></tr><tr><th id="L7"><a href="#L7">7</a></th><td>@STRING{interactions = {{ACM} Interactions}}</td></tr><tr><th id="L8"><a href="#L8">8</a></th><td></td></tr><tr><th id="L9"><a href="#L9">9</a></th><td>@STRING{CHI = {Conference on Human Factors in Computing Systems}}</td></tr><tr><th id="L10"><a href="#L10">10</a></th><td></td></tr><tr><th id="L11"><a href="#L11">11</a></th><td>@INPROCEEDINGS{chen03ontology,</td></tr><tr><th id="L12"><a href="#L12">12</a></th><td>  author = {Harry Chen and Tim Finin},</td></tr><tr><th id="L13"><a href="#L13">13</a></th><td>  title = {An Ontology for Context Aware Pervasive Computing Environments},</td></tr><tr><th id="L14"><a href="#L14">14</a></th><td>  abstract = {We describe a framework for an agent based pervasive computing environment.</td></tr><tr><th id="L15"><a href="#L15">15</a></th><td>         Central to our framework is the presence of an intelligent context</td></tr><tr><th id="L16"><a href="#L16">16</a></th><td>         broker that accepts context related information from devices and</td></tr><tr><th id="L17"><a href="#L17">17</a></th><td>         agents in the environment as well as from other sources, including</td></tr><tr><th id="L18"><a href="#L18">18</a></th><td>         information available on the web describing the space and events and</td></tr><tr><th id="L19"><a href="#L19">19</a></th><td>         activities scheduled for it. The context broker integrates and reasons</td></tr><tr><th id="L20"><a href="#L20">20</a></th><td>         over this information to maintain a coherent model of the space, the</td></tr><tr><th id="L21"><a href="#L21">21</a></th><td>         devices, agents and people in it, and their associated services and</td></tr><tr><th id="L22"><a href="#L22">22</a></th><td>         activities. A key to realizing this architecture is the use of a set of</td></tr><tr><th id="L23"><a href="#L23">23</a></th><td>         common ontologies that undergird the communication and representation.},</td></tr><tr><th id="L24"><a href="#L24">24</a></th><td>  keywords = {ontology architecture},</td></tr><tr><th id="L25"><a href="#L25">25</a></th><td>  pdf = {context\modelling\chen03ontology.pdf},</td></tr><tr><th id="L26"><a href="#L26">26</a></th><td>  crossref = {ijcai03ontology},</td></tr><tr><th id="L27"><a href="#L27">27</a></th><td>}</td></tr><tr><th id="L28"><a href="#L28">28</a></th><td></td></tr><tr><th id="L29"><a href="#L29">29</a></th><td>@ARTICLE{dey01toolkit,</td></tr><tr><th id="L30"><a href="#L30">30</a></th><td>  author = {Anind K. Dey and Daniel Salber and Gregory D. Abowd},</td></tr><tr><th id="L31"><a href="#L31">31</a></th><td>  title = {A Conceptual Framework and a Toolkit for Supporting the Rapid Prototyping of Context-Aware Applications},</td></tr><tr><th id="L32"><a href="#L32">32</a></th><td>  pages = {97-166},</td></tr><tr><th id="L33"><a href="#L33">33</a></th><td>  abstract = {Computing devices and applications are now used beyond the desktop, in diverse</td></tr><tr><th id="L34"><a href="#L34">34</a></th><td>         environments, and this trend toward ubiquitous computing is accelerating.</td></tr><tr><th id="L35"><a href="#L35">35</a></th><td>         One challenge that remains in this emerging research field is the</td></tr><tr><th id="L36"><a href="#L36">36</a></th><td>         ability to enhance the behavior of any application by informing it</td></tr><tr><th id="L37"><a href="#L37">37</a></th><td>         of the context of its use. By context, we refer to any information</td></tr><tr><th id="L38"><a href="#L38">38</a></th><td>         that characterizes a situation related to the interaction between</td></tr><tr><th id="L39"><a href="#L39">39</a></th><td>         humans, applications and the surrounding environment. Context-aware</td></tr><tr><th id="L40"><a href="#L40">40</a></th><td>         applications promise richer and easier interaction, but the current state of</td></tr><tr><th id="L41"><a href="#L41">41</a></th><td>         research in this field is still far removed from that vision. ...},</td></tr><tr><th id="L42"><a href="#L42">42</a></th><td>  pdf = {context\architecture\dey01toolkit.pdf},</td></tr><tr><th id="L43"><a href="#L43">43</a></th><td>  keywords = {toolkit architecture},</td></tr><tr><th id="L44"><a href="#L44">44</a></th><td>  crossref = {2001hcij16},</td></tr><tr><th id="L45"><a href="#L45">45</a></th><td>  url = {http://www.cs.berkeley.edu/~dey/context.html},</td></tr><tr><th id="L46"><a href="#L46">46</a></th><td>}</td></tr><tr><th id="L47"><a href="#L47">47</a></th><td></td></tr><tr><th id="L48"><a href="#L48">48</a></th><td>@INPROCEEDINGS{dietz01diamondtouch,</td></tr><tr><th id="L49"><a href="#L49">49</a></th><td>  author = {Paul Dietz and Darren Leigh},</td></tr><tr><th id="L50"><a href="#L50">50</a></th><td>  title = {Diamond{T}ouch: a Multi-User Touch Technology},</td></tr><tr><th id="L51"><a href="#L51">51</a></th><td>  pages = {219--226},</td></tr><tr><th id="L52"><a href="#L52">52</a></th><td>  doi = {http://doi.acm.org/10.1145/502348.502389},</td></tr><tr><th id="L53"><a href="#L53">53</a></th><td>  pdf = {DiamondTouch\dietz01uist_DT_Multi_User_Touch_Technology.pdf},</td></tr><tr><th id="L54"><a href="#L54">54</a></th><td>  keywords = {diamond},</td></tr><tr><th id="L55"><a href="#L55">55</a></th><td>  crossref = {uist2001},</td></tr><tr><th id="L56"><a href="#L56">56</a></th><td>  isbn = {1-58113-438-X},</td></tr><tr><th id="L57"><a href="#L57">57</a></th><td>  location = {Orlando, Florida},</td></tr><tr><th id="L58"><a href="#L58">58</a></th><td>}</td></tr><tr><th id="L59"><a href="#L59">59</a></th><td></td></tr><tr><th id="L60"><a href="#L60">60</a></th><td>@INPROCEEDINGS{flanagan03hierarchical,</td></tr><tr><th id="L61"><a href="#L61">61</a></th><td>  author = {John A. Flanagan and Johan Himberg and Jani M{\"{a}}ntyh{\"{a}}rvi},</td></tr><tr><th id="L62"><a href="#L62">62</a></th><td>  title = {A Hierarchical Approach to Learning Context and Facilitating User Interaction in Mobile Devices},</td></tr><tr><th id="L63"><a href="#L63">63</a></th><td>  abstract = {Recognizing user context is important in making mobile</td></tr><tr><th id="L64"><a href="#L64">64</a></th><td>         devices simple to use. The device and the underlying mobile service</td></tr><tr><th id="L65"><a href="#L65">65</a></th><td>         can provide a personalized user interface that adapts to the usage</td></tr><tr><th id="L66"><a href="#L66">66</a></th><td>         situation. The device can infer parts of the context of the user from</td></tr><tr><th id="L67"><a href="#L67">67</a></th><td>         features extracted from on-board sensors that make measurements of</td></tr><tr><th id="L68"><a href="#L68">68</a></th><td>         acceleration, noise level, luminosity, humidity, etc. In this paper we</td></tr><tr><th id="L69"><a href="#L69">69</a></th><td>         justify why generating different level descriptions of user context is</td></tr><tr><th id="L70"><a href="#L70">70</a></th><td>         necessary for the development of useful applications. We also show how it</td></tr><tr><th id="L71"><a href="#L71">71</a></th><td>         is possible to generate higher level contexts. A practical method</td></tr><tr><th id="L72"><a href="#L72">72</a></th><td>         based on the clustering of symbol string data using the SCM algorithm</td></tr><tr><th id="L73"><a href="#L73">73</a></th><td>         is described. This is applied to the analysis of a real data set</td></tr><tr><th id="L74"><a href="#L74">74</a></th><td>         from sensor recordings. Based on this data and using the various</td></tr><tr><th id="L75"><a href="#L75">75</a></th><td>         ideas presented, different levels of user context are generated and a</td></tr><tr><th id="L76"><a href="#L76">76</a></th><td>         simple method for changing a mobile device profile are described.},</td></tr><tr><th id="L77"><a href="#L77">77</a></th><td>  keywords = {modelling resolving},</td></tr><tr><th id="L78"><a href="#L78">78</a></th><td>  pdf = {context\modelling\flanagan03hierarchical.pdf},</td></tr><tr><th id="L79"><a href="#L79">79</a></th><td>  crossref = {aims2003},</td></tr><tr><th id="L80"><a href="#L80">80</a></th><td>}</td></tr><tr><th id="L81"><a href="#L81">81</a></th><td></td></tr><tr><th id="L82"><a href="#L82">82</a></th><td>@INPROCEEDINGS{harvel04contextcube,</td></tr><tr><th id="L83"><a href="#L83">83</a></th><td>  author = {Lonnie Harvel and Ling Liu and Gregory D. Abowd and Yu-Xi Lim and Chris Scheibe and Chris Chatham},</td></tr><tr><th id="L84"><a href="#L84">84</a></th><td>  title = {Context {C}ube: Flexible and Effective Manipulation of Sensed Context Data},</td></tr><tr><th id="L85"><a href="#L85">85</a></th><td>  pages = {51--68},</td></tr><tr><th id="L86"><a href="#L86">86</a></th><td>  abstract = {In an effort to support the development of context-aware</td></tr><tr><th id="L87"><a href="#L87">87</a></th><td>         applications that use archived sensor data, we introduce the concept of</td></tr><tr><th id="L88"><a href="#L88">88</a></th><td>         the Context Cube based on techniques of data warehousing and data</td></tr><tr><th id="L89"><a href="#L89">89</a></th><td>         mining. Our implementation of the Context Cube provides a system that</td></tr><tr><th id="L90"><a href="#L90">90</a></th><td>         supports a multidimensional model of context data and with tools for</td></tr><tr><th id="L91"><a href="#L91">91</a></th><td>         accessing, interpreting and aggregating that data by using concept</td></tr><tr><th id="L92"><a href="#L92">92</a></th><td>         relationships defined within the real context of the application. We</td></tr><tr><th id="L93"><a href="#L93">93</a></th><td>         define the Context Cube information model, demonstrate functional</td></tr><tr><th id="L94"><a href="#L94">94</a></th><td>         applications that we have developed with the system, and explore</td></tr><tr><th id="L95"><a href="#L95">95</a></th><td>         possible applications that may be developed more easily using our tool.},</td></tr><tr><th id="L96"><a href="#L96">96</a></th><td>  keywords = {resolving},</td></tr><tr><th id="L97"><a href="#L97">97</a></th><td>  pdf = {context\resolving\harvel04contextcube.pdf},</td></tr><tr><th id="L98"><a href="#L98">98</a></th><td>  crossref = {pervasive2004},</td></tr><tr><th id="L99"><a href="#L99">99</a></th><td>}</td></tr><tr><th id="L100"><a href="#L100">100</a></th><td></td></tr><tr><th id="L101"><a href="#L101">101</a></th><td>@ARTICLE{hong01infrastructure,</td></tr><tr><th id="L102"><a href="#L102">102</a></th><td>  author = {Jason I. Hong and James A. Landay},</td></tr><tr><th id="L103"><a href="#L103">103</a></th><td>  title = {An Infrastructure Approach to Context-Aware Computing},</td></tr><tr><th id="L104"><a href="#L104">104</a></th><td>  pages = {287--303},</td></tr><tr><th id="L105"><a href="#L105">105</a></th><td>  abstract = {The Context Toolkit (Dey,</td></tr><tr><th id="L106"><a href="#L106">106</a></th><td>         Salber, and Abowd 2001 [this special issue]) is only one of many</td></tr><tr><th id="L107"><a href="#L107">107</a></th><td>         possible architectures for supporting context-aware applications.</td></tr><tr><th id="L108"><a href="#L108">108</a></th><td>         In this essay, we look at the trade-offs involved with a service</td></tr><tr><th id="L109"><a href="#L109">109</a></th><td>         infrastructure approach to context-aware computing. We describe the advantages</td></tr><tr><th id="L110"><a href="#L110">110</a></th><td>         that a service infrastructure for contextawareness has over other</td></tr><tr><th id="L111"><a href="#L111">111</a></th><td>         approaches, outline some of the core technical challenges that must</td></tr><tr><th id="L112"><a href="#L112">112</a></th><td>         be addressed before such an infrastructure can be built, and point</td></tr><tr><th id="L113"><a href="#L113">113</a></th><td>         out promising research directions for overcoming these challenges.},</td></tr><tr><th id="L114"><a href="#L114">114</a></th><td>  keywords = {infrastructure architecture},</td></tr><tr><th id="L115"><a href="#L115">115</a></th><td>  pdf = {context\resolving\hong01infrastructure.pdf},</td></tr><tr><th id="L116"><a href="#L116">116</a></th><td>  crossref = {2001hcij16},</td></tr><tr><th id="L117"><a href="#L117">117</a></th><td>}</td></tr><tr><th id="L118"><a href="#L118">118</a></th><td></td></tr><tr><th id="L119"><a href="#L119">119</a></th><td>@INPROCEEDINGS{pedersen97aroma,</td></tr><tr><th id="L120"><a href="#L120">120</a></th><td>  author = {Pedersen, Elin R{\o}nby and Sokoler, Tomas},</td></tr><tr><th id="L121"><a href="#L121">121</a></th><td>  title = {{AROMA}: Abstract Representation of Presence Supporting Mutual Awareness},</td></tr><tr><th id="L122"><a href="#L122">122</a></th><td>  pages = {51--58},</td></tr><tr><th id="L123"><a href="#L123">123</a></th><td>  doi = {http://doi.acm.org/10.1145/258549.258584},</td></tr><tr><th id="L124"><a href="#L124">124</a></th><td>  abstract = {The AROMA project is exploring the kind of awareness that people effortless are able to</td></tr><tr><th id="L125"><a href="#L125">125</a></th><td>         maintain about other beings who are located physically close. We</td></tr><tr><th id="L126"><a href="#L126">126</a></th><td>         are designing technology that attempts to mediate a similar kind of</td></tr><tr><th id="L127"><a href="#L127">127</a></th><td>         awareness among people who are geographically dispersed but want to stay</td></tr><tr><th id="L128"><a href="#L128">128</a></th><td>         better in touch. AROMA technology can be thought of as a stand-alone</td></tr><tr><th id="L129"><a href="#L129">129</a></th><td>         communication device or -- more likely -- an augmentation of existing</td></tr><tr><th id="L130"><a href="#L130">130</a></th><td>         technologies such as the telephone or full-blown media spaces. Our approach</td></tr><tr><th id="L131"><a href="#L131">131</a></th><td>         differs from other recent designs for awareness (a) by choosing pure</td></tr><tr><th id="L132"><a href="#L132">132</a></th><td>         abstract representations on the display site, (b) by possibly remapping</td></tr><tr><th id="L133"><a href="#L133">133</a></th><td>         the signal across media between capture and display, and, finally,</td></tr><tr><th id="L134"><a href="#L134">134</a></th><td>         (c) by explicitly extending the application domain to include more</td></tr><tr><th id="L135"><a href="#L135">135</a></th><td>         than the working life, to embrace social interaction in general.},</td></tr><tr><th id="L136"><a href="#L136">136</a></th><td>  keywords = {architecture},</td></tr><tr><th id="L137"><a href="#L137">137</a></th><td>  pdf = {context\architecture\pedersen97aroma.pdf},</td></tr><tr><th id="L138"><a href="#L138">138</a></th><td>  crossref = {chi1997},</td></tr><tr><th id="L139"><a href="#L139">139</a></th><td>  isbn = {0-89791-802-9},</td></tr><tr><th id="L140"><a href="#L140">140</a></th><td>  location = {Atlanta, Georgia, United States},</td></tr><tr><th id="L141"><a href="#L141">141</a></th><td>}</td></tr><tr><th id="L142"><a href="#L142">142</a></th><td></td></tr><tr><th id="L143"><a href="#L143">143</a></th><td>@INPROCEEDINGS{ranganat03middleware,</td></tr><tr><th id="L144"><a href="#L144">144</a></th><td>  author = {Anand Ranganathan and Roy H. Campbell},</td></tr><tr><th id="L145"><a href="#L145">145</a></th><td>  title = {A Middleware for Context-Aware Agents in Ubiquitous Computing Environments},</td></tr><tr><th id="L146"><a href="#L146">146</a></th><td>  pages = {143--161},</td></tr><tr><th id="L147"><a href="#L147">147</a></th><td>  keywords = {middleware},</td></tr><tr><th id="L148"><a href="#L148">148</a></th><td>  pdf = {context\middleware\ranganat03middleware.pdf},</td></tr><tr><th id="L149"><a href="#L149">149</a></th><td>  crossref = {middleware2003},</td></tr><tr><th id="L150"><a href="#L150">150</a></th><td>}</td></tr><tr><th id="L151"><a href="#L151">151</a></th><td></td></tr><tr><th id="L152"><a href="#L152">152</a></th><td>@INPROCEEDINGS{ranganathan03ontology,</td></tr><tr><th id="L153"><a href="#L153">153</a></th><td>  author = {Anand Ranganathan and Robert E McGrath and Roy H. Campbell and M. Dennis Mickunas},</td></tr><tr><th id="L154"><a href="#L154">154</a></th><td>  title = {Ontologies in a Pervasive Computing Environment},</td></tr><tr><th id="L155"><a href="#L155">155</a></th><td>  abstract = {Ontologies are entering widespread use in many areas such</td></tr><tr><th id="L156"><a href="#L156">156</a></th><td>         as knowledge and content management, electronic commerce and the</td></tr><tr><th id="L157"><a href="#L157">157</a></th><td>         Semantic Web. In this paper we show how the use of ontologies has</td></tr><tr><th id="L158"><a href="#L158">158</a></th><td>         helped us overcome some important problems in the development of</td></tr><tr><th id="L159"><a href="#L159">159</a></th><td>         pervasive computing environments. We have integrated ontologies and</td></tr><tr><th id="L160"><a href="#L160">160</a></th><td>         Semantic Web technology into our Pervasive Computing infrastructure. Our</td></tr><tr><th id="L161"><a href="#L161">161</a></th><td>         investigations have shown that the Semantic Web technology can be integrated</td></tr><tr><th id="L162"><a href="#L162">162</a></th><td>         into our CORBA-based infrastructure to augment several important</td></tr><tr><th id="L163"><a href="#L163">163</a></th><td>         services. This work suggests a number of requirements for future research</td></tr><tr><th id="L164"><a href="#L164">164</a></th><td>         and development of ontologies, reasoners, languages and interfaces.},</td></tr><tr><th id="L165"><a href="#L165">165</a></th><td>  keywords = {ontology},</td></tr><tr><th id="L166"><a href="#L166">166</a></th><td>  pdf = {context\modelling\ranganathan03ontology.pdf},</td></tr><tr><th id="L167"><a href="#L167">167</a></th><td>  crossref = {ijcai03ontology},</td></tr><tr><th id="L168"><a href="#L168">168</a></th><td>}</td></tr><tr><th id="L169"><a href="#L169">169</a></th><td></td></tr><tr><th id="L170"><a href="#L170">170</a></th><td>@INPROCEEDINGS{ringel04release,</td></tr><tr><th id="L171"><a href="#L171">171</a></th><td>  author = {Meredith Ringel and Kathy Ryall and Chia Shen and Clifton Forlines and Fr{\'{e}}d{\'{e}}eric Vernier},</td></tr><tr><th id="L172"><a href="#L172">172</a></th><td>  title = {Release, relocate, reorient, resize: fluid techniques for document sharing on multi-user interactive tables},</td></tr><tr><th id="L173"><a href="#L173">173</a></th><td>  pages = {1441--1444},</td></tr><tr><th id="L174"><a href="#L174">174</a></th><td>  doi = {http://doi.acm.org/10.1145/985921.986085},</td></tr><tr><th id="L175"><a href="#L175">175</a></th><td>  keywords = {diamond},</td></tr><tr><th id="L176"><a href="#L176">176</a></th><td>  pdf = {DiamondTouch\rotate_resize_TR2004-022.pdf},</td></tr><tr><th id="L177"><a href="#L177">177</a></th><td>  crossref = {chi2004ext},</td></tr><tr><th id="L178"><a href="#L178">178</a></th><td>  isbn = {1-58113-703-6},</td></tr><tr><th id="L179"><a href="#L179">179</a></th