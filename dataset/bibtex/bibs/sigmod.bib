@Article{Koster:1976:VT,
  author =       "Cornelis H. A. Koster",
  title =        "Visibility and types",
  journal =      j-SIGMOD,
  volume =       "8",
  number =       "2",
  pages =        "179--190",
  month =        mar,
  year =         "1976",
  CODEN =        "SRECD8",
