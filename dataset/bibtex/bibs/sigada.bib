@Article{Jennings:2009:SLL,
  author =       "Trevor J. Jennings",
  title =        "{SPARK}: the {Libre} language and toolset for
                 high-assurance software engineering",
  journal =      j-SIGADA-LETTERS,
  volume =       "29",
  number =       "3",
  pages =        "9--10",
  month =        dec,
  year =         "2009",
