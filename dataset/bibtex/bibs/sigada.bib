@Article{Baker:1993:SLR,
  author =       "Henry G. {Baker, Jr.}",
  title =        "Safe and leakproof resource management using {Ada83}
                 limited types",
  journal =      j-SIGADA-LETTERS,
  volume =       "13",
  number =       "5",
  pages =        "32--42",
  month =        sep # "\slash " # oct,
