@Article{Mattheij:1985:DSA,
  author =       "R. M. M. Mattheij",
  title =        "Decoupling and Stability of Algorithms for Boundary
                 Value Problems",
  journal =      j-SIAM-REVIEW,
  volume =       "27",
  number =       "1",
  pages =        "1--44",
  month =        mar,
  year =         "1985",
  CODEN =        "SIREAD",
