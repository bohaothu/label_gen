@Article{Glimm:1991:NSP,
  author =       "James Glimm",
  title =        "Nonlinear and Stochastic Phenomena: The Grand
                 Challenge for Partial Differential Equations",
  journal =      j-SIAM-REVIEW,
  volume =       "33",
  number =       "4",
  pages =        "626--643",
  month =        dec,
  year =         "1991",
  CODEN =        "SIREAD",
  ISSN =         "0036-1445 (print), 1095-7200 (electronic)",
  ISSN-L =       "0036-1445",
  MRclass =      "35L65 (35R60 58F13 58F35 76L05 76M99)",
  MRnumber =     "92i:35081",
  bibdate =      "Mon Jan 20 10:31:25 MST 1997",
  bibsource =    "Compendex database;
                 http://www.math.utah.edu/pub/tex/bib/siamreview.bib",
  abstract =     "The major problems for partial differential equations
                 are either nonlinear or stochastic or both. The
                 nonlinear structures emphasized here are the nonlinear
                 hyperbolic waves, which occur in the solutions of
                 nonlinear hyperbolic conservation laws and associated
                 dissipative equations. These equations are those most
                 commonly used to model physical and chemical processes
                 in continuum systems. Stochastic solutions of partial
                 differential equations may result from stochastic data.
                 Stochastic solutions can also result from internal
                 considerations, i.e., from instabilities inherent in
                 the equation itself, if the equation is nonlinear. The
                 instabilities which give rise to chaotic solutions are
                 generally unstable on a very large range of length
                 scales, or in some approximation, on all length scales.
                 Fractal geometries provide one model for the typically
                 intermittent, or blotchy, chaos which can occur, and
                 the renormalization group is an analytic tool which in
                 some cases has given a quantitatively correct theory of
                 chaotic solutions of partial differential equations.",
  acknowledgement = ack-nhfb,
  affiliation =  "State Univ of New York",
  affiliationaddress = "Stony Brook, NY, USA",
  classification = "921; 922",
  fjournal =     "SIAM Review",
  journalabr =   "SIAM Rev",
  keywords =     "Differential Equations; Fractal Geometry; Hyperbolic
                 Waves; Mathematical Techniques; Mathematical Techniques
                 --- Nonlinear Equations; Partial Differential
                 Equations; Probability --- Random Processes; Stochastic
                 Phenomena",
}

