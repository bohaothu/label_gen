@Article{Cuvelier:1990:SNM,
  author =       "C. Cuvelier and R. M. S. M. Schulkes",
  title =        "Some Numerical Methods for the Computation of
                 Capillary Free Boundaries Governed by the
                 {Navier--Stokes} Equations",
  journal =      j-SIAM-REVIEW,
  volume =       "32",
  number =       "3",
  pages =        "355--423",
  month =        sep,
