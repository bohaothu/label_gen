@Article{Hermes:1991:NHO,
  author =       "Henry Hermes",
  title =        "Nilpotent and High-Order Approximations of Vector
                 Field Systems",
  journal =      j-SIAM-REVIEW,
  volume =       "33",
  number =       "2",
  pages =        "238--264",
  month =        jun,
  year =         "1991",
  CODEN =        "SIREAD",
  ISSN =         "0036-1445 (print), 1095-7200 (electronic)",
  ISSN-L =       "0036-1445",
  MRclass =      "93B05 (49J15 57R27 93B27)",
  MRnumber =     "92e:93006",
  MRreviewer =   "Henk Nijmeijer",
  bibdate =      "Mon Jan 20 10:31:25 MST 1997",
  bibsource =    "Compendex database;
                 http://www.math.utah.edu/pub/tex/bib/siamreview.bib",
  abstract =     "This paper is concerned with the study of structurally
                 stable properties associated with systems described by
                 one, or several, real analytic vector fields via
                 approximating systems which retain information
                 pertinent to these properties. For example, let
                 $X_0,\ldots{}, X_m$ be real analytic vector fields on
                 an $n$-manifold $M$. An affine control system has the
                 form (i) $x = X_0(x) + \Sigma_1^m u_iX_i(x)$; a
                 second-order partial differential equation can be
                 written as (ii) $\Sigma_{i=1}^m X_i^2v - X_0v = f$,
                 while if $X_0(p) = 0$ the asymptotic stability of the
                 rest solution requires analysis of (iii) $x = X_0(x)$.
                 If, in (ii), $m < n$ yet the vector fields in the Lie
                 algebra generated by $X_1,\ldots{},X_m$, when evaluated
                 at $p$, span the tangent space to $M$ at $p$, the
                 operator is hypoelliptic but an approximation of the
                 vector fields needed to describe the singularity in a
                 parametrix must retain more information than a
                 linearization of the vector fields does. Similarly, the
                 question of small time local controllability of (i) has
                 been dealt with by constructing higher-order
                 approximating vector fields which generate a nilpotent
                 Lie algebra. The theory of these high-order
                 approximations and their applications is concisely
                 developed.",
  acknowledgement = ack-nhfb,
  affiliation =  "Univ of Colorado",
  affiliationaddress = "Boulder, CO, USA",
  classification = "731; 921",
  fjournal =     "SIAM Review",
  journalabr =   "SIAM Rev",
  keywords =     "Control Systems; Feedback Controls; Mathematical
                 Techniques --- Approximation Theory; Nilpotent
                 Approximations; Stability; Vector Fields",
}

