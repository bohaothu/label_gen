@Article{Bube:1983:ODI,
  author =       "Kenneth P. Bube and Robert Burridge",
  title =        "The one-dimensional inverse problem of reflection
                 seismology",
  journal =      j-SIAM-REVIEW,
  volume =       "25",
  number =       "4",
  pages =        "497--559",
  month =        oct,
  year =         "1983",
  CODEN =        "SIREAD",
