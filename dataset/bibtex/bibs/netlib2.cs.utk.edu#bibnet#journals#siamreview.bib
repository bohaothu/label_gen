  ISSN-L =       "0036-1445",
  MRclass =      "34Bxx (34Lxx 47Axx 47Nxx); 34B24 (34L05 47E05 47N20)",
  MRnumber =     "1 379 040; 97f:34015",
  MRreviewer =   "Lawrence Turyn",
  bibdate =      "Fri Dec 4 21:36:27 MST 1998",
  bibsource =    "Compendex database;
                 http://epubs.siam.org/sam-bin/dbq/toc/SIREV/38/1;
