@InProceedings{nandzik10contentus,
  author = 	 {Jan Nandzik and Andreas He{\ss} and Jan Hannemann and Nicolas Flores-Herr and Klaus Bossert},
  title = 	 {CONTENTUS - towards semantic multi-media libraries},
  booktitle = {World Library and Information Congress: 76th IFLA General Conference and Assembly},
  year = 	 {2010},
  address = 	 {Gothenburg, Sweden},
  month = 	 {August},
  url = "http://www.ifla.org/files/hq/papers/ifla76/149-nandzik-en.pdf",
  abstract ="The ever-growing amount of content and knowledge published online makes it possible for libraries to complement their own data and to present their collections in novel ways. Conceptually related information can be semantically linked so that users may benefit from richer data collections and novel search possibilities that capitalize on the inherent relationships between media, local metadata and external information sources. This paper presents potential solutions for the fundamental challenges of integrating heterogeneous data sources and providing innovative semantic search approaches, as they are developed for libraries and multimedia archives within the CONTENTUS project."
}

