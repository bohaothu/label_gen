@InProceedings{hess08from,
  author = 	 {Andreas He{\ss} and Christian Maa{\ss} and Francis Dierick},
  title = 	 {From Web 2.0 to Semantic Web: A Semi-Automated Approach},
  booktitle = {ESWC 08 Workshop on Collective Semantics: Collective Intelligence and the Semantic Web (CISWeb 2008)},
  year = 	 {2008},
  address = 	 {Tenerife, Spain},
  month = 	 {June},
  url = "http://www.andreas-hess.info/publications/hess-cisweb08.pdf",
  abstract ="Web 2.0 and Semantic Web are regarded as two complementary paradigms that will probably converge in the future. However, whereas the Semantic Web is an established field of research, there has been little analysis devoted to Web 2.0 applications. For this reason it remains unclear how the advantages of both paradigms could be merged. In this paper we make three contributions in this direction. First, we discuss why merging Web 2.0 and the Semantic Web is beneficial and propose five approaches. Second, we show that (semi-) automated tagging of content improves the quality of annotations. Third, we present an automatic approach for improving the tag quality by using duplicate detection techniques. We verify our approach on a large-scale data set from the social search service Lycos IQ."
}

@InProceedings{graefe07alternative,
  author = 	 {Gernot Graefe and Christian Maa{\ss} and Andreas He{\ss}},
  title = 	 {Alternative Searching
  Services: Seven Theses on the Importance of Social
  Bookmarking},
  url = "http://www.andreas-hess.info/publications/graefe-sabre07.pdf",
  booktitle = {Software, Agents and Services for Business, Research and E-Sciences â€” Conference on Social Semantic Web
(SABRE/CSSW 2007)},
  year = 	 {2007},
  address = 	 {Leipzig, Germany},
  abstract = "In recent years social bookmark systems like del.icio.us or Furl have become increasingly popular. These systems sometimes are regarded as alternatives to algorithmic search engines like Google. In this paper we develop seven theses on the potential of these systems in order to establish a conceptual basis for future research in this area. Thereby it becomes clear that social bookmarking systems complement rather than threaten algorithmic search engines."
}

@InProceedings{hess06iterative,
  author = 	 {Andreas He{\ss}},
  title = 	 {An Iterative Algorithm for Ontology Mapping Capable of Using Training Data},
  booktitle = {Proceedings of the 3rd European Semantic Web Conference
(ESWC 2006)},
  year = 	 2006,
  address = 	 {Budva, Montenegro},
  url = "http://www.andreas-hess.info/publications/hess-eswc06.pdf",
  abstract = "We present a new iterative algorithm for ontology mapping where we combine standard string distance metrics with a structural similarity measure that is based on a vector representation. After all pairwise similarities between concepts have been calculated we apply well-known graph algorithms to obtain an optimal matching. Our algorithm is also capable of using existing mappings to a third ontology as training data to improve accuracy. We compare the performance of our algorithm with the performance of other alignment algorithms and show that our algorithm can compete well against the current state-of-the-art."
}


@PhdThesis{hess05supervised,
  author = 	 {Andreas He{\ss}},
  title = 	 {Supervised and Unsupervised Ensemble Learning for the Semantic Web},
  school = 	 {School of Computer Science and Informatics, University College Dublin},
  month = {February},
  year = 	 {2006},
  address = 	 {Dublin, Ireland},
  url = "http://www.andreas-hess.info/publications/hess-thesis.pdf",
  abstract = "The World Wide Web offers an ocean of information and services for everyone, and many of us rely on it on a daily basis. One of the reasons for the Web's success is that the threshold for creating content on the Web was always very low, because comfortable and easy to use HTML editors are available. However, today's Web is limited by the fact that it is machine-readable, but not machine-understandable. The Semantic Web promises a solution to this problem by adding explicit semantics with the goal of making the Web machine-understandable by using description logics and ontologies. However, the threshold for creating this extra markup is very high, and no comfortable tools exist at present. The goal of this work is to develop such tools. Machine Learning techniques have been used in literature for various classification tasks. The central claim of this thesis is that such algorithms, supervised and unsupervised, can be used for this purpose."
}

