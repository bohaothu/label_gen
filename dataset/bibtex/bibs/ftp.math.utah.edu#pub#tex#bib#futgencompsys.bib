@Article{Grahn:1995:IEU,
  author =       "H{\aa}kan Grahn and Per Stenstr{\"o}m and Michel
                 Dubois",
  title =        "Implementation and Evaluation of Update-Based Cache
                 Protocols Under Relaxed Memory Consistency Models",
  journal =      j-FUT-GEN-COMP-SYS,
  volume =       "11",
  number =       "3",
  pages =        "247--271",
  month =        jun,
  year =         "1995",
  CODEN =        "FGSEVI",
  ISSN =         "0167-739X (print), 1872-7115 (electronic)",
  ISSN-L =       "0167-739X",
  bibdate =      "Sat Jan 10 11:53:59 MST 2004",
  bibsource =    "ftp://ftp.ira.uka.de/bibliography/Techreports/DIT.Lund.University.bib",
  note =         "Accepted for publication.",
  abstract =     "Cache protocols in large-scale shared-memory
                 multiprocessors are either invalidation-based or
                 update-based protocols, depending on the primary
                 mechanism to propagate values on writes.
                 Invalidation-based protocols have been extensively
                 studied. Under a relaxed memory consistency model most
                 of the write latency in these protocols can be easily
                 hidden whereas hiding the latency of read misses is
                 difficult. Since update-based protocols trade a
                 reduction in the miss rate for an increase in the write
                 traffic, they may therefore be preferable under relaxed
                 memory consistency models. The purpose of this paper is
                 to compare update- and invalidation-based protocols for
                 their ability to reduce or hide memory access latencies
                 and for their ease of implementation under relaxed
                 memory consistency models. Based on a detailed
                 simulation study, we find that write-update protocols
                 augmented with simple competitive mechanisms - we call
                 such protocols competitive-update protocols - can hide
                 all the write latency and cut the read penalty by as
                 much as 46\% at the cost of some increase in the memory
                 traffic. However, as compared to write-invalidate,
                 update-based protocols require more aggressive memory
                 consistency models and more local buffering in the
                 second-level cache to be effective. In addition, the
                 reduction of the write penalty may be offset by
                 increased synchronization overhead in applications with
                 high contention for critical sections.",
  acknowledgement = ack-nhfb,
  location =     "146",
}

