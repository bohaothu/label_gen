@Article{Collette:1994:SSC,
  author =       "T. Collette and H. Essafi and D. Juvin and J. Kaiser",
  title =        "{SYMPATIX}: {A} {SIMD} computer performing the low and
                 intermediate levels of image processing",
  journal =      j-FUT-GEN-COMP-SYS,
  volume =       "10",
  number =       "1",
  pages =        "3--13",
  month =        apr,
  year =         "1994",
  CODEN =        "FGSEVI",
  ISSN =         "0167-739X (print), 1872-7115 (electronic)",
  ISSN-L =       "0167-739X",
  bibdate =      "Fri Jul 15 09:06:04 MDT 2005",
  bibsource =    "ftp://ftp.ira.uka.de/bibliography/Parallel/PCV.bib;
                 http://www.sciencedirect.com/science/journal/0167739X",
  abstract =     "This SIMD processor based system performs with a good
                 efficiency the low level image processing operations,
                 but this efficiency is drastically cut when handling an
                 intermediate level class of algorithms. This study
                 emphasizes the drawbacks encountered to perform such
                 operations. The main one is the interconnection between
                 processors. So, a new interconnection network, called
                 the open intelligent network, is proposed and added to
                 SYMPTI2 to form SYMPATIX. This network allows irregular
                 transfers of data between the different processing
                 elements of the new system. Furthermore, this network
                 allows the efficient interconnection of specific
                 modules. The architecture is evaluated on
                 representative algorithms of image processing . A
                 behavioural model of SYMPATIX is described using a
                 hardware description language, the VHDL. The SIMD
                 computer efficiency has been considerably upgraded for
                 the low and intermediate levels of image processing .
                 Furthermore its application area is extended. The last
                 part of the paper describes the performance obtained
                 with simulations.",
  acknowledgement = ack-nhfb,
  keywords =     "hardware description language; image processing;
                 interconnection network; open intelligent network;
                 PARALLEL PROCESSING; PERFORMANCE EVALUATION; SIMD
                 computer; SYMPATIX; SYMPTI2; VHDL IMAGE PROCESSING",
}

